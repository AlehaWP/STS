                                                    Let('vTemp3',Copy(KRD_MAIN_2.NBD,vTemp+1,vTemp2-1));
                                                    Case(Length(vTemp3),[5, ENCODEDATE(Convert('200'+COPY(vTemp3,5,1),Integer),Convert(COPY(vTemp3,3,2),Integer),Convert(COPY(vTemp3,1,2),Integer)),
