                                                    ],
                                                    KRD_MAIN_2.BD_DATE
                                                    )
                                              )
                                        )
                                      )
                                      );  
                                      
                       Let('vG073', If(vTemp=0, LEFTPAD (Right(KRD_MAIN_2.NBD, 5), 7, '0'),
                                                if (vTemp2=0, LEFTPAD (Right(KRD_MAIN_2.NBD, 5), 7, '0'),
