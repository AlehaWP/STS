                       Let('vTemp2',StrPos('/',COPY(KRD_MAIN_2.NBD,vTemp+1,(Length(KRD_MAIN_2.NBD)-vTemp))));
                       Let('vG072', If(vTemp=0, KRD_MAIN_2.BD_DATE, 
                                      If(vTemp2=0, KRD_MAIN_2.BD_DATE, 
