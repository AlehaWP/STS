            );
            POST('tmpCOMM');
          )
        );//IF(bFoundGTD,
        iCountProgress := iCountProgress + 1;
        SETPROGRESS(iCountProgress, 100, iCount);
        NEXT('qEXEL_CMR');
      )
    );//WHILE(EOF('qEXEL_CMR') = 0,
    CLOSEDATASET('qEXEL_CMR');
    HIDEPROGRESS();
    iNowCountCMR := iNowCountCMR + 1;
    
    
  )
);//WHILE(iNowCountCMR <= iCountExWithCMR,

WHILE(iNowCountINV <= iCountExWithINV,//Лезем в наши Excel накладные. Сколько у нас файлов, мы получили когда подключали таблицы. Теперь по ним бежим и заполним временную таблицу.
  BLOCK(
    SHOWPROGRESS ('Сверка данных... Excel' + convert(iNowCountINV, string));
    OPENQUERY ('qEXEL_INV', 'select * from '+CHAR (34)+VALUE('sFilePathINV' + convert(iNowCountINV, string))+sListName+CHAR (34)+' where "код ТН ВЭД" is not null','EXCEL_DB');
    FIRST('qEXEL_INV');
    iCount := RECORDCOUNT('qEXEL_INV');
