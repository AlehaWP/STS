                    dDateGTD := ENCODEDATE ('20' + COPY(EXTRACTSTR (sTmpGTD, 2, '/'),5,2), COPY(EXTRACTSTR (sTmpGTD, 2, '/'),3,2), COPY(EXTRACTSTR (sTmpGTD, 2, '/'),1,2));
                    sCodVAL :=  '';
                      OPENQUERY ('qEXEL_VAL', 'select * from '+CHAR (34)+VALUE('sFilePathVAL' + convert(iNowCountVAL, string))+sListName+CHAR (34)+' where "  номер ГТД" ='+
                                              char(39) + EXTRACTSTR (sTmpGTD, 3, '/') + char(39) +
                                              ' and "дата регистрации ГТД" = ' + convert(dDateGTD, integer) +
                                              ' and "код таможни"=' + char(39) + EXTRACTSTR (sTmpGTD, 1, '/') + char(39) ,'EXCEL_DB');
                      IF(RECORDCOUNT ('qEXEL_VAL')<>0,
                        BLOCK(
                          bFoundVAL := True;
                          sCodVAL :=  "qEXEL_VAL"." код валюты цены договора";
                        )
                      );
                      CLOSEDATASET ('qEXEL_VAL');
                      iNowCountVAL := iNowCountVAL + 1;
                    )
                  );
                )//BLOCK)
             );//IF(LOCATE('REL_MAIN
           )//Block IF
        );//IF((sTmpGTD <>
        IF(bFoundGTD,
          BLOCK(
            APPENDRECORD('tmpCOMM');
            EDIT('tmpCOMM');
            SETFIELDVALUE('tmpCOMM',
                          'G32_EX',"qEXEL_CMR"."№ товара ",
                          'DOC_NO', sTmpGTD,
                          'G33_EX', "qEXEL_CMR"."код ТН ВЭД",
                          'CMR_EX', "qEXEL_CMR"."номер документа",
                          'INV_EX','',
                          'TIR_EX',sNumDCD_EX,
                          'G35_EX', "qEXEL_CMR"."вес брутто (кг)",
                          'CARNO_EX', sTranspNo_EX,
