         //sCMR_STS := FROM_STS.PAPERNO;
       )
    );//if(RECORDCOUNT('FROM_STS') <> 0,

    CLOSEDATASET('FROM_STS');
    IF(TmpCOMM.DOC_NO<>sTmpGTD,
       BLOCK(
         sCMR_STS := '';
         sINV_STS := '';
         sNumDCD_STS := '';
         sTranspNo_STS := '';
         sCauseINV_CMR := '';
         OPENQUERY('SELECT_CMR', 'STS_DB', 'SELECT PAPERNO FROM KR_PAPER WHERE PLACEID='+iPLACEID+' AND ID='+iID+' AND PAPERNAME = "CMR" ');
         FIRST('SELECT_CMR');
         WHILE(EOF('SELECT_CMR') = 0,
           BLOCK(
             IF(STRPOS (SELECT_CMR.PAPERNO, tmpComm.CMR_EX) = 0, sCauseINV_CMR := sCauseINV_CMR + 'Указанный CMR ' + SELECT_CMR.PAPERNO + ' не совпал/');
             NEXT('SELECT_CMR');
           )
         );
         sCMR_STS := UNIONVALUES ('SELECT_CMR', ['PAPERNO'], ' ;', '');
         CLOSEDATASET('SELECT_CMR');

         OPENQUERY('SELECT_INV', 'STS_DB', 'SELECT PAPERNO FROM KR_PAPER WHERE PLACEID='+iPLACEID+' AND ID='+iID+' AND PAPERNAME = "ИНВ" ');
         FIRST('SELECT_INV');
         WHILE(EOF('SELECT_INV') = 0,
           BLOCK(
             IF(STRPOS (SELECT_INV.PAPERNO, tmpComm.INV_EX) = 0, sCauseINV_CMR := sCauseINV_CMR + 'Указанный ИНВ ' + SELECT_INV.PAPERNO + ' не совпал/');
             NEXT('SELECT_INV');
           )
         );
         sINV_STS := UNIONVALUES ('SELECT_INV', ['PAPERNO'], ' ;', '');
         CLOSEDATASET('SELECT_INV');

         OPENQUERY('SELECT_DCD', 'STS_DB', 'SELECT PAPERNO FROM KRD_DCD WHERE PLACEID='+iPLACEID+' AND ID='+iID);

         sNumDCD_STS := UNIONVALUES ('SELECT_DCD', ['PAPERNO'], ' ;', '');
         CLOSEDATASET('SELECT_DCD');

         OPENQUERY('SELECT_TR', 'STS_DB', 'SELECT CARNO FROM KR_TRANS WHERE PLACEID='+iPLACEID+' AND ID='+iID);
         sTranspNo_STS := UNIONVALUES ('SELECT_TR', ['CARNO'], ' ;', '');
         CLOSEDATASET('SELECT_TR');
       )
    );
    //showmessage(2);
    IF(tmpComm.G35_EX <> iSTS_RG35, sCause := sCause + ' не совпал вес/');
    IF(tmpComm.G42_COD_EX <> sG42_CUR, sCause := sCause + ' не совпал код валюты/');
    IF(STRPOS( tmpComm.CARNO_EX, sTranspNo_STS) = 0, sCause := sCause + ' не совпало транспортное средство/');
    IF(STRPOS( tmpComm.TIR_EX, sNumDCD_STS) = 0, sCause := sCause + ' не совпал TIR/');
    sCause := sCauseINV_CMR + sCause;
    
    EDIT('tmpCOMM');
    SETFIELDVALUE('tmpCOMM',
                  'PLACEID',iPLACEID,
                  'ID',iID,
                  'G32', iG32,
                  'RELEASE_NO', sRelNO,
                  'NBD', sNBD,
                  'G33', sSTS_G33,
                  'CMR', sCMR_STS,
                  'INV',sINV_STS,
                  'TIR',sNumDCD_STS,
                  'G35', iSTS_RG35,
                  'CARNO', sTranspNo_STS,
                  'G42_CODE',sG42_CUR,
                  'CAUSE', sCause,
                  'MATCHED', IF(sCause <> '', 0, 1)

    );
    POST('tmpCOMM');
    sTmpGTD := TmpCOMM.DOC_NO;
    iCountProgress := iCountProgress + 1;
    SETPROGRESS(iCountProgress, 100, iCount);
    NEXT ('tmpCOMM');
    //showmessage(TmpCOMM.G33_EX);
    //showmessage(EOF('TmpCOMM'));
    //showmessage(1);
  )
);

HIDEPROGRESS();

}







