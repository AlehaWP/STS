      If(vNBD = '',
        TryExcept(
          Block( // try ‚˚˜ËÒÎËÚ¸ ÌÓÏÂ ƒŒ1 ‰Îˇ ‚˚·‡ÌÌÓÈ ÎËˆÂÌÁËË
            bookExpr := UPPERSTR(INIFILE('Docs','MakeBD_No', 'LEFTPAD(GENNO("NBD_KPS"+'+iDocPlaceID+', "π ƒŒ1(Ï‚)/ƒ” «“ "), 7, "0")'));
