  sSQL := 'SELECT MAIN_ID, ID FROM KRD_MAIN WHERE PLACEID=' + iDocPlaceID + ' AND NBD=' +char(39)+ vNBD +char(39)+ ' AND BD_DATE=' +SQLDate(sDO1Date, GetBaseDriver (0));
  
