                  'cat_ru:PersonSurname', 'PersonSurname',
                  'cat_ru:PersonName', 'PersonName',
                  'cat_ru:PersonMiddleName', 'PersonMiddleName',
                  'cat_ru:PersonPost', 'PersonPost',
                  'catWH_ru:LNP', 'LNP',
        );
