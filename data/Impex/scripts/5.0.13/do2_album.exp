          Block(
            VAR ('sRegDate', String, FDT('YYYY-MM-DD', REL_MAIN.REG_TIME));
            IF (sRegDate = '', sRegDate := FDT('YYYY-MM-DD', EXCLUDEDATE (REL_MAIN.REG_RELEASE_NO)));
