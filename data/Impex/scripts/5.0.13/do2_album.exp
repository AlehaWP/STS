    XMLNodeSetValues (XMLBruttoVolQuant, '', 'catWH_ru:MeasureUnitQualifierName', IF (resG35 > 0, 'КГ', 'КГ'));
    XMLNodeSetValues (XMLBruttoVolQuant, '', 'catWH_ru:MeasureUnitQualifierCode', IF (resG35 > 0, '166', '166'));
      )
