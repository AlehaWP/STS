        										   'cat_ru:RegistrationDate', FDT('YYYY-MM-DD', EXCLUDEDATE (REL_MAIN.REG_RELEASE_NO)),
        										   'cat_ru:GTDNumber',        CUSTOMSNOPART (REL_MAIN.REG_RELEASE_NO, 3, '')
        								 );
        							   ),
        							4, Block(
          								 XmlNodeSetValues (XMLCustomRegistration, 'catWH_ru:RegisterNumber',
         										   'cat_ru:CustomsCode',      CUSTOMSNOPART (REL_MAIN.REG_RELEASE_NO, 1, ''),
        										   'cat_ru:RegistrationDate', FDT('YYYY-MM-DD', EXCLUDEDATE (REL_MAIN.REG_RELEASE_NO)),
        										   'cat_ru:GTDNumber',        CUSTOMSNOPART (REL_MAIN.REG_RELEASE_NO, 3, '')
        								 );
        							   ),
        							5, Block(
          								 XmlNodeSetValues (XMLCustomRegistration, 'catWH_ru:RegisterNumber',
         										   'cat_ru:CustomsCode',      CUSTOMSNOPART (REL_MAIN.REG_RELEASE_NO, 1, ''),
        										   'cat_ru:RegistrationDate', FDT('YYYY-MM-DD', EXCLUDEDATE (REL_MAIN.REG_RELEASE_NO)),
        										   'cat_ru:GTDNumber',        CUSTOMSNOPART (REL_MAIN.REG_RELEASE_NO, 3, ''),
