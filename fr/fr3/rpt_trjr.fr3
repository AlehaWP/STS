<TfrxReport Version="4.1.24" DotMatrixReport="False" EngineOptions.ConvertNulls="False" EngineOptions.DoublePass="True" IniFile="\Software\Fast Reports" PreviewOptions.Buttons="4095" PreviewOptions.Zoom="1" PrintOptions.Printer="По умолчанию" PrintOptions.PrintOnSheet="0" ReportOptions.CreateDate="38267.7381034722" ReportOptions.Description.Text="" ReportOptions.Name="Журнал учета тр.средств (пр.290)" ReportOptions.LastChange="39204.4949534606" ScriptLanguage="PascalScript" ScriptText.Text="var&#13;&#10;  CustomsDocsOnly: Boolean;&#13;&#10;  KOL, n, iVariant: Integer; // выбранный вариант (период =1, текущая книга учета=2)&#13;&#10;  date_begin, date_end, vOutDate: TDateTime; // конец периода&#13;&#10;  period: string;&#13;&#10;  MESTO, NNN, id_m, placeid_m, placeid_p, id_p, paper,&#13;&#10;  CARNO, dat, NO_BY_TRANSP_JOURNAL,&#13;&#10;  STORE_DOC_NO, Text3, Text2, Text1, MAIN_PLACEID, no_lic: string;&#13;&#10;&#13;&#10;procedure Page1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  iVariant := 0; // выбранный вариант (период =1, текущая книга учета=2)&#13;&#10;  MAIN_PLACEID := '0'; // выбранная складская площпдка&#13;&#10;  date_begin := DATE; // начало периода&#13;&#10;  date_end := DATE; // конец периода&#13;&#10;  RadioButton1.Checked := True;&#13;&#10;  Label33.Enabled := True;&#13;&#10;  DateEdit1.Enabled := True;&#13;&#10;  DateEdit2.Enabled := True;&#13;&#10;&#13;&#10;  DateEdit1.Date := STRTODATEDEF(COMPUTE('INIFILE(&#34;Print&#34;,&#34;PeriodStart&#34;,&#34;&#34;)'), DATE);&#13;&#10;  DateEdit2.Date := STRTODATEDEF(COMPUTE('INIFILE(&#34;Print&#34;,&#34;PeriodEnd&#34;,&#34;&#34;)'), DATE);&#13;&#10;&#13;&#10;  ColorButton1.Enabled := False;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure ColorButton1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  CustomsDocsOnly := (COMPUTE('INIFILE(&#34;RegBook&#34;, &#34;CustomsDocsOnly&#34;, 1)') = '1') or&#13;&#10;                     (COMPUTE('INIFILE(&#34;Database&#34;, &#34;StoreDocsBlock&#34;, 1)') = '0');&#13;&#10;  if Label33.Enabled  then begin&#13;&#10;    iVariant := 1;&#13;&#10;    date_begin := Trunc(DateEdit1.Date);&#13;&#10;    date_end := Trunc(DateEdit2.Date) + STRTOTIME('23:59:59');&#13;&#10;  end else if Label11.Enabled  then&#13;&#10;    iVariant := 2;&#13;&#10;  if iVariant = 1 then begin&#13;&#10;    Table1.Active := True;&#13;&#10;    Table2.Active := True;&#13;&#10;    Table3.Active := True;&#13;&#10;    Table4.Active := True;&#13;&#10;    Table5.Active := True;&#13;&#10;    Table6.TableName := CorrectTableName('RELEASE');        &#13;&#10;    Table6.Active := True;&#13;&#10;&#13;&#10;    Table1.First;&#13;&#10;    Table2.First;&#13;&#10;    Table3.First;&#13;&#10;    Table4.First;&#13;&#10;    if not CustomsDocsOnly then&#13;&#10;      Table5.First;&#13;&#10;    Table6.First;&#13;&#10;    MasterData1.DataSet := Table1&#13;&#10;  end;&#13;&#10;&#13;&#10;  if iVariant = 2 then begin&#13;&#10;    Compute('First (&#34;KRD_MAIN&#34;)');&#13;&#10;    Compute('First (&#34;KRD_SLOTM&#34;)');&#13;&#10;    Compute('First (&#34;KRD_PAPERS&#34;)');&#13;&#10;    Compute('First (&#34;KRD_TRANSP&#34;)');&#13;&#10;    Compute('First (&#34;STORES&#34;)');&#13;&#10;    Compute('First (&#34;REL_MAIN&#34;)');&#13;&#10;    if not CustomsDocsOnly then&#13;&#10;      Compute('First (&#34;OUT_MAIN&#34;)');&#13;&#10;    MasterData1.DataSetName := 'KRD_MAIN'&#13;&#10;  end;&#13;&#10;  Compute('WRITEINIFILE(&#34;Print&#34;,&#34;PeriodStart&#34;,&#34;'+DATETOSTR(DateEdit1.Date)+'&#34;)');&#13;&#10;  Compute('WRITEINIFILE(&#34;Print&#34;,&#34;PeriodEnd&#34;,&#34;'+DateToStr(DateEdit2.Date)+'&#34;)');&#13;&#10;end;&#13;&#10;&#13;&#10;procedure RadioButton1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  Label33.Enabled := True;&#13;&#10;  Label11.Enabled := False;&#13;&#10;  DateEdit1.Enabled := True;&#13;&#10;  DateEdit2.Enabled := True;&#13;&#10;  if Edit2.Text= '' then&#13;&#10;    ColorButton1.Enabled := False&#13;&#10;  else&#13;&#10;    ColorButton1.Enabled := True;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure RadioButton2OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  Label11.Enabled := True;&#13;&#10;  Label33.Enabled := False;&#13;&#10;  DateEdit1.Enabled := False;&#13;&#10;  DateEdit2.Enabled := False;&#13;&#10;  ColorButton1.Enabled := True;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure ColorButton3OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  COMPUTE(' globalvar (&#34;strStoreNumLic&#34;,String)');&#13;&#10;  COMPUTE(' globalvar (&#34;sPLACEID&#34;,Integer)');&#13;&#10;  COMPUTE(' globalvar (&#34;strStoreNumLic_Lit&#34;,String)');&#13;&#10;  COMPUTE(' globalvar (&#34;strStoreNum&#34;,String)');&#13;&#10;  COMPUTE(' globalvar (&#34;strStoreName&#34;,String)');&#13;&#10;  COMPUTE('OPENQUERY (&#34;Stors&#34;, &#34;STS_DB&#34;, &#34;Select * from STORES&#34;)');&#13;&#10;  COMPUTE('SelectValues(&#34;Список складов&#34;, &#34;Stors&#34;,[[&#34;LICENCENO&#34;,   &#34;Номер лицензии&#34;, 15],[&#34;LICENCENO_EXT&#34;,   &#34; &#34;, 1],[&#34;STORE_NO&#34;,   &#34;Номер склада&#34;, 15],[&#34;NAME&#34;,   &#34;Название склада&#34;, 30],[&#34;PlaceId&#34;,   &#34;&#34;, 10]],[[&#34;LICENCENO&#34;, &#34;strStoreNumLic&#34;],[&#34;STORE_NO&#34;, &#34;strStoreNum&#34;],[&#34;NAME&#34;, &#34;strStoreName&#34;],[&#34;PlaceId&#34;, &#34;sPlaceId&#34;],],&#34;&#34;, &#34;STS_DB&#34;)');&#13;&#10;&#13;&#10;&#13;&#10;  no_lic := COMPUTE('value(&#34;strStoreNumLic&#34;)');  // номер лицензии склада&#13;&#10;  MAIN_PLACEID := COMPUTE('value(&#34;sPLACEID&#34;)');   // ID склада&#13;&#10;&#13;&#10;  Edit2.Text := COMPUTE('value(&#34;strStoreNumLic&#34;)') +COMPUTE('value(&#34;strStoreNumLic_Lit&#34;)') ;      //   номер лицензии&#13;&#10;  Edit1.Text := '№ скл.: ' + COMPUTE('value(&#34;strStoreNum&#34;)') ; //   номер склада&#13;&#10;  Edit3.Text := COMPUTE('value(&#34;strStoreName&#34;)') ;     //   название склада&#13;&#10;  if Edit2.Text= '' then&#13;&#10;    ColorButton1.Enabled := False&#13;&#10;  else&#13;&#10;    ColorButton1.Enabled := True;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Page2OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;end;&#13;&#10;&#13;&#10;procedure MasterData1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  if iVariant = 1 then&#13;&#10;   if ((&#60;Table1.&#34;G261&#34;&#62; = '30') and (&#60;Table1.&#34;PLACEID&#34;&#62; = MAIN_PLACEID) and&#13;&#10;      ((&#60;Table1.&#34;ARR_DATE&#34;&#62; &#62;= date_begin) and(&#60;Table1.&#34;ARR_DATE&#34;&#62; &#60;= date_end))) then&#13;&#10;     MasterData1.Visible := True&#13;&#10;   else&#13;&#10;     MasterData1.Visible := False&#13;&#10;  else if iVariant = 2 then&#13;&#10;    if &#60;KRD_MAIN.&#34;G261&#34;&#62; = '30' then&#13;&#10;      MasterData1.Visible := True&#13;&#10;    else&#13;&#10;      MasterData1.Visible := False;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Memo2OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  Text3 := '';&#13;&#10;  Text2 := '';&#13;&#10;  Text1 := '';&#13;&#10;  if iVariant = 1 then begin&#13;&#10;    Text3 := Edit3.Text;&#13;&#10;    Text2 := Edit2.Text;&#13;&#10;    Text1 := COMPUTE('value(&#34;strStoreNum&#34;)');&#13;&#10;  end else if iVariant = 2 then begin&#13;&#10;    if Edit3.Text = '' then begin&#13;&#10;{      Text3 := &#60;Table7.&#34;NAME&#34;&#62;;&#13;&#10;      Text2 := &#60;Table7.&#34;LICENCENO&#34;&#62;;&#13;&#10;      Text1 := &#60;Table7.&#34;STORE_NO&#34;&#62;;}&#13;&#10;    end else begin&#13;&#10;      Text3 := Edit3.Text;&#13;&#10;      Text2 := Edit2.Text;&#13;&#10;      Text1 := COMPUTE('value(&#34;strStoreNum&#34;)');&#13;&#10;    end&#13;&#10;  end&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Memo11OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  NO_BY_TRANSP_JOURNAL :=''; // номер п/п&#13;&#10;  if iVariant = 1 then&#13;&#10;    NO_BY_TRANSP_JOURNAL := &#60;Table1.&#34;NO_BY_TRANSP_JOURNAL&#34;&#62;&#13;&#10;  else if iVariant = 2 then&#13;&#10;    NO_BY_TRANSP_JOURNAL := &#60;KRD_MAIN.&#34;NO_BY_TRANSP_JOURNAL&#34;&#62;;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Memo12OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  if iVariant = 1 then&#13;&#10;    dat := FORMATDATETIME('dd.mm.yyyy hh:mm', &#60;Table1.&#34;ARR_DATE&#34;&#62;)&#13;&#10;  else if iVariant = 2 then&#13;&#10;    dat := FORMATDATETIME('dd.mm.yyyy hh:mm', &#60;KRD_MAIN.&#34;ARR_DATE&#34;&#62;)  ;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Memo13OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  CARNO:='';&#13;&#10;  if iVariant = 1 then&#13;&#10;    CARNO := &#60;Table2.&#34;CARNO&#34;&#62;&#13;&#10;  else if iVariant = 2 then&#13;&#10;    CARNO :=  &#60;KRD_TRANSP.&#34;CARNO&#34;&#62;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Memo14OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  paper:=''; //документы&#13;&#10;  if iVariant = 1 then begin&#13;&#10;    Table3.First;&#13;&#10;    while not Table3.eof do begin&#13;&#10;      if &#60;Table3.&#34;PAPERNAME&#34;&#62; = 'CMR' then begin&#13;&#10;        if paper &#60;&#62; '' then paper := paper + ', ';&#13;&#10;        paper := paper + &#60;Table3.&#34;PAPERNAME&#34;&#62; + #160 + &#60;Table3.&#34;PAPERNO&#34;&#62;;&#13;&#10;        if &#60;Table3.&#34;PAPERDATE&#34;&#62; &#60;&#62; Null then&#13;&#10;          paper := paper + #160'ОТ'#160 + DateToStr(&#60;Table3.&#34;PAPERDATE&#34;&#62;);&#13;&#10;        if &#60;Table3.&#34;PAPER_REG_NO&#34;&#62; &#60;&#62; Null then&#13;&#10;          paper := paper + ' / ' + &#60;Table3.&#34;PAPER_REG_NO&#34;&#62;;&#13;&#10;      end;&#13;&#10;      Table3.Next;&#13;&#10;    end&#13;&#10;  end else if iVariant = 2 then begin&#13;&#10;    id_p := &#60;KRD_MAIN.&#34;ID&#34;&#62;;&#13;&#10;    placeid_p := &#60;KRD_MAIN.&#34;PLACEID&#34;&#62;;&#13;&#10;    Compute('SETRANGE(&#34;KRD_PAPERS_2&#34;,[ &#34;'+ placeid_p +'&#34;, &#34;'+ id_p +'&#34;]) ');&#13;&#10;    KOL := StrToInt(Compute('RECORDCOUNT(&#34;KRD_PAPERS_2&#34;) '));&#13;&#10;    KRD_PAPERS_2.First;&#13;&#10;    while not KRD_PAPERS_2.Eof do begin&#13;&#10;      if &#60;KRD_PAPERS_2.&#34;PAPERNAME&#34;&#62; = 'CMR' then begin&#13;&#10;        if paper &#60;&#62; '' then paper := paper + ', ';&#13;&#10;        paper := paper + &#60;KRD_PAPERS_2.&#34;PAPERNAME&#34;&#62; + #160 +&#13;&#10;                               &#60;KRD_PAPERS_2.&#34;PAPERNO&#34;&#62;;&#13;&#10;        if &#60;KRD_PAPERS_2.&#34;PAPERDATE&#34;&#62; &#60;&#62; Null then&#13;&#10;          paper := paper + #160'ОТ'#160 + DateToStr(&#60;KRD_PAPERS_2.&#34;PAPERDATE&#34;&#62;);&#13;&#10;        if &#60;KRD_PAPERS_2.&#34;PAPER_REG_NO&#34;&#62; &#60;&#62; Null then&#13;&#10;          paper := paper + ' / ' + &#60;KRD_PAPERS_2.&#34;PAPER_REG_NO&#34;&#62;;&#13;&#10;      end;&#13;&#10;      KRD_PAPERS_2.Next;&#13;&#10;    end;&#13;&#10;    Compute('CANCELRANGE(&#34;KRD_PAPERS_2&#34;) ');&#13;&#10;  end&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Memo15OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  NNN := '';&#13;&#10;  if iVariant = 1 then&#13;&#10;    IF &#60;Table1.&#34;NU&#34;&#62; = NULL then&#13;&#10;      NNN:=&#60;Table1.&#34;PP&#34;&#62;&#13;&#10;    else&#13;&#10;      NNN:=&#60;Table1.&#34;NU&#34;&#62;&#13;&#10;  else if iVariant = 2 then&#13;&#10;    IF &#60;KRD_MAIN.&#34;NU&#34;&#62; = NULL then&#13;&#10;      NNN:=&#60;KRD_MAIN.&#34;PP&#34;&#62;&#13;&#10;    else&#13;&#10;      NNN:=&#60;KRD_MAIN.&#34;NU&#34;&#62;;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Memo16OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  STORE_DOC_NO := '';&#13;&#10;  if iVariant = 1 then&#13;&#10;    STORE_DOC_NO := &#60;Table1.&#34;STORE_DOC_NO&#34;&#62;&#13;&#10;  else if iVariant = 2 then&#13;&#10;    STORE_DOC_NO := &#60;KRD_MAIN.&#34;STORE_DOC_NO&#34;&#62;;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Memo17OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  MESTO := '';&#13;&#10;  if iVariant = 1 then begin&#13;&#10;    id_m := &#60;Table1.&#34;ID&#34;&#62;;&#13;&#10;    placeid_m := &#60;Table1.&#34;PLACEID&#34;&#62;;&#13;&#10;  end else if iVariant = 2 then begin&#13;&#10;    id_m := &#60;KRD_MAIN.&#34;ID&#34;&#62;;&#13;&#10;    placeid_m := &#60;KRD_MAIN.&#34;PLACEID&#34;&#62;;&#13;&#10;  end;&#13;&#10;  Compute('LOCATE(&#34;KRD_SLOTM_2&#34;, &#34;PLACEID;ID;KRECC&#34;, [&#34;'+placeid_m+'&#34;,&#34;'+id_m+'&#34;,&#34;46&#34;])');&#13;&#10;  MESTO := &#60;KRD_SLOTM_2.&#34;GC_1&#34;&#62;;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Memo18OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  vOutDate := 0;&#13;&#10;  if iVariant = 1 then begin&#13;&#10;    if &#60;Table1.&#34;TR_OUTDATE&#34;&#62; &#60;&#62; NULL then&#13;&#10;      vOutDate := &#60;Table1.&#34;TR_OUTDATE&#34;&#62;&#13;&#10;    else if &#60;Table5.&#34;G542&#34;&#62; &#60;&#62; NULL then&#13;&#10;      vOutDate := &#60;Table5.&#34;G542&#34;&#62;&#13;&#10;    else if &#60;Table6.&#34;OUT_DATE&#34;&#62; &#60;&#62; NULL then&#13;&#10;      vOutdate := &#60;Table6.&#34;OUT_DATE&#34;&#62;&#13;&#10;    else if &#60;Table6.&#34;RELEASE_DATE&#34;&#62; &#60;&#62; NULL then&#13;&#10;      vOutDate := &#60;Table6.&#34;RELEASE_DATE&#34;&#62;&#13;&#10;  end else if iVariant = 2 then begin&#13;&#10;    if &#60;KRD_MAIN.&#34;TR_OUTDATE&#34;&#62;&#60;&#62; NULL then&#13;&#10;      vOutDate :=&#60;KRD_MAIN.&#34;TR_OUTDATE&#34;&#62;&#13;&#10;    else if &#60;REL_MAIN.&#34;OUT_DATE&#34;&#62; &#60;&#62; NULL then&#13;&#10;      vOutDate := &#60;REL_MAIN.&#34;OUT_DATE&#34;&#62;&#13;&#10;    else if &#60;REL_MAIN.&#34;RELEASE_DATE&#34;&#62; &#60;&#62; NULL then&#13;&#10;      vOutDate := &#60;REL_MAIN.&#34;RELEASE_DATE&#34;&#62;&#13;&#10;    else if not CustomsDocsOnly and (&#60;OUT_MAIN.&#34;G542&#34;&#62; &#60;&#62; NULL) then&#13;&#10;      vOutDate :=&#60;OUT_MAIN.&#34;G542&#34;&#62;&#13;&#10;  end&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Memo19OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  period := '';&#13;&#10;  if iVariant = 1 then&#13;&#10;    period := 'c ' + FORMATDATETIME('dd.mm.yyyy', date_begin) + ' по ' + FORMATDATETIME('dd.mm.yyyy', date_end);&#13;&#10;end;&#13;&#10;&#13;&#10;begin&#13;&#10;&#13;&#10;end." OnStartReport="OnStartReport" PropData="">
