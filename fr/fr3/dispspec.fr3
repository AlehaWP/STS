    <TfrxBDEQuery Name="qryDispSpec" GroupIndex="0" UserName="qryDispSpec" FieldAliases.Text="" SQL.Text="SELECT DISTINCT KC.PLACEID, KC.ID,&#13;&#10;                KC.G312, KC.FORESTLENGTH, KC.FORESTTYPE, KC.FORESTGOST,&#13;&#10;                KCM.OUT_TRANSP_NO&#13;&#10;FROM KRD_COMM KC&#13;&#10;     INNER JOIN KRD_CMAP KCM ON KC.PLACEID = KCM.PLACEID&#13;&#10;                            AND KC.ID = KCM.ID&#13;&#10;                            AND KC.G32 = KCM.G32&#13;&#10;     INNER JOIN REL_COMM RC ON RC.PLACEID = KCM.PLACEID&#13;&#10;                           AND RC.ID = KCM.ID&#13;&#10;                           AND RC.G32 = KCM.G32&#13;&#10;                           AND RC.COUNTER = KCM.RELEASE_COUNTER&#13;&#10;     INNER JOIN RELEASE R ON R.PLACEID = RC.PLACEID&#13;&#10;                         AND R.ID = RC.ID&#13;&#10;                         AND R.COUNTER = RC.COUNTER&#13;&#10;WHERE KC.PLACEID = :PLACEID&#13;&#10;  AND KC.ID = :ID&#13;&#10;  AND R.RELEASE_NO = :RELEASE_NO&#13;&#10;  AND KCM.OUT_TRANSP_NO &#60;&#62; ''&#13;&#10;  AND ((:ALL_TRANSP = False&#13;&#10;       AND KCM.OUT_TRANSP_NO = :TRANSPNO)&#13;&#10;    OR :ALL_TRANSP = True)&#13;&#10;ORDER BY KCM.OUT_TRANSP_NO" DatabaseName="STS_DB" SessionName="Default" PropData=""/>
