    <TfrxBDEQuery Name="qryRC1RT" GroupIndex="0" UserName="qryRC1RT" FieldAliases.Text="" Master="qryDispSpecRT" SQL.Text="SELECT DISTINCT KC.FORESTLENGTH, KC.FORESTDIAMETER, RT.TRANSPNO AS OUT_TRANSP_NO,&#13;&#10;       KC.PLACEID, KC.ID,&#13;&#10;       RT.COUNTER&#13;&#10;FROM RELTRANS RT&#13;&#10;     INNER JOIN RELEASE R ON RT.PLACEID = R.PLACEID&#13;&#10;                          AND RT.ID = R.ID                                             &#13;&#10;                          AND RT.COUNTER = R.COUNTER&#13;&#10;     INNER JOIN KRD_COMM KC ON RT.PLACEID = KC.PLACEID&#13;&#10;                          AND RT.ID = KC.ID&#13;&#10;                          AND RT.G32 = KC.G32   &#13;&#10;WHERE RT.PLACEID = :PLACEID&#13;&#10;  AND RT.ID = :ID&#13;&#10;  AND RT.COUNTER = :COUNTER&#13;&#10;  AND RT.TRANSPNO = :OUT_TRANSP_NO&#13;&#10;  AND R.RELEASE_NO = :RELEASE_NO&#13;&#10;  AND KC.FORESTLENGTH = :FORESTLENGTH&#13;&#10;  AND KC.FORESTTYPE = :FORESTTYPE&#13;&#10;  AND KC.FORESTGOST = :FORESTGOST&#13;&#10;  AND KC.FORESTDIAMETER &#60;= 24&#13;&#10;ORDER BY KC.FORESTDIAMETER" DatabaseName="STS_DB" SessionName="Default" PropData=""/>
    <TfrxBDEQuery Name="qryRC2RT" GroupIndex="0" UserName="qryRC2RT" FieldAliases.Text="" Master="qryDispSpecRT" SQL.Text="SELECT DISTINCT KC.FORESTLENGTH, KC.FORESTDIAMETER, RT.TRANSPNO AS OUT_TRANSP_NO,&#13;&#10;       KC.PLACEID, KC.ID,&#13;&#10;       RT.COUNTER&#13;&#10;FROM RELTRANS RT&#13;&#10;     INNER JOIN RELEASE R ON RT.PLACEID = R.PLACEID&#13;&#10;                          AND RT.ID = R.ID                                             &#13;&#10;                          AND RT.COUNTER = R.COUNTER&#13;&#10;     INNER JOIN KRD_COMM KC ON RT.PLACEID = KC.PLACEID&#13;&#10;                          AND RT.ID = KC.ID&#13;&#10;                          AND RT.G32 = KC.G32   &#13;&#10;WHERE RT.PLACEID = :PLACEID&#13;&#10;  AND RT.ID = :ID&#13;&#10;  AND RT.COUNTER = :COUNTER&#13;&#10;  AND RT.TRANSPNO = :OUT_TRANSP_NO&#13;&#10;  AND R.RELEASE_NO = :RELEASE_NO&#13;&#10;  AND KC.FORESTLENGTH = :FORESTLENGTH&#13;&#10;  AND KC.FORESTTYPE = :FORESTTYPE&#13;&#10;  AND KC.FORESTGOST = :FORESTGOST&#13;&#10;  AND KC.FORESTDIAMETER &#62; 24&#13;&#10;  AND KC.FORESTDIAMETER &#60;= 32&#13;&#10;ORDER BY KC.FORESTDIAMETER" DatabaseName="STS_DB" SessionName="Default" PropData=""/>
