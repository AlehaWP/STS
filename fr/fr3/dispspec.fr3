    <TfrxBDEQuery Name="qrySORT1RT" GroupIndex="0" UserName="qrySORT1RT" FieldAliases.Text="" Master="qryRC1RT" SQL.Text="SELECT SUM(RT.RELEASE_G311) AS SUM_RG311,&#13;&#10;       SUM(RT.RELEASE_G315C) AS SUM_RG315С&#13;&#10;FROM RELTRANS RT&#13;&#10;     INNER JOIN KRD_COMM KC ON RT.PLACEID = KC.PLACEID&#13;&#10;                       AND RT.ID = KC.ID&#13;&#10;                       AND RT.G32 = KC.G32&#13;&#10;WHERE RT.PLACEID = :PLACEID&#13;&#10;  AND RT.ID = :ID&#13;&#10;  AND RT.COUNTER = :COUNTER&#13;&#10;  AND RT.TRANSPNO = :OUT_TRANSP_NO&#13;&#10;  AND KC.FORESTDIAMETER = :FORESTDIAMETER&#13;&#10;  AND KC.FORESTLENGTH = :FORESTLENGTH&#13;&#10;  AND KC.FORESTTYPE = :FORESTTYPE&#13;&#10;  AND KC.FORESTSORT = 1&#13;&#10;GROUP BY KC.FORESTLENGTH,&#13;&#10;         KC.FORESTDIAMETER,&#13;&#10;         KC.FORESTSORT,&#13;&#10;         KC.FORESTTYPE,&#13;&#10;         RT.TRANSPNO" DatabaseName="STS_DB" SessionName="Default" PropData=""/>
