<TfrxReport Version="4.6.78" DotMatrixReport="False" EngineOptions.ConvertNulls="False" EngineOptions.DoublePass="True" IniFile="\Software\Fast Reports" PreviewOptions.Buttons="4095" PreviewOptions.Zoom="1" PrintOptions.Printer="По умолчанию" PrintOptions.PrintOnSheet="0" ReportOptions.CreateDate="38250.6715903241" ReportOptions.Description.Text="" ReportOptions.Name="Форма ДО1 (Пр.№ 715 от 06.04.2011)" ReportOptions.LastChange="41135.4424171181" ReportOptions.VersionMajor="2" ScriptLanguage="PascalScript" ScriptText.Text="Var&#13;&#10;  iTotalPages, nPP: Integer;&#13;&#10;  vPlaceId, vId, vG32, vMainID: Integer;                                             //&#13;&#10;  sValBukCode, vStorageEndDate,&#13;&#10;  sBackSheet, sCurSheet, sDocNames, sActs,             &#13;&#10;  sStoreNum, &#13;&#10;  vG311, vG35, vG42, v7, sSumVal, sSumCod: String;&#13;&#10;  iSumG311, iSumG315C, iSumG315A, iSumG35, iSumG38, iSumG42 : Double; &#13;&#10;  sTypeSVH, sNameSVH, sTypeDoc, sDateDoc, sNumDoc, sConstStore, sConts: string;&#13;&#10;&#13;&#10;  vMainTable, vSQLText: String;&#13;&#10;  &#13;&#10;  bBrutto, bNetto, bUnits, bVolume: Boolean;&#13;&#10;  bGoodStoreNumKPS: Boolean;                             &#13;&#10;&#13;&#10;  bUseFactInfo: Boolean;&#13;&#10;  bUseConts: Boolean;&#13;&#10;  bPrintItog: Boolean;&#13;&#10;&#13;&#10;  bPrintG42, bPrintUSDG42, bPrintEurG42, bPrintRubG42, bPrintClearG42 : Boolean;  &#13;&#10;    &#13;&#10;  bPrintWaybill, bPrintWaybill2, bPrintBill: Boolean;&#13;&#10;  bRecipient :boolean;                                             &#13;&#10;  bAcceptDate, bBdDate, bBegKeep, bG542: Boolean;                                                             &#13;&#10;&#13;&#10;  bPrintRegNum, bPrintContType,bPrintPlomb: Boolean;&#13;&#10;  bStorageDateNoPrint, bPartPlace: Boolean;                                               &#13;&#10;&#13;&#10;  bOnlyBaseList, bOnlyDopList, bBaseDopList : Boolean;                                                                  &#13;&#10;  sPartPlaceText: String;&#13;&#10;&#13;&#10;  n : Integer;                             &#13;&#10;  sPrim : String;                                                     &#13;&#10;  vDate : Date;&#13;&#10;&#13;&#10;  iG311, iG42, iG315A, iG35, iG38, iG315C : Double;&#13;&#10;  sG33, sG312, sG315, sG315CN, sG41A, sG31_82, sSumV7 : string;  &#13;&#10;  sDopListNO : string;      &#13;&#10;Const&#13;&#10;  sIniFullPath = ProgramPath() + 'print.ini';&#13;&#10;  sIniSection = 'do1.fr3';&#13;&#10;  sIniSectionCommon = 'Common';&#13;&#10;&#13;&#10;&#13;&#10;&#13;&#10;&#13;&#10;procedure DialogPage1OnShow(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  chkRegNum.Checked := bPrintRegNum;&#13;&#10;    &#13;&#10;  chkItog.Checked := bPrintItog;&#13;&#10;  chkG42.Enabled := bPrintItog;&#13;&#10;  chkG42.Checked := bPrintG42;&#13;&#10;  chkUSDG42.Enabled := bPrintG42 and bPrintItog;      &#13;&#10;  chkEURG42.Enabled := bPrintG42 and bPrintItog;  &#13;&#10;  chkRUBG42.Enabled := bPrintG42 and bPrintItog;&#13;&#10;  chkClearG42.Enabled := bPrintG42 and bPrintItog;&#13;&#10;    &#13;&#10;  chkUSDG42.Checked := bPrintUSDG42;      &#13;&#10;  chkEURG42.Checked := bPrintEURG42;  &#13;&#10;  chkRUBG42.Checked := bPrintRUBG42;&#13;&#10;  chkClearG42.Checked := bPrintClearG42;      &#13;&#10;  &#13;&#10;  &#13;&#10;  chkBrutto.Checked := bBrutto;&#13;&#10;  chkNetto.Enabled := bBrutto;&#13;&#10;  chkNetto.Checked := bNetto;&#13;&#10;  chkUnits.Checked := bUnits;&#13;&#10;  chkVolume.Checked := bVolume;&#13;&#10;&#13;&#10;  chkPartPlace.Checked := bPartPlace;&#13;&#10;  edPartPlaceText.Text := sPartPlaceText;      &#13;&#10;&#13;&#10;  chkStorageDateNoPrint.Checked := bStorageDateNoPrint;&#13;&#10;    &#13;&#10;  chkGoodStoreNumKPS.Checked := bGoodStoreNumKPS;&#13;&#10;  chkShow.Checked := IniFile(sIniSection, 'ShowDialog', 'False', sIniFullPath) = 'True';&#13;&#10;&#13;&#10;&#13;&#10;  rbtnInfoFact.Checked := bUseFactInfo;&#13;&#10;  rbtnInfoDoc.Checked := not bUseFactInfo;&#13;&#10;    &#13;&#10;  rbtnAcceptDate.Checked := bAcceptDate;                                                               &#13;&#10;  rbtnBdDate.Checked :=  bBdDate;                                                                  &#13;&#10;  rbtnBegKeep.Checked := bBegKeep;&#13;&#10;  rbtnG542.Checked := bG542;&#13;&#10;    &#13;&#10;  chkUseConts.Checked := bUseConts;&#13;&#10;&#13;&#10;  chkPrintPlomb.Checked := bPrintPlomb;&#13;&#10;  chkPrintPlomb.Enabled := bUseConts;      &#13;&#10;    &#13;&#10;  chkPrintContType.Checked := bPrintContType;&#13;&#10;  chkPrintContType.Enabled := bUseConts;&#13;&#10;    &#13;&#10;  cbxOnlyWaybill.Checked := bPrintWaybill;&#13;&#10;&#13;&#10;  rbtnOnlyBaseList.Checked := bOnlyBaseList;                                                               &#13;&#10;  rbtnOnlyDopList.Checked := bOnlyDopList;                                                               &#13;&#10;  rbtnBaseDopList.Checked := bBaseDopList;&#13;&#10;&#13;&#10;&#13;&#10;&#13;&#10;  chkRecipient.Checked := bRecipient;&#13;&#10;  cbxPrintBill.Checked := bPrintBill;                            &#13;&#10;  cbxOnlyWayBill2.Enabled := cbxPrintBill.Checked;&#13;&#10;  if cbxPrintBill.Checked then                        &#13;&#10;     cbxOnlyWayBill2.Checked :=  bPrintWaybill2&#13;&#10;  else cbxOnlyWayBill2.Checked := False;                                   &#13;&#10;    &#13;&#10;end;&#13;&#10;&#13;&#10;procedure cbxPrintBillOnClick(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  cbxOnlyWayBill2.Enabled := cbxPrintBill.Checked;&#13;&#10;  cbxOnlyWayBill2.Checked :=  cbxPrintBill.Checked;  &#13;&#10;end;&#13;&#10;&#13;&#10;procedure chkItogOnClick(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  If chkItog.Checked then&#13;&#10;  begin&#13;&#10;    ChkG42.Enabled := True;&#13;&#10;    chkUSDG42.Enabled := chkG42.Checked;&#13;&#10;    chkEurG42.Enabled := chkG42.Checked;&#13;&#10;    chkRubG42.Enabled := chkG42.Checked;&#13;&#10;    chkClearG42.Enabled := chkG42.Checked;&#13;&#10;  end&#13;&#10;  else&#13;&#10;  begin&#13;&#10;    ChkG42.Enabled := False;&#13;&#10;    chkUSDG42.Enabled := False;&#13;&#10;    chkEurG42.Enabled := False;&#13;&#10;    chkRubG42.Enabled := False;&#13;&#10;    chkClearG42.Enabled := False;&#13;&#10;  end  &#13;&#10;end;&#13;&#10;&#13;&#10;procedure chkG42OnClick(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;    chkUSDG42.Enabled := chkG42.Checked;&#13;&#10;    chkEurG42.Enabled := chkG42.Checked;&#13;&#10;    chkRubG42.Enabled := chkG42.Checked;&#13;&#10;    chkClearG42.Enabled := chkG42.Checked;  &#13;&#10;end;  &#13;&#10;  &#13;&#10;procedure chkUseContsOnClick(Sender: TfrxComponent);&#13;&#10;begin                              &#13;&#10;  chkPrintContType.Enabled := chkUseConts.Checked;&#13;&#10;  chkPrintPlomb.Enabled := chkUseConts.Checked;      &#13;&#10;  chkPrintPlomb.Checked :=  chkUseConts.Checked;      &#13;&#10;end;  &#13;&#10;&#13;&#10;procedure chkBruttoOnClick(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;    chkNetto.Enabled := chkBrutto.Checked;&#13;&#10;    if chkNetto.Enabled = false then chkNetto.Checked := False;                                                                                                                               &#13;&#10;end;    &#13;&#10;&#13;&#10;procedure btnOKOnClick(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  WriteIniFile(sIniSection, 'ShowDialog', VarToStr(chkShow.Checked), sIniFullPath);                                   &#13;&#10;    &#13;&#10;  bBrutto := chkBrutto.Checked;&#13;&#10;  bNetto :=  chkNetto.Checked;                                                &#13;&#10;  bUnits := chkUnits.Checked;&#13;&#10;  bVolume := chkVolume.Checked;&#13;&#10;&#13;&#10;  bPartPlace := chkPartPlace.Checked;&#13;&#10;&#13;&#10;  sPartPlaceText := edPartPlaceText.Text;&#13;&#10;&#13;&#10;  bStorageDateNoPrint := chkStorageDateNoPrint.Checked;      &#13;&#10;    &#13;&#10;  bGoodStoreNumKPS := chkGoodStoreNumKPS.Checked;&#13;&#10;&#13;&#10;  bUseFactInfo := rbtnInfoFact.Checked;&#13;&#10;  bPrintWaybill := cbxOnlyWaybill.Checked;&#13;&#10;  bPrintWaybill2 := cbxOnlyWaybill2.Checked;&#13;&#10;  bPrintbill := cbxPrintbill.Checked;      &#13;&#10;&#13;&#10;  bUseConts := chkUseConts.Checked;      &#13;&#10;&#13;&#10;  bAcceptDate := rbtnAcceptDate.Checked;                                                                                      &#13;&#10;  bBdDate := rbtnBdDate.Checked;&#13;&#10;  bBegKeep := rbtnBegKeep.Checked;&#13;&#10;  bG542 := rbtnG542.Checked;      &#13;&#10;&#13;&#10;  bStorageDateNoPrint := chkStorageDateNoPrint.Checked;&#13;&#10;    &#13;&#10;  bPrintContType := chkPrintContType.Checked;&#13;&#10;  bPrintPlomb :=  chkPrintPlomb.Checked;&#13;&#10;&#13;&#10;  bPrintRegNum := chkRegNum.Checked;&#13;&#10;&#13;&#10;  bOnlyBaseList := rbtnOnlyBaseList.Checked;    &#13;&#10;  bOnlyDopList := rbtnOnlyDopList.Checked;&#13;&#10;  bBaseDopList := rbtnBaseDopList.Checked;      &#13;&#10;&#13;&#10;&#13;&#10;  bRecipient := chkRecipient.Checked;&#13;&#10;&#13;&#10;  // &#34;итоги&#34;&#13;&#10;  bPrintItog := chkItog.Checked;&#13;&#10;  bPrintG42 := chkG42.Checked;     &#13;&#10;  bPrintUSDG42 := chkUSDG42.Checked;&#13;&#10;  bPrintEurG42 := chkEurG42.Checked;&#13;&#10;  bPrintRubG42 := chkRubG42.Checked;      &#13;&#10;  bPrintClearG42 := chkClearG42.Checked;&#13;&#10;      &#13;&#10;        &#13;&#10;  WriteIniFile(sIniSection, 'PrintRegNum', VarToStr(bPrintRegNum), sIniFullPath);  &#13;&#10;  &#13;&#10;  WriteIniFile(sIniSection, 'PrintItog', VarToStr(bPrintItog), sIniFullPath);&#13;&#10;  WriteIniFile(sIniSection, 'SumG42', VarToStr(bPrintG42), sIniFullPath);&#13;&#10;  WriteIniFile(sIniSection, 'SumUSDG42', VarToStr(bPrintUSDG42), sIniFullPath);&#13;&#10;  WriteIniFile(sIniSection, 'SumEURG42', VarToStr(bPrintEURG42), sIniFullPath);&#13;&#10;  WriteIniFile(sIniSection, 'SumRubG42', VarToStr(bPrintRubG42), sIniFullPath);&#13;&#10;  WriteIniFile(sIniSection, 'SumClearG42', VarToStr(bPrintClearG42), sIniFullPath);      &#13;&#10;  &#13;&#10;  WriteIniFile(sIniSection, 'Brutto', VarToStr(bBrutto), sIniFullPath);&#13;&#10;  WriteIniFile(sIniSection, 'Netto', VarToStr(bNetto), sIniFullPath);      &#13;&#10;  WriteIniFile(sIniSection, 'Units', VarToStr(bUnits), sIniFullPath);&#13;&#10;  WriteIniFile(sIniSection, 'Volume', VarToStr(bVolume), sIniFullPath);    &#13;&#10;&#13;&#10;  WriteIniFile(sIniSection, 'GoodStoreNumKPS', VarToStr(bGoodStoreNumKPS), sIniFullPath);&#13;&#10;&#13;&#10;  WriteIniFile(sIniSection, 'UseFactInfo', VarToStr(bUseFactInfo), sIniFullPath);&#13;&#10;&#13;&#10;  WriteIniFile(sIniSection, 'UseConts', VarToStr(bUseConts), sIniFullPath);    &#13;&#10;&#13;&#10;  WriteIniFile(sIniSection, 'AcceptDate', VarToStr(bAcceptDate), sIniFullPath);    &#13;&#10;  WriteIniFile(sIniSection, 'BdDate', VarToStr(bBdDate), sIniFullPath);&#13;&#10;  WriteIniFile(sIniSection, 'BegKeep', VarToStr(bBegKeep), sIniFullPath);&#13;&#10;  WriteIniFile(sIniSection, 'G542', VarToStr(bG542), sIniFullPath);&#13;&#10;    &#13;&#10;  WriteIniFile(sIniSection, 'PartPlace', VarToStr(bPartPlace), sIniFullPath);&#13;&#10;&#13;&#10;  WriteIniFile(sIniSection, 'PartPlaceText', sPartPlaceText, sIniFullPath);&#13;&#10;&#13;&#10;  WriteIniFile(sIniSection, 'StorageDateNoPrint', VarToStr(bStorageDateNoPrint), sIniFullPath);     &#13;&#10;  WriteIniFile(sIniSection, 'PrintPlomb', VarToStr(bPrintPlomb), sIniFullPath);      &#13;&#10;  WriteIniFile(sIniSection, 'PrintContType', VarToStr(bPrintContType), sIniFullPath);&#13;&#10;  WriteIniFile(sIniSection, 'PrintWaybill', VarToStr(bPrintWaybill), sIniFullPath);&#13;&#10;  WriteIniFile(sIniSection, 'PrintWaybill2', VarToStr(bPrintWaybill2), sIniFullPath);      &#13;&#10;  WriteIniFile(sIniSection, 'Printbill', VarToStr(bPrintbill), sIniFullPath);&#13;&#10;    &#13;&#10;  WriteIniFile(sIniSection, 'PrintOnlyBaseList', VarToStr(bOnlyBaseList), sIniFullPath);&#13;&#10;  WriteIniFile(sIniSection, 'PrintOnlyDopList', VarToStr(bOnlyDopList), sIniFullPath);&#13;&#10;  WriteIniFile(sIniSection, 'PrintBaseDopList', VarToStr(bBaseDopList), sIniFullPath);&#13;&#10;    &#13;&#10;      &#13;&#10;  &#13;&#10;  WriteIniFile(sIniSection, 'Recipient', VarToStr(bRecipient), sIniFullPath);&#13;&#10;&#13;&#10;end;  &#13;&#10;  &#13;&#10;  &#13;&#10;&#13;&#10;procedure Page2OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  If bOnlyBaseList then&#13;&#10;    begin&#13;&#10;      Child1.Visible := False;                                                                                              &#13;&#10;    end;&#13;&#10;    &#13;&#10;  If bOnlyDopList then&#13;&#10;    begin&#13;&#10;      Page3.Visible := False;&#13;&#10;      PageHeader1.Visible := False;&#13;&#10;      fldTransDoc.Visible := False;                                                                                              &#13;&#10;    end;&#13;&#10;    &#13;&#10;  nPP := 0;  // нумерация товаров                                                                    &#13;&#10;    &#13;&#10;  sTypeSVH := '';&#13;&#10;  sNameSVH := '';&#13;&#10;  sTypeDoc := '';&#13;&#10;  sDateDoc := '';&#13;&#10;  sNumDoc  := '';          &#13;&#10;&#13;&#10;  if COMPUTE('FINDKEY(&#34;STORES_2&#34;, [&#34;' + IntToStr(&#60;KRD_MAIN.&#34;PLACEID&#34;&#62;) + '&#34;])') &#60;&#62; '0' then&#13;&#10;    sTypeSVH := &#60;STORES_2.&#34;STORE_TYPE&#34;&#62;&#13;&#10;  else sTypeSVH := '';&#13;&#10;&#13;&#10;  if (&#60;KRD_MAIN.&#34;G142&#34;&#62; &#60;&#62; Null) and (&#60;KRD_MAIN.&#34;G142&#34;&#62; &#60;&#62; '') then begin&#13;&#10;    sNameSVH := &#60;KRD_MAIN.&#34;G142&#34;&#62;;&#13;&#10;    sDateDoc := &#60;KRD_MAIN.&#34;G145&#34;&#62;;&#13;&#10;    sNumDoc := &#60;KRD_MAIN.&#34;G144&#34;&#62;;&#13;&#10;  end&#13;&#10;  else if COMPUTE('FINDKEY(&#34;STORES_2&#34;, [&#34;' + IntToStr(&#60;KRD_MAIN.&#34;PLACEID&#34;&#62;) + '&#34;])') &#60;&#62; '0' then begin&#13;&#10;    sNameSVH := &#60;STORES_2.&#34;Name&#34;&#62;;&#13;&#10;    sDateDoc := DateToStr(&#60;STORES_2.&#34;LICENCEDATE&#34;&#62;);&#13;&#10;    sNumDoc  := &#60;STORES_2.&#34;LICENCENO&#34;&#62;&#13;&#10;  end&#13;&#10;  else begin&#13;&#10;    sNameSVH := '';&#13;&#10;    sDateDoc := '';&#13;&#10;    sNumDoc  := '';&#13;&#10;  end;&#13;&#10;&#13;&#10;  case &#60;STORES_2.&#34;STORE_TYPE&#34;&#62; of&#13;&#10;    '', 'СВХ', Null: begin&#13;&#10;      sTypeSVH := 'СВХ';&#13;&#10;      sTypeDoc := 'Свидетельство о включении в реестр владельцев СВХ от';&#13;&#10;    end;&#13;&#10;    'СП': begin&#13;&#10;      sTypeSVH := 'Склад получателя товаров';&#13;&#10;      sTypeDoc := 'Разрешение на временное хранение на складе получателя товаров от';&#13;&#10;    end;&#13;&#10;    'ПЗТК': begin&#13;&#10;      sTypeSVH := 'ПЗТК';&#13;&#10;      sTypeDoc := 'Приказ(разрешение) таможни от';&#13;&#10;    end;&#13;&#10;    'ВЗТК': begin&#13;&#10;      sTypeSVH := 'ВЗТК ';&#13;&#10;      sTypeDoc := 'Приказ(разрешение) таможни от';            &#13;&#10;    end;                &#13;&#10;    'ЗТК': begin&#13;&#10;      sTypeSVH := 'ЗТК';&#13;&#10;      sTypeDoc := 'Приказ(разрешение) таможни от';&#13;&#10;    end;&#13;&#10;    'СВХУТ': begin&#13;&#10;      sTypeSVH := 'СВХ';&#13;&#10;      sTypeDoc := 'Свидетельство о включении в реестр владельцев СВХ от';&#13;&#10;    end;&#13;&#10;  end;            &#13;&#10;    &#13;&#10;end;&#13;&#10;  &#13;&#10;procedure PageHeader1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  If bBaseDopList then&#13;&#10;    PageHeader1.Visible := (&#60;PAGE#&#62; = 1)                                                                                                 &#13;&#10;end;  &#13;&#10;&#13;&#10;procedure fldtransDocOnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin                                                  &#13;&#10;    sBackSheet := &#34;&#34;;&#13;&#10;      &#13;&#10;    COMPUTE('OPENQUERY(&#34;QRYMAIN&#34;, &#34;STS_DB&#34;, &#34;SELECT PLACEID, ID  ' + &#13;&#10;                                               'FROM KRD_MAIN ' + &#13;&#10;                                               'WHERE PLACEID = ' + IntToStr(vPlaceId) +&#13;&#10;                                              ' AND MAIN_ID = ' + IntToStr(vMainId) + '&#34;)');                                                   &#13;&#10;&#13;&#10;    While StrToInt(COMPUTE('EOF(&#34;QRYMAIN&#34;)')) = 0  do begin&#13;&#10;      &#13;&#10;      COMPUTE('OPENQUERY(&#34;PAPER&#34;, &#34;STS_DB&#34;, &#34; SELECT KP.PAPERNO, KP.PAPERNAME, KP.PAPERDATE FROM KR_PAPER KP '+&#13;&#10;                                           IIF(bPrintWaybill, ', PAPERS P ', '')+&#13;&#10;                                           ' WHERE '+&#13;&#10;                                           IIF(bPrintWaybill, ' (KP.PAPERNAME = P.PAPERNAME) AND ', '') +                                                           &#13;&#10;                                           ' (KP.PLACEID= ' +IntToStr(COMPUTE('FIELDVALUE(&#34;QRYMAIN&#34;, &#34;PLACEID&#34;)'))+                       &#13;&#10;                                           ' ) AND (KP.ID= ' +IntToStr(COMPUTE('FIELDVALUE(&#34;QRYMAIN&#34;, &#34;ID&#34;)'))+ ')'+                                                                                                 &#13;&#10;                                           IIF(bPrintWaybill,' AND (P.PAPER_IS_WAYBILL = 1) ', '')+&#13;&#10;                                           '&#34;)');&#13;&#10;      COMPUTE('FIRST(&#34;PAPER&#34;)');&#13;&#10;      WHILE COMPUTE('EOF(&#34;Paper&#34;)') = 0 do&#13;&#10;      begin                  &#13;&#10;        if LENGTH(sBackSheet) &#62; 0 then sBackSheet := sBackSheet + &#34;, &#34;;        &#13;&#10;        sBackSheet := sBackSheet + COMPUTE('FIELDVALUE(&#34;PAPER&#34;, &#34;PAPERNAME&#34;)') + &#13;&#10;                                 ' №' + COMPUTE('FIELDVALUE(&#34;PAPER&#34;, &#34;PAPERNO&#34;)') + &#13;&#10;                                 IIF(STR(COMPUTE('FIELDVALUE(&#34;PAPER&#34;, &#34;PAPERDATE&#34;)')) &#60;&#62; '0:00:00',' от ' + STR(COMPUTE('FIELDVALUE(&#34;PAPER&#34;, &#34;PAPERDATE&#34;)')), '');&#13;&#10;        //COMPUTE('UNIONVALUES(&#34;PAPER&#34;, [&#34;PAPERNAME&#34;, &#34;PAPERNO&#34;, &#34;PAPERDATE&#34;], &#34;, &#34;, &#34; № &#34;)');&#13;&#10;        COMPUTE('NEXT(&#34;PAPER&#34;)');                                     &#13;&#10;      end;&#13;&#10;      COMPUTE('CLOSEDATASET(&#34;PAPER&#34;)');&#13;&#10;&#13;&#10;      COMPUTE('NEXT(&#34;QRYMAIN&#34;)');&#13;&#10;    end;&#13;&#10;    &#13;&#10;    if (LENGTH(sBackSheet)) &#62; 105 then begin&#13;&#10;      sCurSheet := &#34;см. на обороте&#34;;&#13;&#10;      Page3.Visible := True;&#13;&#10;    end&#13;&#10;    else begin&#13;&#10;      sCurSheet := sBackSheet;&#13;&#10;      sBackSheet := '';&#13;&#10;    end;&#13;&#10;                           &#13;&#10;end;  &#13;&#10;&#13;&#10;  &#13;&#10;procedure PrintPlomb (s : integer);      &#13;&#10;begin&#13;&#10;sConstStore :='';  &#13;&#10;  COMPUTE('SETFILTER(&#34;KRD_COMM_PAPERS_2&#34;, &#34;PLACEID = ' + IntToStr(vPlaceId) +&#13;&#10;                                          ' AND ID = ' + IntToStr(vId) +&#13;&#10;                                         ' AND G32 = ' + IntToStr(vG32) +&#13;&#10;                                    ' AND DOC_TYPE = 11&#34;)');  &#13;&#10;  if StrToInt(COMPUTE('RECORDCOUNT(&#34;KRD_COMM_PAPERS_2&#34;)')) &#62; 0 then begin&#13;&#10;    COMPUTE('FIRST(&#34;KRD_COMM_PAPERS_2&#34;)');&#13;&#10;    While StrToInt(COMPUTE('EOF(&#34;KRD_COMM_PAPERS_2&#34;)')) = 0 do begin&#13;&#10;      COMPUTE('SETRANGE(&#34;KRD_CONT_2&#34;, [&#34;' + IntToStr(&#60;KRD_COMM_PAPERS_2.&#34;PLACEID&#34;&#62;) + '&#34;, &#34;' +&#13;&#10;                                            IntToStr(&#60;KRD_COMM_PAPERS_2.&#34;ID&#34;&#62;) + '&#34;, &#34;' +&#13;&#10;                                            IntToStr(&#60;KRD_COMM_PAPERS_2.&#34;DOC_COUNTER&#34;&#62;) + '&#34;])');&#13;&#10;      if Length(sConstStore) &#62; 0 then sConstStore := sConstStore + ',';&#13;&#10;      if (COMPUTE('FIELDVALUE(&#34;KRD_CONT_2&#34;, &#34;PLOMB&#34;)') &#60;&#62; '') and chkPrintPlomb.Checked then begin&#13;&#10;        if chkPrintContType.Checked then                                                                                                                   &#13;&#10;          sConstStore := sConstStore + COMPUTE('FIELDVALUE(&#34;KRD_CONT_2&#34;, &#34;CONTTYPE&#34;)') + ' ' + COMPUTE('FIELDVALUE(&#34;KRD_CONT_2&#34;, &#34;CONTNO&#34;)') + ' : '+ COMPUTE('FIELDVALUE(&#34;KRD_CONT_2&#34;, &#34;PLOMB&#34;)')&#13;&#10;        else&#13;&#10;          sConstStore := sConstStore + COMPUTE('FIELDVALUE(&#34;KRD_CONT_2&#34;, &#34;CONTNO&#34;)') + ' : '+ COMPUTE('FIELDVALUE(&#34;KRD_CONT_2&#34;, &#34;PLOMB&#34;)');                      &#13;&#10;      end                                 &#13;&#10;      else begin&#13;&#10;        if chkPrintContType.Checked then              &#13;&#10;          sConstStore := sConstStore + COMPUTE('FIELDVALUE(&#34;KRD_CONT_2&#34;, &#34;CONTTYPE&#34;)') + ' ' + COMPUTE('FIELDVALUE(&#34;KRD_CONT_2&#34;, &#34;CONTNO&#34;)')&#13;&#10;        else&#13;&#10;          sConstStore := sConstStore + COMPUTE('FIELDVALUE(&#34;KRD_CONT_2&#34;, &#34;CONTNO&#34;)');                                               &#13;&#10;      end;&#13;&#10;      COMPUTE('CANCELRANGE(&#34;KRD_CONT_2&#34;)');&#13;&#10;      COMPUTE('NEXT(&#34;KRD_COMM_PAPERS_2&#34;)');&#13;&#10;    end;&#13;&#10;  end&#13;&#10;  else begin&#13;&#10;    COMPUTE('SETRANGE(&#34;KRD_CONT_2&#34;, [&#34;' + IntToStr(vPlaceId) + '&#34;, &#34;' +&#13;&#10;                                          IntToStr(vId) + '&#34;])');&#13;&#10;    if chkPrintPlomb.Checked then begin                                                                               &#13;&#10;      if chkPrintContType.Checked then                                                  &#13;&#10;      sConstStore := COMPUTE('UNIONVALUES(&#34;KRD_CONT_2&#34;, [&#34;CONTTYPE&#34;, &#34;CO