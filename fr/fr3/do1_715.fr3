ramName := ExtractWord(i, sSetsCommon, sPDelim);&#13;&#10;    If WordCount(sParamName, sPDelim2) &#62; 1 then begin //Если переименовываем параметр&#13;&#10;      sParamNameOld := ExtractWord(1, sParamName, sPDelim2);&#13;&#10;      sParamNameNew := ExtractWord(2, sParamName, sPDelim2);&#13;&#10;    end&#13;&#10;    else begin                                        //Если НЕ переименовываем параметр&#13;&#10;      sParamNameOld := ExtractWord(1, sParamName, sPDelim2);&#13;&#10;      sParamNameNew := ExtractWord(1, sParamName, sPDelim2);&#13;&#10;    end;&#13;&#10;    sFromOldIni := IniFile('Print', sParamNameOld, '');&#13;&#10;    sFromNewIni := IniFile(sIniSectionCommon, sParamNameNew, '', sIniFullPath);&#13;&#10;    if Trim(sFromNewIni) = '' then&#13;&#10;      WriteIniFile(sIniSectionCommon, sParamNameNew, sFromOldIni, sIniFullPath);&#13;&#10;  end;&#13;&#10;end;  &#13;&#10;&#13;&#10;&#13;&#10;begin&#13;&#10;  MoveSettingsToNewIni();&#13;&#10;&#13;&#10;  iSumG311:= Null;&#13;&#10;  iSumG315C:= Null;&#13;&#10;  iSumG315A:= Null;&#13;&#10;  iSumG35:= Null;&#13;&#10;  iSumG38 := Null;&#13;&#10;  iSumG42 := Null;                 &#13;&#10;    &#13;&#10;&#13;&#10;&#13;&#10;&#13;&#10;  bPrintRegNum := IniFile(sIniSection, 'PrintRegNum', 'False', sIniFullPath) = 'True';&#13;&#10;  &#13;&#10;  bPrintItog := IniFile(sIniSection, 'PrintItog', 'False', sIniFullPath) = 'True';&#13;&#10;  bPrintG42 := IniFile(sIniSection, 'SumG42', 'False', sIniFullPath) = 'True';      &#13;&#10;  bPrintUSDG42 := IniFile(sIniSection, 'SumUSDG42', 'False', sIniFullPath) = 'True';  &#13;&#10;  bPrintEURG42 := IniFile(sIniSection, 'SumEURG42', 'False', sIniFullPath) = 'True';  &#13;&#10;  bPrintRUBG42 := IniFile(sIniSection, 'SumRubG42', 'False', sIniFullPath) = 'True';  &#13;&#10;  bPrintClearG42 := IniFile(sIniSection, 'SumClearG42', 'True', sIniFullPath) = 'True';  &#13;&#10;  &#13;&#10;  //Что печатать в графе 7&#13;&#10;  bBrutto := IniFile(sIniSection, 'Brutto', 'False', sIniFullPath) = 'T