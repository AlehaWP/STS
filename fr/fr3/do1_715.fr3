LL = 1) ', '')+&#13;&#10;                                             '&#34;)');                                                   &#13;&#10;      COMPUTE('FIRST(&#34;PAPER&#34;)');  &#13;&#10;      IF COMPUTE('RECORDCOUNT(&#34;PAPER&#34;)') &#60;&#62; 0 then                                                         &#13;&#10;         sDocNames := COMPUTE('UNIONVALUES(&#34;PAPER&#34;, [&#34;PAPERNAME&#34;, &#34;PAPERNO&#34;], &#34;, &#34;, &#34; № &#34;)')&#13;&#10;        else  begin          &#13;&#10;          COMPUTE('CLOSEDATASET(&#34;PAPER&#34;)');                                                     &#13;&#10;          COMPUTE('OPENQUERY(&#34;PAPER&#34;, &#34;STS_DB&#34;, &#34; SELECT KP.PAPERNO, KP.PAPERNAME, KP.PAPERDATE FROM KR_PAPER KP '+&#13;&#10;                                               IIF(bPrintWaybill2, ', PAPERS P ', '')+                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                            