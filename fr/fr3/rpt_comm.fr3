<TfrxReport Name="" DotMatrixReport="False" EngineOptions.ConvertNulls="False" EngineOptions.DoublePass="True" IniFile="\Software\Fast Reports" PreviewOptions.Buttons="2047" PreviewOptions.Zoom="1" PrintOptions.Printer="По умолчанию" ReportOptions.CreateDate="38267.8107595486" ReportOptions.Description.Text="" ReportOptions.Name="Отчет о товарах и тр.средствах (пр.290)" ReportOptions.LastChange="38520.3892034143" ScriptLanguage="PascalScript" ScriptText.Text="var&#13;&#10;  AistCompatible, CustomDocsOnly: Boolean;&#13;&#10;  NumLines, WithPlace : Integer;&#13;&#10;  MAINID, vFilterCrit, vPlaceID: string;&#13;&#10;  PrintFirstAll: Boolean;&#13;&#10;  dtPeriodEnd, dtPeriodStart, PeriodStart: TDateTime;&#13;&#10;  vSumRelG35, vG35Rest: Double;&#13;&#10;  nuv, ReportPeriod, vBegKeep, vG311, vG35, vG46, vNumLine: string;&#13;&#10;  sStoreNum, TransDocNames, TransDocRegNames, vBOXNO, iWithPlace: string;&#13;&#10;  TransNums, vOutDate, vGTDNo, vOutG311, vOutG35, vOutRemark: string;&#13;&#10;&#13;&#10;procedure Page1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  fldStoreLicNum.Text := '';&#13;&#10;  fldStoreNum.Text := '';&#13;&#10;  lblStoreName.Caption := '';&#13;&#10;  fldLicExt.Text := '';&#13;&#10;  dtOnDate.Enabled := False;&#13;&#10;  dtFrom.Enabled := True;&#13;&#10;  dtTo.Enabled := True;&#13;&#10;  cbxNotInclude.Enabled := True;&#13;&#10;  lblPer.Enabled := True;&#13;&#10;  rbtnDate.Checked := True;&#13;&#10;  cbtnOk.Enabled := False;&#13;&#10;&#13;&#10;  ReportPeriod:=COMPUTE('INIFILE(&#34;Print&#34;,&#34;PeriodStart&#34;,&#34;&#34;)');&#13;&#10;  dtFrom.Date := STRTODATEDEF(ReportPeriod, DATE);&#13;&#10;  ReportPeriod := COMPUTE('INIFILE(&#34;Print&#34;,&#34;PeriodEnd&#34;,&#34;&#34;)');&#13;&#10;  dtTo.Date := STRTODATEDEF(ReportPeriod, DATE);&#13;&#10;  ReportPeriod := COMPUTE('INIFILE(&#34;Print&#34;,&#34;OnDate&#34;,&#34;&#34;)');&#13;&#10;  dtOnDate.Date := STRTODATEDEF(ReportPeriod, DATE);&#13;&#10;&#13;&#10;  if Compute('INIFILE(&#34;Print&#34;,&#34;CalcG46onReportDate&#34;,&#34;1&#34;)') = '1' then begin&#13;&#10;      rbtnG46date.Checked := True;&#13;&#10;      rbtnG46bd_date.Checked := False&#13;&#10;    end&#13;&#10;  else begin&#13;&#10;   rbtnG46date.Checked := False;&#13;&#10;   rbtnG46bd_date.Checked := True&#13;&#10;   end;&#13;&#10;&#13;&#10;  If Compute('INIFILE(&#34;Print&#34;,&#34;ReportForPeriod&#34;,&#34;1&#34;)') = '1' then&#13;&#10;     rbtnPeriod.Checked := true&#13;&#10;  else rbtnPeriod.Checked := False&#13;&#10;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure btnCallRefOnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  COMPUTE(' globalvar (&#34;strStoreNumLic&#34;,String)');&#13;&#10;  COMPUTE(' globalvar (&#34;strStoreNum&#34;,String)');&#13;&#10;  COMPUTE(' globalvar (&#34;strStoreName&#34;,String)');&#13;&#10;  COMPUTE(' globalvar (&#34;strLicExt&#34;,String)');&#13;&#10;  COMPUTE(' globalvar (&#34;sPlaceId&#34;,Integer)');&#13;&#10;  COMPUTE('OPENQUERY (&#34;Stors&#34;, &#34;STS_DB&#34;, &#34;Select * from STORES&#34;)');&#13;&#10;  COMPUTE('SelectValues(&#34;Список складов&#34;, &#34;Stors&#34;,[[&#34;LICENCENO&#34;,   &#34;Номер лицензии&#34;, 15],[&#34;LICENCENO_EXT&#34;,   &#34; &#34;, 1],[&#34;STORE_NO&#34;,   &#34;Номер склада&#34;, 15],[&#34;NAME&#34;,   &#34;Название склада&#34;, 30],[&#34;PlaceId&#34;,   &#34;&#34;, 10]],[[&#34;LICENCENO&#34;, &#34;strStoreNumLic&#34;],[&#34;STORE_NO&#34;, &#34;strStoreNum&#34;],[&#34;NAME&#34;, &#34;strStoreName&#34;],[&#34;LICENCENO_EXT&#34;, &#34;strLicExt&#34;],[&#34;PlaceId&#34;, &#34;sPlaceId&#34;],],&#34;&#34;, &#34;STS_DB&#34;)');&#13;&#10;&#13;&#10;  fldStoreLicNum.Text := COMPUTE('Value(&#34;strStoreNumLic&#34;)') ;      //   номер лицензии&#13;&#10;  fldStoreNum.Text := '№ склада: ' + COMPUTE('value(&#34;strStoreNum&#34;)') ; //   номер склада&#13;&#10;  lblStoreName.Caption := COMPUTE('Value(&#34;strStoreName&#34;)') ;     //   название склада&#13;&#10;  fldLicExt.Text := COMPUTE('Value(&#34;strLIcExt&#34;)');     //   Признак перерегистрации&#13;&#10;  vPlaceId := COMPUTE('Value(&#34;sPlaceId&#34;)');&#13;&#10;  cbtnOk.Enabled := True&#13;&#10;end;&#13;&#10;&#13;&#10;procedure rbtnDateOnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  dtOnDate.Enabled := True;&#13;&#10;  dtFrom.Enabled := False;&#13;&#10;  dtTo.Enabled := False;&#13;&#10;  cbxNotInclude.Enabled := False;&#13;&#10;  lblPer.Enabled := False&#13;&#10;end;&#13;&#10;&#13;&#10;procedure rbtnPeriodOnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  dtOnDate.Enabled := False;&#13;&#10;  dtFrom.Enabled := True;&#13;&#10;  dtTo.Enabled := True;&#13;&#10;  cbxNotInclude.Enabled := True;&#13;&#10;  lblPer.Enabled := True&#13;&#10;end;&#13;&#10;&#13;&#10;procedure cbtnOkOnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;     //Взяли из ini-шника &#34;Только таможенные документы учета&#34;&#13;&#10;     CustomDocsOnly := (COMPUTE('INIFILE(&#34;RegBook&#34;, &#34;CustomsDocsOnly&#34;, 1)') = '1') or&#13;&#10;                       (COMPUTE('INIFILE(&#34;Database&#34;, &#34;StoreDocsBlock&#34;, 1)') = '0');&#13;&#10;&#13;&#10;     //Совместимость с аистом&#13;&#10;     AistCompatible := COMPUTE('INIFILE(&#34;Docs&#34;, &#34;AistCompatible&#34;, 0)') = '1';&#13;&#10;     //Надо ли включать только &#34;принятие на хранение&#34;&#13;&#10;     if cbxInclude.Checked = True then&#13;&#10;         WithPlace := 0 else WithPlace := 2;&#13;&#10;&#13;&#10;     //Далее см. Params к таблицам&#13;&#10;     //На дату или за период?&#13;&#10;     if rbtnDate.Checked = True then&#13;&#10;       begin&#13;&#10;         dtPeriodEnd := Trunc(dtOnDate.Date) + STRTOTIME('23:59:59');&#13;&#10;         dtPeriodStart := STRTODATE('01.01.1900') + STRTOTIME('00:00:01');&#13;&#10;         //Запомним начало периода отчета&#13;&#10;         PeriodStart := Trunc(dtOnDate.Date) + STRTOTIME('23:59:59')&#13;&#10;       end&#13;&#10;     else&#13;&#10;       begin&#13;&#10;         dtPeriodEnd := dtTo.Date + STRTOTIME('23:59:59');&#13;&#10;         //Проверим галку &#34;Не включать ранее периода&#34;&#13;&#10;         if cbxNotInclude.Checked = True then&#13;&#10;           dtPeriodStart := Trunc(dtFrom.Date) + STRTOTIME('00:00:00')&#13;&#10;         else&#13;&#10;           dtPeriodStart := STRTODATE('01.01.1900') + STRTOTIME('00:00:01');&#13;&#10;         //Запомним начало периода отчета&#13;&#10;         PeriodStart := Trunc(dtFrom.Date) + STRTOTIME('00:00:00');&#13;&#10;       end;&#13;&#10;&#13;&#10;     //Объявляем переменные&#13;&#10;     NumLines := 0;&#13;&#10;     MAINID := '';&#13;&#10;     PrintFirstAll := True;&#13;&#10;     vFilterCrit := &#34;(PLACEID = &#34; + vPlaceID + &#34;) &#34; +&#13;&#10;                    &#34; AND (BEG_KEEP &#60;= '&#34; + STR(dtPeriodEnd) + &#34;')&#34; +&#13;&#10;                    &#34; AND (BEG_KEEP &#62;= '&#34; + FORMATDATETIME(&#34;DD/MM/YYYY 00:01:00&#34;, dtPeriodStart) + &#34;')&#34; +&#13;&#10;                    &#34; AND (NOTICE_RW_ONLY=0)&#34;;&#13;&#10;     COMPUTE('SETRANGE(&#34;STORES_2&#34;, [&#34;' + STR(vPlaceId) + '&#34;])');&#13;&#10;&#13;&#10;     // сохраняем настройки&#13;&#10;     Compute('WRITEINIFILE(&#34;Print&#34;,&#34;OnDate&#34;,&#34;'+DATETOSTR(dtOnDate.Date)+'&#34;)');&#13;&#10;     Compute('WRITEINIFILE(&#34;Print&#34;,&#34;PeriodStart&#34;,&#34;'+DATETOSTR(dtFrom.Date)+'&#34;)');&#13;&#10;     Compute('WRITEINIFILE(&#34;Print&#34;,&#34;PeriodEnd&#34;,&#34;'+DateToStr(dtTo.Date)+'&#34;)');&#13;&#10;     if rbtnG46date.Checked = True then&#13;&#10;        Compute('WRITEINIFILE(&#34;Print&#34;,&#34;CalcG46onReportDate&#34;,&#34;1&#34;)')&#13;&#10;     else Compute('WRITEINIFILE(&#34;Print&#34;,&#34;CalcG46onReportDate&#34;,&#34;0&#34;)');&#13;&#10;     If rbtnPeriod.Checked then&#13;&#10;       Compute('WRITEINIFILE(&#34;Print&#34;,&#34;ReportForPeriod&#34;,&#34;1&#34;)')&#13;&#10;     else&#13;&#10;       Compute('WRITEINIFILE(&#34;Print&#34;,&#34;ReportForPeriod&#34;,&#34;0&#34;)');&#13;&#10;end;&#13;&#10;&#13;&#10;procedure ReportSummary1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  COMPUTE('CANCELRANGE(&#34;STORES_2&#34;)');&#13;&#10;  COMPUTE('CANCELRANGE(&#34;KRD_COMM_2&#34;)');&#13;&#10;  COMPUTE('SETFILTER(&#34;KRD_MAIN_2&#34;, &#34;&#34;)');&#13;&#10;  COMPUTE('CANCELRANGE(&#34;REL_MAIN_2&#34;)');&#13;&#10;  COMPUTE('CANCELRANGE(&#34;REL_COMM_2&#34;)');&#13;&#10;  if not CustomDocsOnly then begin&#13;&#10;    COMPUTE('CANCELRANGE(&#34;OUT_MAIN_2&#34;)');&#13;&#10;    COMPUTE('CANCELRANGE(&#34;OUT_COMM_2&#34;)');&#13;&#10;  end&#13;&#10;end;&#13;&#10;&#13;&#10;procedure MasterData2OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  with MasterData2, Engine do&#13;&#10;  begin&#13;&#10;  COMPUTE('CANCELRANGE(&#34;KRD_COMM_2&#34;)');&#13;&#10;  COMPUTE('SETRANGE(&#34;KRD_COMM_2&#34;, [&#34;' + STR(&#60;KRD_MAIN_2.&#34;PLACEID&#34;&#62;) + '&#34;, &#34;' + STR(&#60;KRD_MAIN_2.&#34;ID&#34;&#62;) + '&#34;])')&#13;&#10;  end&#13;&#10;end;&#13;&#10;&#13;&#10;procedure DetailData1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  if not CustomDocsOnly then&#13;&#10;    COMPUTE('SETRANGE(&#34;IN_COMM_2&#34;, [&#34;' + VARTOSTR(vPlaceId) + '&#34;, &#34;' + VARTOSTR(&#60;KRD_COMM_2.&#34;ID&#34;&#62;) + '&#34;, &#34;' + VARTOSTR(&#60;KRD_COMM_2.&#34;G32&#34;&#62;) + '&#34;])');&#13;&#10;&#13;&#10;  DetailData1.Visible := True;&#13;&#10;&#13;&#10;  if Engine.FINALPASS then begin&#13;&#10;    if MAINID &#60;&#62; VARTOSTR(&#60;KRD_MAIN_2.&#34;ID&#34;&#62;) then&#13;&#10;      PrintFirstAll := True&#13;&#10;    else&#13;&#10;      PrintFirstAll := False;&#13;&#10;    MAINID := VARTOSTR(&#60;KRD_MAIN_2.&#34;ID&#34;&#62;);&#13;&#10;  end;&#13;&#10;&#13;&#10;  //Здесь мы должны окончательно решить - показывать или нет строку&#13;&#10;  //Посчитаем остатки&#13;&#10;  //Некоторые переменные&#13;&#10;  vSumRelG35 := 0;&#13;&#10;  vG35Rest := 0;&#13;&#10;&#13;&#10;  if CustomDocsOnly then begin&#13;&#10;      //Сначала снимем предыдущий фильтр&#13;&#10;      if COMPUTE('FINDKEY(&#34;REL_COMM_2&#34;, [&#34;' + VARTOSTR(&#60;KRD_MAIN_2.&#34;PLACEID&#34;&#62;) + '&#34;, &#34;' + VARTOSTR(&#60;KRD_MAIN_2.&#34;ID&#34;&#62;) + '&#34;, &#34;' + VARTOSTR(&#60;KRD_COMM_2.&#34;G32&#34;&#62;) + '&#34;])') then begin&#13;&#10;          COMPUTE('CANCELRANGE(&#34;REL_MAIN_2&#34;)');&#13;&#10;          COMPUTE('CANCELRANGE(&#34;REL_COMM_2&#34;)');&#13;&#10;          //Поставим новый фильтр для текущего товара и документа&#13;&#10;          COMPUTE('SETRANGE(&#34;REL_MAIN_2&#34;, [&#34;' + STR(vPlaceId) + '&#34;, &#34;' + STR(&#60;KRD_MAIN_2.&#34;ID&#34;&#62;) + '&#34;])');&#13;&#10;          COMPUTE('SETRANGE(&#34;REL_COMM_2&#34;, [&#34;' + STR(vPlaceId) + '&#34;, &#34;' + STR(&#60;KRD_MAIN_2.&#34;ID&#34;&#62;) + '&#34;, &#34;' + STR(&#60;KRD_COMM_2.&#34;G32&#34;&#62;) + '&#34;])');&#13;&#10;          //Пройдем цикл по выпускам&#13;&#10;          //Товары, чтобы посмотреть вес&#13;&#10;          //======================================&#13;&#10;          REL_COMM_2.First;&#13;&#10;          while not REL_COMM_2.Eof do begin&#13;&#10;              //Выпуски, чтобы посмотреть даты&#13;&#10;              //-------------------------------&#13;&#10;              REL_MAIN_2.First;&#13;&#10;              while not REL_MAIN_2.Eof do begin&#13;&#10;                if (&#60;REL_COMM_2.&#34;COUNTER&#34;&#62; = &#60;REL_MAIN_2.&#34;COUNTER&#34;&#62;) then                  //Нам нужен выпуск к конкретному товару&#13;&#10;                  if (&#60;REL_MAIN_2.&#34;RELEASE_DATE&#34;&#62; &#60; PeriodStart) then                      //Все, что полностью выпущено раньше начала периода, нас не интересует&#13;&#10;                    if &#60;REL_COMM_2.&#34;RELEASE_G35&#34;&#62; &#60;&#62; Null then                             //Вес в выпуске д.б. заполнен&#13;&#10;                      vSumRelG35 := vSumRelG35 + &#60;REL_COMM_2.&#34;RELEASE_G35&#34;&#62;;&#13;&#10;                REL_MAIN_2.Next&#13;&#10;              end;&#13;&#10;              //-------------------------------&#13;&#10;              REL_COMM_2.Next&#13;&#10;            end;&#13;&#10;          //======================================&#13;&#10;        end;&#13;&#10;      //Итак остаток веса&#13;&#10;      if (&#60;KRD_COMM_2.&#34;FACT_G35&#34;&#62; &#60;&#62; Null) and (&#60;KRD_COMM_2.&#34;FACT_G35&#34;&#62; &#60;&#62; 0) then&#13;&#10;        begin&#13;&#10;          vG35Rest := &#60;KRD_COMM_2.&#34;FACT_G35&#34;&#62; - vSumRelG35;&#13;&#10;          if (((vG35Rest &#62; 0)&#13;&#10;                AND (&#60;KRD_COMM_2.&#34;FACT_G35&#34;&#62; &#60;&#62; NULL))&#13;&#10;              OR (&#60;KRD_COMM_2.&#34;FACT_G35&#34;&#62; = NULL)) then&#13;&#10;              DetailData1.Visible := True else DetailData1.Visible := False;&#13;&#10;        end&#13;&#10;      else&#13;&#10;        begin&#13;&#10;          vG35Rest := &#60;KRD_COMM_2.&#34;G35&#34;&#62; - vSumRelG35;&#13;&#10;          if (((vG35Rest &#62; 0)&#13;&#10;                AND (&#60;KRD_COMM_2.&#34;G35&#34;&#62; &#60;&#62; NULL))&#13;&#10;              OR (&#60;KRD_COMM_2.&#34;G35&#34;&#62; = NULL)) then&#13;&#10;              DetailData1.Visible := True else DetailData1.Visible := False;&#13;&#10;        end;&#13;&#10;&#13;&#10;      //Сначала снимем предыдущий фильтр&#13;&#10;      COMPUTE('CANCELRANGE(&#34;REL_MAIN_2&#34;)');&#13;&#10;      COMPUTE('CANCELRANGE(&#34;REL_COMM_2&#34;)');&#13;&#10;      //Поставим новый фильтр для текущего товара и документа&#13;&#10;      COMPUTE('SETRANGE(&#34;REL_MAIN_2&#34;, [&#34;' + STR(vPlaceId) + '&#34;, &#34;' + STR(&#60;KRD_MAIN_2.&#34;ID&#34;&#62;) + '&#34;])');&#13;&#10;      COMPUTE('SETRANGE(&#34;REL_COMM_2&#34;, [&#34;' + STR(vPlaceId) + '&#34;, &#34;' + STR(&#60;KRD_MAIN_2.&#34;ID&#34;&#62;) + '&#34;, &#34;' + STR(&#60;KRD_COMM_2.&#34;G32&#34;&#62;) + '&#34;])');&#13;&#10;    end&#13;&#10;  else&#13;&#10;    begin&#13;&#10;      if COMPUTE('FINDKEY(&#34;OUT_COMM_2&#34;, [&#34;' + VARTOSTR(&#60;KRD_MAIN_2.&#34;PLACEID&#34;&#62;) + '&#34;, &#34;' + VARTOSTR(&#60;KRD_MAIN_2.&#34;ID&#34;&#62;) + '&#34;, &#34;' + VARTOSTR(&#60;KRD_COMM_2.&#34;G32&#34;&#62;) + '&#34;])') then&#13;&#10;        begin&#13;&#10;          //Сначала снимем предыдущий фильтр&#13;&#10;          COMPUTE('CANCELRANGE(&#34;OUT_MAIN_2&#34;)');&#13;&#10;          COMPUTE('CANCELRANGE(&#34;OUT_COMM_2&#34;)');&#13;&#10;          //Поставим новый фильтр для текущего товара и документа&#13;&#10;          COMPUTE('SETRANGE(&#34;OUT_MAIN_2&#34;, [&#34;' + STR(vPlaceId) + '&#34;, &#34;' + STR(&#60;KRD_MAIN_2.&#34;ID&#34;&#62;) + '&#34;])');&#13;&#10;          COMPUTE('SETRANGE(&#34;OUT_COMM_2&#34;, [&#34;' + STR(vPlaceId) + '&#34;, &#34;' + STR(&#60;KRD_MAIN_2.&#34;ID&#34;&#62;) + '&#34;])');&#13;&#10;&#13;&#10;          //Пройдем цикл по выпускам&#13;&#10;          //Товары, чтобы посмотреть вес&#13;&#10;          //======================================&#13;&#10;          OUT_COMM_2.First;&#13;&#10;          while not OUT_COMM_2.Eof do begin&#13;&#10;              //Выпуски, чтобы посмотреть даты&#13;&#10;              //-------------------------------&#13;&#10;              OUT_MAIN_2.First;&#13;&#10;              while not OUT_MAIN_2.Eof do begin&#13;&#10;                  if (&#60;OUT_COMM_2.&#34;ACTOUTID&#34;&#62; = &#60;OUT_MAIN_2.&#34;ACTOUTID&#34;&#62;) and (&#60;OUT_COMM_2.&#34;G32&#34;&#62; = &#60;KRD_COMM_2.&#34;G32&#34;&#62;) then                  //Нам нужен выпуск к конкретному товару&#13;&#10;                    if (&#60;OUT_MAIN_2.&#34;G542&#34;&#62; &#60; PeriodStart) then                                //Все, что полностью выпущено раньше начала периода, нас не интересует&#13;&#10;                      if &#60;OUT_COMM_2.&#34;OUT_G35&#34;&#62; &#60;&#62; Null then                                   //Вес в выпуске д.б. заполнен&#13;&#10;                        vSumRelG35 := vSumRelG35 + &#60;OUT_COMM_2.&#34;OUT_G35&#34;&#62;;&#13;&#10;                  OUT_MAIN_2.Next&#13;&#10;                end;&#13