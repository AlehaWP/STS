<?xml version="1.0" encoding="utf-8"?>
<TfrxReport Version="4.6.78" DotMatrixReport="False" EngineOptions.ConvertNulls="False" IniFile="\Software\Fast Reports" PreviewOptions.Buttons="4095" PreviewOptions.Zoom="1" PrintOptions.Printer="По умолчанию" PrintOptions.PrintOnSheet="0" ReportOptions.Author="Алексей Нерченко" ReportOptions.CreateDate="40947.7428343981" ReportOptions.Description.Text="" ReportOptions.Name="Отчет по получателю/отправителю" ReportOptions.LastChange="40952.4968872917" ScriptLanguage="PascalScript" ScriptText.Text="VAR      &#13;&#10;bOnCurrentBook, bPeriod, bRecepient, bSender, bStore, bDateReport  : Boolean;&#13;&#10;sIniFile : string;                          &#13;&#10;sDataSetName, vSQL, sRecepient, sSender, vLinePrint, vDate, vG082, vG022, vDONUM, vTranspCat, vTranspNO, vG312, vDateReport : string;&#13;&#10;vPlaceID, vID, vLine : integer;&#13;&#10;vG35, SumG35 : Double;                        &#13;&#10;dFrom, dTo : date;                                                                                                 &#13;&#10;&#13;&#10;&#13;&#10;Const&#13;&#10;  sIniFullPath = ProgramPath() + 'print.ini';&#13;&#10;  sIniSection = 'rpt_recepient.fr3';&#13;&#10;  //sIniSectionCommon = 'Common';  &#13;&#10;  &#13;&#10;procedure FormSettings(s: integer);&#13;&#10;begin&#13;&#10;  if s=1 then begin&#13;&#10;    if cbxStore.Checked then begin&#13;&#10;      lblNumLicence.Font.Color := clBlack;&#13;&#10;      lblOt.Font.Color := clBlack;        &#13;&#10;      fdStoreLicNum.Enabled := True;&#13;&#10;      lblStoreName.Enabled := True;&#13;&#10;      fldStoreLicDate.Enabled := True;&#13;&#10;      btnCallRef.Enabled := True;                                                                &#13;&#10;        &#13;&#10;      vPlaceId := IniFile(sIniSection, 'vPlaceId', '', sIniFullPath);  &#13;&#10;      If COMPUTE('FindKey(&#34;STORES_2&#34;, [&#34;' + VARtoSTR(vPlaceId) + '&#34;])') then Begin&#13;&#10;        fdStoreLicNum.Text := &#60;STORES_2.&#34;LICENCENO&#34;&#62; ;      //   номер лицензии   &#13;&#10;        lblStoreName.Text := &#60;STORES_2.&#34;NAME&#34;&#62; ;     //   название склада &#13;&#10;        fldStoreLicDate.Date := &#60;STORES_2.&#34;LICENCEDATE&#34;&#62;;&#13;&#10;      end&#13;&#10;      else begin                           &#13;&#10;        vPlaceId:=COMPUTE('INIFILE(&#34;WorkPlace&#34;,&#34;PlaceNum&#34;,&#34;&#34;)');&#13;&#10;        If COMPUTE('FindKey(&#34;STORES_2&#34;, [&#34;' + VARtoSTR(vPlaceId) + '&#34;])') then Begin&#13;&#10;          fdStoreLicNum.Text := &#60;STORES_2.&#34;LICENCENO&#34;&#62; ;      //   номер лицензии   &#13;&#10;          lblStoreName.Text := &#60;STORES_2.&#34;NAME&#34;&#62; ;     //   название склада &#13;&#10;          fldStoreLicDate.Date := &#60;STORES_2.&#34;LICENCEDATE&#34;&#62;;        &#13;&#10;        end;&#13;&#10;      end;&#13;&#10;    end&#13;&#10;    else begin&#13;&#10;      lblNumLicence.Font.Color := clGray;&#13;&#10;      lblOt.Font.Color := clGray;        &#13;&#10;      fdStoreLicNum.Enabled := False;&#13;&#10;      lblStoreName.Enabled := False;&#13;&#10;      fldStoreLicDate.Enabled := False;&#13;&#10;      btnCallRef.Enabled := False;          &#13;&#10;      fdStoreLicNum.Text := '' ;      //   номер лицензии   &#13;&#10;      lblStoreName.Text := '' ;     //   название склада &#13;&#10;      fldStoreLicDate.Date := Date();    &#13;&#10;    end;  &#13;&#10;  end;&#13;&#10;  if s=2 then begin&#13;&#10;    if cbxFromPeriod.Checked then begin&#13;&#10;      lblFrom.Font.Color := clBlack;&#13;&#10;      lblTo.Font.Color := clBlack;        &#13;&#10;      dtFrom.Enabled := True;&#13;&#10;      dtTo.Enabled := True;   &#13;&#10;      dtFrom.Date :=  StrToDateDef(IniFile(sIniSection, 'dFrom', '', sIniFullPath), date);&#13;&#10;      dtTo.Date :=  StrToDateDef(IniFile(sIniSection, 'dTo', '', sIniFullPath), date);          &#13;&#10;    end&#13;&#10;    else begin&#13;&#10;      lblFrom.Font.Color := clGray;&#13;&#10;      lblTo.Font.Color := clGray;        &#13;&#10;      dtFrom.Enabled := False;&#13;&#10;      dtTo.Enabled := False;   &#13;&#10;      dtFrom.Date :=  Date;&#13;&#10;      dtTo.Date :=  Date;    &#13;&#10;    end;  &#13;&#10;  end;&#13;&#10;  if s=3 then begin&#13;&#10;    if cbxRecepient.Checked then begin&#13;&#10;      btnRecepient.Enabled := True;        &#13;&#10;      fdRecepient.Enabled := True;                                                                                      &#13;&#10;      fdRecepient.Text :=  IniFile(sIniSection, 'sRecepient', '', sIniFullPath);&#13;&#10;    end&#13;&#10;    else begin      &#13;&#10;      btnRecepient.Enabled := False;&#13;&#10;      fdRecepient.Enabled := False;           &#13;&#10;      fdRecepient.Text :=  '';        &#13;&#10;    end;              &#13;&#10;  end;&#13;&#10;  if s=4 then begin&#13;&#10;    if cbxSender.Checked then begin&#13;&#10;      btnSender.Enabled := True;                       &#13;&#10;      fdSender.Enabled := True;                                                                                      &#13;&#10;      fdSender.Text :=  IniFile(sIniSection, 'sSender', '', sIniFullPath);     &#13;&#10;    end&#13;&#10;    else begin&#13;&#10;      btnSender.Enabled := False;        &#13;&#10;      fdSender.Enabled := False;                                                                                      &#13;&#10;      fdSender.Text :=  '';        &#13;&#10;    end;  &#13;&#10;  end;&#13;&#10;  if s=5 then begin&#13;&#10;    if cbxDateReport.Checked then begin&#13;&#10;      DateReport.Enabled := True;                                                                                      &#13;&#10;      DateReport.Date :=  Date;     &#13;&#10;    end&#13;&#10;    else begin&#13;&#10;      DateReport.Enabled := False;                                                                                      &#13;&#10;      DateReport.Date :=  Date;        &#13;&#10;    end;                 &#13;&#10;  end;      &#13;&#10;end;          &#13;&#10;&#13;&#10;  &#13;&#10;procedure DialogPage1OnShow(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  cbExport.Checked := IniFile(sIniSection, 'Export', 'False', sIniFullPath);                       &#13;&#10;  cbxStore.Checked := IniFile(sIniSection, 'bStore', 'False', sIniFullPath);&#13;&#10;  FormSettings(1);&#13;&#10;  cbxFromPeriod.Checked := IniFile(sIniSection, 'bPeriod', 'False', sIniFullPath);&#13;&#10;  FormSettings(2);&#13;&#10;  cbxRecepient.Checked := IniFile(sIniSection, 'bRecepient', 'False', sIniFullPath);&#13;&#10;  FormSettings(3);      &#13;&#10;  cbxSender.Checked := IniFile(sIniSection, 'bSender', 'False', sIniFullPath);&#13;&#10;  FormSettings(4);      &#13;&#10;  cbxCurBook.Checked := IniFile(sIniSection, 'bOnCurrentBook', 'False', sIniFullPath);&#13;&#10;  cbxDateReport.Checked := IniFile(sIniSection, 'bDateReport', 'False', sIniFullPath);&#13;&#10;  FormSettings(5);      &#13;&#10;end;  &#13;&#10;&#13;&#10;procedure cbxStoreOnClick(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10; FormSettings(1);  &#13;&#10;end;&#13;&#10;&#13;&#10;procedure cbxFromPeriodOnClick(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  FormSettings(2);  &#13;&#10;end;&#13;&#10;&#13;&#10;procedure cbxRecepientOnClick(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  FormSettings(3);  &#13;&#10;end;&#13;&#10;&#13;&#10;procedure cbxSenderOnClick(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  FormSettings(4);  &#13;&#10;end;&#13;&#10;&#13;&#10;procedure cbxDateReportOnClick(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  FormSettings(5);  &#13;&#10;end;&#13;&#10;&#13;&#10;procedure btnCallRefOnClick(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  with btnCallRef, Engine do&#13;&#10;  begin&#13;&#10;    COMPUTE(' globalvar (&#34;strStoreNumLic&#34;,String)'); &#13;&#10;    COMPUTE(' globalvar (&#34;strStoreName&#34;,String)');&#13;&#10;    COMPUTE(' globalvar (&#34;sPlaceId&#34;,Integer)');&#13;&#10;    COMPUTE(' globalvar (&#34;sLicenceDate&#34;,DateTime)');&#13;&#10;    COMPUTE('OPENQUERY (&#34;Stors&#34;, &#34;STS_DB&#34;, &#34;Select * from STORES&#34;)');&#13;&#10;&#13;&#10;&#13;&#10;    IF COMPUTE('SelectValues(&#34;Выбор складских площадок&#34;, &#34;STORS&#34;,'+&#13;&#10;                            '['+&#13;&#10;                             '[&#34;LICENCENO&#34;, &#34;№ лицензии&#34;, 12],'+&#13;&#10;                             '[&#34;LICENCEDATE&#34;, &#34;Дата лицензии&#34;, 12],'+                                 &#13;&#10;                             '[&#34;NAME&#34;, &#34;Название склада&#34;, 30]'+&#13;&#10;                            '],'+&#13;&#10;                            '['+&#13;&#10;                             '[&#34;PLACEID&#34;, &#34;sPlaceid&#34;],'+                                &#13;&#10;                             '[&#34;LICENCENO&#34;, &#34;strStoreNumLic&#34;],'+&#13;&#10;                             '[&#34;LICENCEDATE&#34;, &#34;sLicenceDate&#34;],'+ &#13;&#10;                             '[&#34;NAME&#34;, &#34;strStoreName&#34;]'+  &#13;&#10;                            '],'+  &#13;&#10;                            '&#34;&#34;, &#34;STS_DB&#34;)') = '1' then&#13;&#10;&#13;&#10;      begin      &#13;&#10;        fdStoreLicNum.Text := COMPUTE('strStoreNumLic');  //   номер  лиценцзии  &#13;&#10;        lblStoreName.Text :=COMPUTE('strStoreName') ;     //   название склада      &#13;&#10;        vPlaceId := COMPUTE('sPlaceid');       &#13;&#10;&#13;&#10;        //vLicenceDate := COMPUTE('sLicenceDate');&#13;&#10;        fldStoreLicDate.Date := STRTODATEDEF(COMPUTE('sLicenceDate'), Date);  // дата выдачи лицензии  &#13;&#10;      end;  &#13;&#10;&#13;&#10;  end  &#13;&#10;end;&#13;&#10;&#13;&#10;procedure btnRecepientOnClick(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  with btnRecepient, Engine do&#13;&#10;  begin&#13;&#10;    COMPUTE(' globalvar (&#34;strRecepient&#34;,String)'); &#13;&#10;    COMPUTE(' globalvar (&#34;strRecipientINN&#34;,String)'); &#13;&#10;    COMPUTE('OPENQUERY (&#34;Recepient&#34;, &#34;STS_DB&#34;, &#34;Select Distinct G082, G084C  from KRD_MAIN WHERE G082 is not NULL '+&#13;&#10;                                                IIF((cbxStore.Checked) and (VarToStr(vPlaceId) &#60;&#62; '') and (VarToStr(vPlaceId) &#60;&#62; 'Null'), ' AND PLACEID= '+ VarToStr(vPlaceID) +, '')+ &#13;&#10;                                                IIF(cbxFromPeriod.Checked, ' AND BD_DATE &#62;= ' + CHR(39) + FormatDateTime('DD.MM.YYYY', dtFrom.Date) + ' 00:00' + CHR(39) +&#13;&#10;                                                ' AND BD_DATE &#60;= '+ CHR(39) + FormatDateTime('DD.MM.YYYY', dtTo.Date) + ' 23:59'+ CHR(39),'') +&#13;&#10;                                                //IIF(cbxSender.Checked and (fdSender.text &#60;&#62; ''), ' AND G022= '+ CHR(39) + VarToStr(fdSender.text) + CHR(39) +, '')+                                                    &#13;&#10;                                                ' &#34;)');&#13;&#10;&#13;&#10;&#13;&#10;    IF COMPUTE('SelectValues(&#34;Выбор складских площадок&#34;, &#34;Recepient&#34;,'+&#13;&#10;                            '['+&#13;&#10;                             '[&#34;G082&#34;, &#34;Наименование&#34;, 30],'+                                     &#13;&#10;                             '[&#34;G084C&#34;, &#34;ИНН&#34;, 15]'+&#13;&#10;                            '],'+&#13;&#10;                            '['+&#13;&#10;                             '[&#34;G082&#34;, &#34;strRecepient&#34;],'+                                &#13;&#10;                             '[&#34;G084C&#34;, &#34;strRecipientINN&#34;]'+  &#13;&#10;                            '],'+  &#13;&#10;                            '&#34;&#34;, &#34;STS_DB&#34;)') = '1' then&#13;&#10;&#13;&#10;      begin      &#13;&#10;        fdRecepient.Text := COMPUTE('strRecepient');  //   номер  лиценцзии          &#13;&#10;      end;  &#13;&#10;&#13;&#10;  end  &#13;&#10;end;&#13;&#10;&#13;&#10;procedure btnSenderOnClick(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  with btnSender, Engine do&#13;&#10;  begin&#13;&#10;    COMPUTE(' globalvar (&#34;strSender&#34;,String)'); &#13;&#10;    COMPUTE(' globalvar (&#34;strSenderINN&#34;,String)'); &#13;&#10;    COMPUTE('OPENQUERY (&#34;Sender&#34;, &#34;STS_DB&#34;, &#34;Select Distinct G022, G024C  from KRD_MAIN WHERE G022 is not NULL '+&#13;&#10;                                                IIF((cbxStore.Checked) and (VarToStr(vPlaceId) &#60;&#62; '') and (VarToStr(vPlaceId) &#60;&#62; 'Null'), ' AND PLACEID= '+ VarToStr(vPlaceID) +, '')+              &#13;&#10;                                                IIF(cbxFromPeriod.Checked, ' AND BD_DATE &#62;= ' + CHR(39) + FormatDateTime('DD.MM.YYYY', dtFrom.Date) + ' 00:00' + CHR(39) +&#13;&#10;                                                ' AND BD_DATE &#60;= '+ CHR(39) + FormatDateTime('DD.MM.YYYY', dtTo.Date) + ' 23:59'+ CHR(39),'') +&#13;&#10;                                                IIF(cbxRecepient.Checked and (fdRecepient.text &#60;&#62; ''), ' AND G082= '+ CHR(39) + VarToStr(fdRecepient.text) + CHR(39) +, '')+                                                    &#13;&#10;                                                ' &#34;)');&#13;&#10;&#13;&#10;&#13;&#10;    IF COMPUTE('SelectValues(&#34;Выбор складских площадок&#34;, &#34;Sender&#34;,'+&#13;&#10;                            '['+&#13;&#10;                             '[&#34;G022&#34;, &#34;Наименование&#34;, 30],'+                                     &#13;&#10;                             '[&#34;G024C&#34;, &#34;ИНН&#34;, 15]'+&#13;&#10;                            '],'+&#13;&#10;                            '['+&#13;&#10;                             '[&#34;G022&#34;, &#34;strSender&#34;],'+                                &#13;&#10;                             '[&#34;G024C&#34;, &#34;strSenderINN&#34;]'+  &#13;&#10;                            '],'+  &#13;&#10;                            '&#34;&#34;, &#34;STS_DB&#34;)') = '1' then&#13;&#10;&#13;&#10;      begin      &#13;&#10;        fdSender.Text := COMPUTE('strSender');  //   номер  лиценцзии          &#13;&#10;      end;  &#13;&#10;&#13;&#10;  end  &#13;&#10;end;    &#13;&#10;  &#13;&#10;procedure cbtnOkOnClick(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  bPeriod := cbxFromPeriod.Checked;                                &#13;&#10;  bStore := cbxStore.Checked;                        &#13;&#10;  bRecepient := cbxRecepient.Checked;&#13;&#10;  bSender := cbxSender.Checked;&#13;&#10;  bOnCurrentBook := cbxCurBook.Checked;&#13;&#10;  bDateReport := cbxDateReport.Checked;                                        &#13;&#10;                                        &#13;&#10;  dFrom := dtFrom.Date;&#13;&#10;  dTo := dtTo.Date;&#13;&#10;  sRecepient := fdRecepient.Text;&#13;&#10;  sSender := fdSender.Text;&#13;&#10;&#13;&#10;  if cbExport.Checked then begin&#13;&#10;    PageHeader1.Visible := False;&#13;&#10;    ReportTitle1.Visible := True;                                                                  &#13;&#10;    Page1.EndlessHeight := True;&#13;&#10;  end            &#13;&#10;  else begin&#13;&#10;    PageHeader1.Visible := True;                 &#13;&#10;    ReportTitle1.Visible := False;     &#13;&#10;    Page1.EndlessHeight := False;&#13;&#10;  end;            &#13;&#10;&#13;&#10;  WriteIniFile(sIniSection, 'bStore', VarToStr(bStore), sIniFullPath);&#13;&#10;    if bStore then                                                                                &#13;&#10;      WriteIniFile(sIniSection, 'vPlaceId', VarToStr(vPlaceId), sIniFullPath);&#13;&#10;        &#13;&#10;  WriteIniFile(sIniSection, 'bPeriod', VarToStr(bPeriod), sIniFullPath);&#13;&#10;    if bPeriod then  begin          &#13;&#10;        WriteIniFile(sIniSection, 'dFrom', FormatDateTime('DD.MM.YYYY', dFrom), sIniFullPath);&#13;&#10;        WriteIniFile(sIniSection, 'dTo', FormatDateTime('DD.MM.YYYY', dTo), sIniFullPath);            &#13;&#10;    end;                  &#13;&#10;  WriteIniFile(sIniSection, 'bRecepient', VarToStr(bRecepient), sIniFullPath);&#13;&#10;    if bRecepient then&#13;&#10;      WriteIniFile(sIniSection, 'sRecepient', VarToStr(sRecepient), sIniFullPath);                       &#13;&#10;  WriteIniFile(sIniSection, 'bSender', VarToStr(bSender), sIniFullPath);&#13;&#10;    if bSender then&#13;&#10;      WriteIniFile(sIniSection, 'sSender', VarToStr(sSender), sIniFullPath);      &#13;&#10;  WriteIniFile(sIniSection, 'bOnCurrentBook', VarToStr(bOnCurrentBook), sIniFullPath);&#13;&#10;  WriteIniFile(sIniSection, 'bDateReport', VarToStr(bDateReport), sIniFullPath);&#13;&#10;  WriteIniFile(sIniSection, 'Export', VarToStr(cbExport.Checked), sIniFullPath);                                &#13;&#10;    &#13;&#10;  &#13;&#10;  if bOnCurrentBook then begin&#13;&#10;    sDataSetName := 'KRD_MAIN';&#13;&#10;    MasterData1.DataSetName := 'KRD_MAIN';&#13;&#10;    COMPUTE('SETFILTER(&#34;KRD_MAIN&#34;,&#34; NBD is not Null ' +&#13;&#10;                       IIF((bStore) and (VarToStr(vPlaceId) &#60;&#62; '') and (VarToStr(vPlaceId) &#60;&#62; 'Null'), ' AND PLACEID= '+ VarToStr(vPlaceID) +, '')+                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                        &#13;&#10;                       IIF(bPeriod, ' AND BD_DATE &#62;= ' + CHR(39) + FormatDateTime('DD.MM.YYYY', dtFrom.Date) + ' 00:00' + CHR(39) +&#13;&#10;                       ' AND BD_DATE &#60;= '+ CHR(39) + FormatDateTime('DD.MM.YYYY', dtTo.Date) + ' 23:59'+ CHR(39),'') +&#13;&#10;                       IIF(bRecepient, ' AND G082= '+ CHR(39) + VarToStr(sRecepient) + CHR(39) +, '')+&#13;&#10;                       IIF(bSender, ' AND G022= '+ CHR(39) + VarToStr(sSender) + CHR(39) +, '')+  &#13;&#10;    ' &#34;)');&#13;&#10;  end&#13;&#10;  else begin&#13;&#10;    vSQL := 'SELECT * FROM KRD_MAIN  WHERE NBD is not Null ' +  &#13;&#10;            IIF((bStore) and (VarToStr(vPlaceId) &#60;&#62; '') and (VarToStr(vPlaceId) &#60;&#62; 'Null'), ' AND PLACEID= '+ VarToStr(vPlaceID) +, '')+                        &#13;&#10;            IIF(bPeriod, ' AND BD_DATE &#62;= ' + CHR(39) + FormatDateTime('DD.MM.YYYY', dtFrom.Date) + ' 00:00' + CHR(39) +&#13;&#10;            ' AND BD_DATE &#60;= '+ CHR(39) + FormatDateTime('DD.MM.YYYY', dtTo.Date) + ' 23:59'+ CHR(39),'') +&#13;&#10;            IIF(bRecepient, ' AND G082= '+ CHR(39) + VarToStr(sRecepient) + CHR(39) +, '')+&#13;&#10;            IIF(bSender, ' AND G022= '+ CHR(39) + VarToStr(sSender) + CHR(39) +, '')+                &#13;&#10;            ' ORDER BY SHOW_NBD';&#13;&#10;     //showmessage(vSQL);                                          &#13;&#10;&#13;&#10;    COMPUTE('OPENQUERY(&#34;QRYKRDMAIN2&#34;, &#34;STS_DB&#34;, &#34;' + vSQL + '&#34;)');&#13;&#10;    sDataSetName := 'QRYKRDMAIN2';&#13;&#10;    MasterData1.RowCount := StrToInt(COMPUTE('RECORDCOUNT(&#34;QRYKRDMAIN2&#34;)'));        &#13;&#10;  end;            &#13;&#10;  SumG35 := 0;                         &#13;&#10;end;&#13;&#10;&#13;&#10;&#13;&#10;&#13;&#10;procedure Page1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;   COMPUTE('FIRST(&#34;' + sDataSetName + '&#34;)');  &#13;&#10;end;  &#13;&#10;&#13;&#10;procedure MasterData1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin   &#13;&#10;  COMPUTE('CANCELRANGE(&#34;KRD_COMM_2&#34;)');&#13;&#10;  COMPUTE('SETRANGE(&#34;KRD_COMM_2&#34;, [&#34;' + VarToStr(COMPUTE('FIELDVALUE(&#34;' + sDataSetName + '&#34;, &#34;PLACEID&#34;)')) + '&#34;, &#34;' +&#13;&#10;                                        VarToStr(COMPUTE('FIELDVALUE(&#34;' + sDataSetName + '&#34;, &#34;ID&#34;)')) + '&#34;])');&#13;&#10;  COMPUTE('FIRST(&#34;KRD_COMM_2&#34;)');                                                              &#13;&#10;      &#13;&#10;end;&#13;&#10;  &#13;&#10;&#13;&#10;&#13;&#10;procedure Footer1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;   if not bOnCurrentBook then COMPUTE('NEXT(&#34;QRYKRDMAIN2&#34;)');  &#13;&#10;end;&#13;&#10;&#13;&#10;procedure DetailData1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  vID := StrToInt(COMPUTE('FIELDVALUE(&#34;' + sDataSetName + '&#34;, &#34;ID&#34;)'));&#13;&#10;  //showmessage(vId);                                &#13;&#10;  SetLine(vId);&#13;&#10;  if vDONum = '' then begin                                                             &#13;&#10;      COMPUTE('OPENQUERY(&#34;REL&#34;, &#34;STS_DB&#34;, &#34;SELECT RELEASE_NO FROM RELEASE R, REL_COMM RC WHERE ((R.PLACEID = RC.PLACEID) and (R.ID = RC.ID) and (R.COUNTER = RC.COUNTER)) ' + &#13;&#10;            ' AND (R.PLACEID= '+ VarToStr(COMPUTE('FIELDVALUE(&#34;' + sDataSetName + '&#34;, &#34;PLACEID&#34;)')) +&#13;&#10;            ') AND (R.ID='+ VarToStr(vID) + ') AND (RC.G32='+ VarToStr(&#60;KRD_COMM_2.&#34;G32&#34;&#62;) + ')' +                                                                                                                  &#13;&#10;            ' ORDER BY RELEASE_NO&#34;)');&#13;&#10;    COMPUTE('FIRST(&#34;REL&#34;)');   &#13;&#10;    IF COMPUTE('RECORDCOUNT(&#34;REL&#34;)') &#60;&#62; 0 then                         &#13;&#10;      vDONUM := 'ДО2: ' + COMPUTE('UNIONVALUES(&#34;REL&#34;,[&#34;RELEASE_NO&#34;], &#34;;&#34; , &#34;&#34;)');&#13;&#10;    COMPUTE('CLOSEDATASET(&#34;REL&#34;)');          &#13;&#10;  end;         &#13;&#10;  vG35 := &#60;KRD_COMM_2.&#34;G35&#34;&#62;;&#13;&#10;  SumG35 := SumG35 + &#60;KRD_COMM_2.&#34;G35&#34;&#62;;                                                  &#13;&#10;  vG312 := &#60;KRD_COMM_2.&#34;G312&#34;&#62;;                          &#13;&#10;    &#13;&#10;end;&#13;&#10;&#13;&#10;&#13;&#10;procedure SetLine(ID: Integer);&#13;&#10;var&#13;&#10;  vCurID: Integer;&#13;&#10;begin&#13;&#10;  vDate := ''; vDONUM := ''; vG082 := ''; vTranspCat := ''; vTranspNO := ''; vLinePrint := '';                             &#13;&#10;  //Номер по порядку и Дата помещения&#13;&#10;  if vCurID &#60;&#62; ID then begin&#13;&#10;    vLine := vLine + 1;&#13;&#10;    vLinePrint := VarToStr(vLine);                                                          &#13;&#10;    //Дата ДО                   &#13;&#10;    vDate := FormatDateTime('DD.MM.YYYY', COMPUTE('FIELDVALUE(&#34;' + sDataSetName + '&#34;, &#34;BD_DATE&#34;)'));                  &#13;&#10;    //Номер ДО                                       &#13;&#10;    COMPUTE('OPENQUERY(&#34;REL&#34;, &#34;STS_DB&#34;, &#34;SELECT RELEASE_NO FROM RELEASE R, REL_COMM RC WHERE ((R.PLACEID = RC.PLACEID) and (R.ID = RC.ID) and (R.COUNTER = RC.COUNTER)) ' + &#13;&#10;            ' AND (R.PLACEID= '+ VarToStr(COMPUTE('FIELDVALUE(&#34;' + sDataSetName + '&#34;, &#34;PLACEID&#34;)')) +&#13;&#10;            ') AND (R.ID='+ VarToStr(ID) + ') AND (RC.G32 = 1)' +                                                                                                                  &#13;&#10;            ' ORDER BY RELEASE_NO&#34;)');&#13;&#10;    COMPUTE('FIRST(&#34;REL&#34;)');              &#13;&#10;    vDONUM := 'ДО1: ' + VarToStr(COMPUTE('FIELDVALUE(&#34;' + sDataSetName + '&#34;, &#34;NBD&#34;)'));   &#13;&#10;    IF COMPUTE('RECORDCOUNT(&#34;REL&#34;)') &#60;&#62; 0 then                         &#13;&#10;      vDONUM := vDONUM + CHR(13) + CHR(13) +'ДО2: ' + COMPUTE('UNIONVALUES(&#34;REL&#34;,[&#34;RELEASE_NO&#34;], &#34;;&#34; , &#34;&#34;)');&#13;&#10;    //Получатель                      