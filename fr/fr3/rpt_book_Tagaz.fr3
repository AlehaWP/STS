<?xml version="1.0" encoding="utf-8"?>
<TfrxReport Version="4.6.78" DotMatrixReport="False" EngineOptions.ConvertNulls="False" IniFile="\Software\Fast Reports" PreviewOptions.Buttons="4095" PreviewOptions.Zoom="1" PrintOptions.Printer="По умолчанию" PrintOptions.PrintOnSheet="0" ReportOptions.CreateDate="38287.7802357292" ReportOptions.Description.Text="" ReportOptions.Name="Книга учета товаров и тр.средств (ТАГАЗ)" ReportOptions.LastChange="41141.5643068171" ScriptLanguage="PascalScript" ScriptText.Text="Var  &#13;&#10;  iLine: Integer;&#13;&#10;  vPlaceID, vID, vFirstPodhTov: Integer;&#13;&#10;  dtPeriodEnd, dtPeriodStart, dtOnDate : TDateTime;  &#13;&#10;  strPeriod: String;&#13;&#10;&#13;&#10;  vOutDate, vOutTime: String;&#13;&#10;&#13;&#10;  dRelAtRepDateG311: Double;    &#13;&#10;  sRelAtPeriodG311: String;   &#13;&#10;  dQtyAtStoreG311: Double;&#13;&#10;&#13;&#10;  dSumQtyAtStoreG311: Double;&#13;&#10;  dSumRelAtPeriodG311: Double;&#13;&#10;  dSumRestAtRepDateG311: Double;&#13;&#10;&#13;&#10;  vFilterCrit, vSQL: String;&#13;&#10;  vLine: Integer; &#13;&#10;  vLinePrint, vQtyAtStoreG311: String;&#13;&#10;  vStoreDate, vContno, vContComment, vComment, vStoreDocNo, vAddInfo, vRemark: String;&#13;&#10;  TransDocName, TransDocNo, TransDocDate,                                                          &#13;&#10;  vGTD, vDTType, vDTDate, vG33, vG082 :String; &#13;&#10;  vG311_OUT,vG35_OUT,vG42_OUT: Double;&#13;&#10;  dtLastDateOut : TDateTime;                            &#13;&#10;  vOnCurrentBook: Boolean;&#13;&#10;  vCanPrint: Boolean;&#13;&#10;  iRecCount, i : integer;                          &#13;&#10;  sDataSetName: String;&#13;&#10;&#13;&#10;&#13;&#10;&#13;&#10;procedure Page1OnActivate(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  fldLicExt.Text := '';&#13;&#10;  fldStoreLicNum.Text := '';&#13;&#10;  fldStoreNum.Text := '';&#13;&#10;  Num.Text := '1';                                &#13;&#10;  dtOn.Enabled := False;  &#13;&#10;&#13;&#10;  vPlaceId:=StrToInt(COMPUTE('INIFILE(&#34;Print&#34;,&#34;PlaceId&#34;,&#34;0&#34;)'));&#13;&#10;  If COMPUTE('FindKey(&#34;STORES_2&#34;, [&#34;' + STR(vPlaceId) + '&#34;])') then begin&#13;&#10;     fldStoreLicNum.Text := &#60;STORES_2.&#34;LICENCENO&#34;&#62; ;      //   номер лицензии&#13;&#10;     fldStoreNum.Text := '№ склада: ' + &#60;STORES_2.&#34;STORE_NO&#34;&#62; ; //   номер склада&#13;&#10;     lblStoreName.Caption := &#60;STORES_2.&#34;NAME&#34;&#62; ;     //   название склада&#13;&#10;     fldLicExt.Text := &#60;STORES_2.&#34;LICENCENO_EXT&#34;&#62;;     //   Признак перерегистрации&#13;&#10;     vCanPrint := True;&#13;&#10;     cbtnOk.Enabled := True;&#13;&#10;  end&#13;&#10;  else begin&#13;&#10;    vPlaceId:=StrToInt(COMPUTE('INIFILE(&#34;WorkPlace&#34;,&#34;PlaceNum&#34;,&#34;0&#34;)'));&#13;&#10;    If COMPUTE('FindKey(&#34;STORES_2&#34;, [&#34;' + STR(vPlaceId) + '&#34;])') then begin&#13;&#10;      fldStoreLicNum.Text := &#60;STORES_2.&#34;LICENCENO&#34;&#62; ;      //   номер лицензии&#13;&#10;      fldStoreNum.Text := '№ склада: ' + &#60;STORES_2.&#34;STORE_NO&#34;&#62; ; //   номер склада&#13;&#10;      lblStoreName.Caption := &#60;STORES_2.&#34;NAME&#34;&#62; ;     //   название склада&#13;&#10;      fldLicExt.Text := &#60;STORES_2.&#34;LICENCENO_EXT&#34;&#62;;     //   Признак перерегистрации&#13;&#10;      vCanPrint := True;&#13;&#10;      cbtnOk.Enabled := True;&#13;&#10;    end&#13;&#10;    else begin&#13;&#10;      fldStoreLicNum.Text := '';&#13;&#10;      fldStoreNum.Text := '№ склада: ';&#13;&#10;      lblStoreName.Caption := '';&#13;&#10;      fldLicExt.Text := '';&#13;&#10;      vCanPrint := False;&#13;&#10;      cbtnOk.Enabled := False;&#13;&#10;    end&#13;&#10;  end;&#13;&#10;&#13;&#10;  dtFrom.Date := STRTODATEDEF(COMPUTE('INIFILE(&#34;Print&#34;,&#34;PeriodStart&#34;,&#34;&#34;)'), DATE);&#13;&#10;  dtTo.Date := STRTODATEDEF(COMPUTE('INIFILE(&#34;Print&#34;,&#34;PeriodEnd&#34;,&#34;&#34;)'), DATE);&#13;&#10;&#13;&#10;  if COMPUTE('INIFILE(&#34;Print&#34;,&#34;DO1orDO2&#34;,&#34;False&#34;)') = 'True' then&#13;&#10;    cbxDO1orDO2.Checked := True&#13;&#10;  else cbxDO1orDO2.Checked := False;&#13;&#10;    &#13;&#10;  if COMPUTE('INIFILE(&#34;Print&#34;,&#34;Ostatki&#34;,&#34;True&#34;)') = 'True' then begin&#13;&#10;     rbtnOstatki.Checked := True;&#13;&#10;     dtOn.Enabled := True;         &#13;&#10;     dtOn.Date := Date();                                                                       &#13;&#10;  end;    &#13;&#10;  &#13;&#10;  if COMPUTE('INIFILE(&#34;Print&#34;,&#34;OnCurrentBook&#34;,&#34;True&#34;)') = 'True' then begin&#13;&#10;     rbtnCurBook.Checked := True;&#13;&#10;     cbxDO1orDO2.Checked := False;         &#13;&#10;  end;               &#13;&#10;&#13;&#10;&#13;&#10;&#13;&#10;&#13;&#10;end;            &#13;&#10;&#13;&#10;procedure btnCallRefOnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  with btnCallRef, Engine do&#13;&#10;  begin&#13;&#10;    COMPUTE('GLOBALVAR(&#34;strStoreNumLic&#34;,String)');&#13;&#10;    COMPUTE('GLOBALVAR(&#34;strStoreNum&#34;,String)');&#13;&#10;    COMPUTE('GLOBALVAR(&#34;strStoreName&#34;,String)');&#13;&#10;    COMPUTE('GLOBALVAR(&#34;strLicExt&#34;,String)');&#13;&#10;    COMPUTE('GLOBALVAR(&#34;sPlaceId&#34;,Integer)');&#13;&#10;    COMPUTE('OPENQUERY (&#34;Stors&#34;, &#34;STS_DB&#34;, &#34;Select * from STORES&#34;)');&#13;&#10;    COMPUTE('SelectValues(&#34;Список складов&#34;, &#34;Stors&#34;,[[&#34;LICENCENO&#34;,   &#34;Номер лицензии&#34;, 15],[&#34;LICENCENO_EXT&#34;,   &#34; &#34;, 1],[&#34;STORE_NO&#34;,   &#34;Номер склада&#34;, 15],[&#34;NAME&#34;,   &#34;Название склада&#34;, 30],[&#34;PlaceId&#34;,   &#34;&#34;, 10]],[[&#34;LICENCENO&#34;, &#34;strStoreNumLic&#34;],[&#34;STORE_NO&#34;, &#34;strStoreNum&#34;],[&#34;NAME&#34;, &#34;strStoreName&#34;],[&#34;LICENCENO_EXT&#34;, &#34;strLicExt&#34;],[&#34;PlaceId&#34;, &#34;sPlaceId&#34;],],&#34;&#34;, &#34;STS_DB&#34;)');&#13;&#10;&#13;&#10;&#13;&#10;    fldStoreLicNum.Text := COMPUTE('Value(&#34;strStoreNumLic&#34;)') ;      //   номер лицензии&#13;&#10;    fldStoreNum.Text := '№ склада: ' + COMPUTE('value(&#34;strStoreNum&#34;)') ; //   номер склада&#13;&#10;    lblStoreName.Caption := COMPUTE('Value(&#34;strStoreName&#34;)') ;     //   название склада&#13;&#10;    fldLicExt.Text := COMPUTE('Value(&#34;strLIcExt&#34;)');     //   Признак перерегистрации&#13;&#10;    vPlaceId := StrToInt(COMPUTE('Value(&#34;sPlaceId&#34;)'));&#13;&#10;    if Length(COMPUTE('Value(&#34;strStoreNumLic&#34;)')) &#62; 0 then&#13;&#10;      cbtnOk.Enabled := True&#13;&#10;    else cbtnOk.Enabled := False;&#13;&#10;  end&#13;&#10;end;&#13;&#10;&#13;&#10;&#13;&#10;procedure rbtnPeriodOnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  with rbtnPeriod, Engine do&#13;&#10;  begin&#13;&#10;&#13;&#10;    dtFrom.Enabled := True;&#13;&#10;    dtTo.Enabled := True;&#13;&#10;    cbxDO1orDO2.Enabled := True;&#13;&#10;    lblPer.Font.color := clWindowText;&#13;&#10;    fldStoreLicNum.Enabled := True;&#13;&#10;    fldStoreNum.Enabled := True;&#13;&#10;    lblStoreName.Enabled := True;&#13;&#10;    fldLicExt.Enabled := True;&#13;&#10;    btnCallRef.Enabled := True;&#13;&#10;    lblNumLicence.Enabled := True;&#13;&#10;    dtOn.Enabled := False;                                                                                &#13;&#10;&#13;&#10;    cbtnOk.Enabled := vCanPrint;&#13;&#10;  end&#13;&#10;end;&#13;&#10;&#13;&#10;procedure rbtnOstatkiOnClick(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  with rbtnOstatki, Engine do&#13;&#10;  begin&#13;&#10;&#13;&#10;    dtFrom.Enabled := False;&#13;&#10;    dtTo.Enabled := False;&#13;&#10;    cbxDO1orDO2.Enabled := False;&#13;&#10;    cbxDO1orDO2.Checked := False;&#13;&#10;    lblPer.Font.color := clWindowFrame; &#13;&#10;    fldStoreLicNum.Enabled := True;&#13;&#10;    fldStoreNum.Enabled := True;&#13;&#10;    lblStoreName.Enabled := True;&#13;&#10;    fldLicExt.Enabled := True;&#13;&#10;    btnCallRef.Enabled := True;&#13;&#10;    lblNumLicence.Enabled := True;&#13;&#10;    dtOn.Enabled := True;        &#13;&#10;    dtOn.Date := Date();  &#13;&#10;    cbtnOk.Enabled := vCanPrint;&#13;&#10;&#13;&#10;  end  &#13;&#10;end;  &#13;&#10;  &#13;&#10;procedure rbtnCurBookOnClick(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  with rbtnCurBook, Engine do&#13;&#10;  begin&#13;&#10;&#13;&#10;    dtFrom.Enabled := False;&#13;&#10;    dtTo.Enabled := False;&#13;&#10;    dtOn.Enabled := False;      &#13;&#10;    cbxDO1orDO2.Enabled := False;&#13;&#10;    cbxDO1orDO2.Checked := False;&#13;&#10;    lblPer.Font.color := clWindowFrame; &#13;&#10;    fldStoreLicNum.Enabled := False;&#13;&#10;    fldStoreNum.Enabled := False;&#13;&#10;    lblStoreName.Enabled := False;&#13;&#10;    fldLicExt.Enabled := False;&#13;&#10;    btnCallRef.Enabled := False;&#13;&#10;    lblNumLicence.Enabled := False;&#13;&#10;&#13;&#10;      &#13;&#10;    vCanPrint := cbtnOk.Enabled;&#13;&#10;    cbtnOk.Enabled := True;&#13;&#10;&#13;&#10;  end&#13;&#10;end;&#13;&#10;&#13;&#10;    &#13;&#10;&#13;&#10;procedure cbtnOkOnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  with cbtnOk, Engine do begin&#13;&#10;    &#13;&#10;    IF NUM.text &#60;&#62;'' then&#13;&#10;      IF STRTOINT(NUM.text) &#62; 0 then&#13;&#10;        iLine := STRTOINT(NUM.text) - 1;                                                                     &#13;&#10;&#13;&#10;    vOnCurrentBook := rbtnCurBook.Checked;&#13;&#10;&#13;&#10;    if rbtnPeriod.checked then begin&#13;&#10;         dtPeriodStart := Trunc(dtFrom.Date) + STRTOTIME('00:00:00');  &#13;&#10;         dtPeriodEnd := Trunc(dtTo.Date) + STRTOTIME('23:59:59');&#13;&#10;        //Начало периода для подсчета кол-ва на начало периода&#13;&#10;       &#13;&#10;         strPeriod := 'с ' + FORMATDATETIME(&#34;DD MMMM YYYY&#34;, dtPeriodStart) + ' по ' + FORMATDATETIME(&#34;DD MMMM YYYY&#34;, dtPeriodend);   &#13;&#10;         if cbxDO1orDO2.checked then begin  &#13;&#10;            vSQL :=  'SELECT DISTINCT M.PLACEID, M.ID, M.NBD, M.SHOW_NBD, M.MAIN_ID, M.BD_DATE, M.BEG_KEEP, M.G082, M.STORE_DOC_NO, M.ADDINFO, M.A_MODE, M.REMARK ' +           &#13;&#10;                     'FROM KRD_MAIN M LEFT JOIN RELEASE R ON ((M.PLACEID = R.PLACEID) AND (M.ID = R.ID)) WHERE PLACEID = ' + IntToStr(vPlaceId) +                                                                                                                                    &#13;&#10;                     ' AND '+&#13;&#10;                     '( '+&#13;&#10;                     '   ( M.BD_DATE &#60;= ' + CHR(39) + VarToStr(dtPeriodEnd) + CHR(39) +&#13;&#10;                     '   AND M.BD_DATE &#62;= ' + CHR(39) + VarToStr(dtPeriodStart) + CHR(39) +&#13;&#10;                     '   ) ' +     &#13;&#10;                     '  OR '+        &#13;&#10;                     '   (R.OUT_DATE &#60;= ' + CHR(39) + VarToStr(dtPeriodEnd) + CHR(39) +&#13;&#10;                     '   AND R.OUT_DATE &#62;= ' + CHR(39) + VarToStr(dtPeriodStart) + CHR(39) +        &#13;&#10;                     '   ) OR (R.RELEASE_NO IS NULL)'+&#13;&#10;                     ') ORDER BY SHOW_NBD';&#13;&#10;         end&#13;&#10;         else  begin&#13;&#10;             vSQL := 'SELECT * FROM KRD_MAIN  WHERE PLACEID = ' + IntToStr(vPlaceId) +                                                                                                                                                             &#13;&#10;                     ' AND BD_DATE &#60;= ' + CHR(39) + VarToStr(dtPeriodEnd) + CHR(39) +&#13;&#10;                     ' AND BD_DATE &#62;= ' + CHR(39) + VarToStr(dtPeriodStart) + CHR(39) +&#13;&#10;                     ' ORDER BY SHOW_NBD';&#13;&#10;         end;                         &#13;&#10;         COMPUTE('OPENQUERY(&#34;QRYKRDMAIN2&#34;, &#34;STS_DB&#34;, &#34;' + vSQL + '&#34;)');&#13;&#10;         sDataSetName := 'QRYKRDMAIN2';&#13;&#10;         //showmessage(COMPUTE('RECORDCOUNT(&#34;QRYKRDMAIN2&#34;)'));&#13;&#10;         COMPUTE('SHOWPROGRESS(&#34;Происходит обработка ДО&#34;)');&#13;&#10;         iRecCount := StrToInt(COMPUTE('RECORDCOUNT(&#34;QRYKRDMAIN2&#34;)'));                                                  &#13;&#10;         Band1.RowCount := StrToInt(COMPUTE('RECORDCOUNT(&#34;QRYKRDMAIN2&#34;)'));&#13;&#10;    end;  &#13;&#10;&#13;&#10;    if rbtnOstatki.checked then begin&#13;&#10;&#13;&#10;          dtOnDate :=  Trunc(dtOn.Date) + STRTOTIME('23:59:59');  &#13;&#10;          vSQL := vSQL + ' SELECT ';                                       &#13;&#10;          vSQL := vSQL + ' M.NBD, M.BD_DATE, M.PLACEID, M.ID, M.G082, M.G022, M.G042,  ';&#13;&#10;          vSQL := vSQL + ' M.SHOW_NBD, M.MAIN_ID, M.BEG_KEEP, M.STORE_DOC_NO, M.ADDINFO, M.A_MODE  ';                     &#13;&#10;          vSQL := vSQL + 'FROM  ';&#13;&#10;          vSQL := vSQL + ' KRD_MAIN M ';&#13;&#10;          //vSQL := vSQL + ' LEFT JOIN KRD_COMM C ON ((M.PLACEID = C.PLACEID) AND (M.ID = C.ID)) ';                                                              &#13;&#10;          vSQL := vSQL + 'WHERE ';&#13;&#10;          vSQL := vSQL + ' (M.PLACEID = ' + IntToStr(vPlaceId) + ') AND ';              &#13;&#10;          vSQL := vSQL + ' (M.BD_DATE &#60;= ' + CHR(39) + VarToStr(dtOnDate) + CHR(39) + ') ';                                                    &#13;&#10;         { &#13;&#10;          vSQL := vSQL + 'GROUP BY ';&#13;&#10;          vSQL := vSQL + ' PLACEID, ID, NBD, BD_DATE, G082, G022, G042, SHOW_NBD, MAIN_ID, BEG_KEEP, STORE_DOC_NO, A_MODE, ADDINFO '; &#13;&#10;          vSQL := vSQL + 'HAVING  ( ';                           &#13;&#10;          vSQL := vSQL + ' ((SELECT MAX(RELEASE_NO) FROM RELEASE R WHERE (R.PLACEID = M.PLACEID) AND (M.ID = R.ID) AND (R.OUT_DATE &#60;= ' + CHR(39) + VarToStr(dtOnDate) + CHR(39) + ')) is NULL)';&#13;&#10;          vSQL := vSQL + ' OR( ';                          &#13;&#10;          vSQL := vSQL + '   ( SUM(C.G35) &#60;&#62; (SELECT SUM(RELEASE_G35) FROM REL_COMM RC, RELEASE R WHERE  ';&#13;&#10;          vSQL := vSQL + '                    (R.PLACEID=RC.PLACEID) AND (R.ID=RC.ID) AND (R.COUNTER = RC.COUNTER) ';                                                                                                                                                                                                                                                                      &#13;&#10;          vSQL := vSQL + '                    AND (M.PLACEID = RC.PLACEID) AND (M.ID = RC.ID)AND (R.OUT_DATE &#60;= ' + CHR(39) + VarToStr(dtOnDate) + CHR(39) + ')) )';                                        &#13;&#10;          vSQL := vSQL + '   OR ';&#13;&#10;          vSQL := vSQL + '   ( SUM(C.G311) &#60;&#62; (SELECT SUM(RELEASE_G311) FROM REL_COMM RC, RELEASE R WHERE  ';      &#13;&#10;          vSQL := vSQL + '                    (R.PLACEID=RC.PLACEID) AND (R.ID=RC.ID) AND (R.COUNTER = RC.COUNTER) ';                                                                                                                                                                                                                                                                      &#13;&#10;          vSQL := vSQL + '                    AND (M.PLACEID = RC.PLACEID) AND (M.ID = RC.ID)AND (R.OUT_DATE &#60;= ' + CHR(39) + VarToStr(dtOnDate) + CHR(39) + ')) )';            &#13;&#10;          vSQL := vSQL + '  ) ';  //OR&#13;&#10;          vSQL := vSQL + ') '; //Having &#13;&#10;          } //Убранно из-зи нехватки у пользователя оперативки на выполнение запроса                                                                                                                                                       &#13;&#10;          vSQL := vSQL + 'ORDER BY BD_DATE, SHOW_NBD ';&#13;&#10;          //showmessage(vSQL);&#13;&#10;&#13;&#10;         COMPUTE('OPENQUERY(&#34;QRYKRDMAIN2&#34;, &#34;STS_DB&#34;, &#34;' + vSQL + '&#34;)');&#13;&#10;         COMPUTE('SHOWPROGRESS(&#34;Происходит обработка ДО&#34;)');&#13;&#10;         iRecCount := StrToInt(COMPUTE('RECORDCOUNT(&#34;QRYKRDMAIN2&#34;)'));             &#13;&#10;         sDataSetName := 'QRYKRDMAIN2';                                         &#13;&#10;         Band1.RowCount := StrToInt(COMPUTE('RECORDCOUNT(&#34;QRYKRDMAIN2&#34;)'));              &#13;&#10;        &#13;&#10;    end;        &#13;&#10;    &#13;&#10;    if rbtnCurBook.checked then begin&#13;&#10;      dtPeriodStart := STRTODATE('30.08.1981') + STRTOTIME('00:00:01');&#13;&#10;      dtPeriodEnd := Date + STRTOTIME('23:59:59');&#13;&#10;      vPlaceId := &#60;KRD_MAIN.&#34;PLACEID&#34;&#62;;&#13;&#10;      sDataSetName := 'KRD_MAIN';&#13;&#10;      Band1.DataSetName := 'KRD_MAIN';&#13;&#10;    end;&#13;&#10;&#13;&#10;&#13;&#10;    //Чтобы взять информацию о складе&#13;&#10;    COMPUTE('FINDKEY(&#34;STORES_2&#34;, [&#34;' + IntToStr(vPlaceId) + '&#34;])');&#13;&#10;&#13;&#10;    //сохраняем настройки&#13;&#10;&#13;&#10;    COMPUTE('WRITEINIFILE(&#34;Print&#34;,&#34;OnCurrentBook&#34;,&#34;'+VarToStr(rbtnCurBook.Checked)+'&#34;)');&#13;&#10;    COMPUTE('WRITEINIFILE(&#34;Print&#34;,&#34;Ostatki&#34;,&#34;'+VarToStr(rbtnOstatki.Checked)+'&#34;)');        &#13;&#10;&#13;&#10;    COMPUTE('WRITEINIFILE(&#34;Print&#34;,&#34;DO1orDO2&#34;,&#34;'+VarToStr(cbxDO1orDO2.Checked)+'&#34;)');&#13;&#10;    COMPUTE('WRITEINIFILE(&#34;Print&#34;,&#34;PeriodStart&#34;,&#34;'+DATETOSTR(dtFrom.Date)+'&#34;)');&#13;&#10;    COMPUTE('WRITEINIFILE(&#34;Print&#34;,&#34;PeriodEnd&#34;,&#34;'+DateToStr(dtTo.Date)+'&#34;)');&#13;&#10;    COMPUTE('WRITEINIFILE(&#34;Print&#34;,&#34;PlaceId&#34;,&#34;'+IntToStr(vPlaceId)+'&#34;)');&#13;&#10;  end&#13;&#10;end;&#13;&#10;&#13;&#10;&#13;&#10;&#13;&#10;&#13;&#10;&#13;&#10;procedure Page2OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  COMPUTE('FIRST(&#34;' + sDataSetName + '&#34;)');&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Band1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  COMPUTE('CANCELRANGE(&#34;KRD_COMM_2&#34;)');&#13;&#10;  COMPUTE('SETRANGE(&#34;KRD_COMM_2&#34;, [&#34;' + VarToStr(COMPUTE('FIELDVALUE(&#34;' + sDataSetName + '&#34;, &#34;PLACEID&#34;)')) + '&#34;, &#34;' +&#13;&#10;                                        VarToStr(COMPUTE('FIELDVALUE(&#34;' + sDataSetName + '&#34;, &#34;ID&#34;)')) + '&#34;])');&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Footer1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin                                   &#13;&#10; if not vOnCurrentBook then begin&#13;&#10;   i := i + 1;&#13;&#10;   COMPUTE('SETPROGRESS (' + STR(i) + ', 100, '+ STR(iRecCount) +')');                         &#13;&#10;   COMPUTE('NEXT(&#34;QRYKRDMAIN2&#34;)');&#13;&#10; end;             &#13;&#10;end;  &#13;&#10;&#13;&#10;procedure FirstTovar(vUsl : integer);&#13;&#10;begin&#13;&#10;    IF &#60;KRD_COMM_2.&#34;G32&#34;&#62; &#62; vUsl then &#13;&#10;      begin                                                                                                   &#13;&#10;        Memo6.Visible := False;&#13;&#10;        Memo17.Visible := False;           &#13;&#10;        Memo20.Visible := False;&#13;&#10;        vContno := &#34;&#34;;&#13;&#10;        //vContComment := '';&#13;&#10;        vComment := '';            &#13;&#10;      end            &#13;&#10;      else begin&#13;&#10;        if rbtnOstatki.checked then begin&#13;&#10;           COMPUTE('LOCATE(&#34;KRD_MAIN_2&#34;, &#34;PLACEID; ID&#34;, ['+VarToStr(&#60;KRD_COMM_2.&#34;PLACEID&#34;&#62;)+','+VarToStr(&#60;KRD_COMM_2.&#34;ID&#34;&#62;)+'])');                                                                  &#13;&#10;           {&#13;&#10;           COMPUTE('OPENQUERY(&#34;REMARK&#34;, &#34;STS_DB&#34;, &#34; SELECT REMARK FROM KRD_MAIN ' +&#13;&#10;                                                    ' WHERE PLACEID = ' + VarToStr(&#60;KRD_COMM_2.&#34;PLACEID&#34;&#62;) +&#13;&#10;                                                    ' AND ID = ' + VarToStr(&#60;KRD_COMM_2.&#34;ID&#34;&#62;) +&#13;&#10;                                                    ' &#34;)');&#13;&#10;           }             &#13;&#10;           vRemark :=  &#60;KRD_MAIN_2.&#34;REMARK&#34;&#62;;&#13;&#10;           vComment := &#60;KRD_MAIN_2.&#34;G531&#34;&#62;;                                                                          &#13;&#10;           //COMPUTE('CLOSEDATASET(&#34;REMARK&#34;)');  &#13;&#10;        end&#13;&#10;        else                  &#13;&#10;          vRemark := COMPUTE('FIELDVALUE(&#34;' + sDataSetName + '&#34;, &#34;Remark&#34;)');&#13;&#10;        &#13;&#10;        vG082 := VarToStr(COMPUTE('FIELDVALUE(&#34;' + sDataSetName + '&#34;, &#34;G082&#34;)'));&#13;&#10;        COMPUTE('OPENQUERY(&#34;CONT&#34;, &#34;STS_DB&#34;, &#34;SELECT CONTNO, SVHCOMMENT FROM KRD_CONT WHERE PLACEID = '+ VarToStr(&#60;KRD_COMM_2.&#34;PLACEID&#34;&#62;) +' AND ID = ' + VarToStr(&#60;KRD_COMM_2.&#34;ID&#34;&#62;) + '&#34;)');&#13;&#10;        COMPUTE('FIRST(&#34;CONT&#34;)');&#13;&#10;        //vContComment := COMPUTE('FIELDVALUE(&#34;CONT&#34;, &#34;SVHCOMMENT&#34;)');                                      &#13;&#10;        vContno :=  COMPUTE('UNIONVALUES(&#34;CONT&#34;, [&#34;CONTNO&#34;], &#34;, &#34;, &#34; : &#34;)');&#13;&#10;        Memo6.Visible := True;&#13;&#10;        Memo17.Visible := True;         &#13;&#10;        Memo20.Visible := True;&#13;&#10;      end;  &#13;&#10;end;&#13;&#10;  &#13;&#10;procedure DetailData1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  if COMPUTE('FIELDVALUE(&#34;' + sDataSetName + '&#34;, &#34;A_MODE&#34;)') = '7' then begin&#13;&#10;    //Запомним ID для установки номера текущей строки&#13;&#10;    vID := StrToInt(COMPUTE('FIELDVALUE(&#34;' + sDataSetName + '&#34;, &#34;MAIN_ID&#34;)'));&#13;&#10;  end&#13;&#10;  else begin&#13;&#10;    //Запомним ID для установки номера текущей строки&#13;&#10;    vID := StrToInt(COMPUTE('FIELDVALUE(&#34;' + sDataSetName + '&#34;, &#34;ID&#34;)'));&#13;&#10;  end;&#13;&#10;                                            &#13;&#10;  TransDocName := &#34;&#34;;&#13;&#10;  TransDocNo := &#34;&#34;; &#13;&#10;  TransDocDate := &#34;&#34;;&#13;&#10;  //Подсчет остатков; информация по документам выпуска&#13;&#10;  dQtyAtStoreG311:= 0;&#13;&#10;  vQtyAtStoreG311 := '';                      &#13;&#10;  sRelAtPeriodG311 := '';  &#13;&#10;  vOutDate := '';&#13;&#10;  vOutTime := '';                                    &#13;&#10;  vGTD :='';&#13;&#10;  vDTType := '';&#13;&#10;  vDTDate := '';                     &#13;&#10;  vContNO := '';&#13;&#10;  vContComment := '';                      &#13;&#10;  vStoreDocNo := '';               &#13;&#10;  vAddInfo := '';&#13;&#10;  vRemark := '';                      &#13;&#10;    &#13;&#10;  vStoreDocNo :=  COMPUTE('FIELDVALUE(&#34;' + sDataSetName + '&#34;, &#34;STORE_DOC_NO&#34;)');&#13;&#10;  vAddInfo := COMPUTE('FIELDVALUE(&#34;' + sDataSetName + '&#34;, &#34;ADDINFO&#34;)');&#13;&#10;                                &#13;&#10;  if rbtnOstatki.checked then begin&#13;&#10;    //If Engine.FinalPass then begin&#13;&#10;      //showmessage(1);                                                 &#13;&#10;      IF &#60;KRD_COMM_2.&#34;G32&#34;&#62; = 1 then begin&#13;&#10;        vFirstPodhTov := 0;&#13;&#10;      end;&#13;&#10;      ReleaseAtPeriod(&#60;KRD_COMM_2.&#34;PLACEID&#34;&#62;,&#60;KRD_COMM_2.&#34;ID&#34;&#62;,&#60;KRD_COMM_2.&#34;G32&#34;&#62;,StrToDate('01.01.1900'),dtOnDate,vG311_OUT,vG35_OUT,vG42_OUT,dtLastDateOut);  &#13;&#10;      {COMPUTE('OPENQUERY(&#34;RELEASEATPERIOD&#34;, &#34;STS_DB&#34;, &#34; SELECT SUM(RC2.RELEASE_G35) as RelG35, SUM(RC2.RELEASE_G311) as RelG311 FROM REL_COMM RC2, RELEASE R2 '+&#13;&#10;                                             ' WHERE '+&#13;&#10;                                             ' (R2.PLACEID=RC2.PLACEID) AND (R2.ID=RC2.ID) AND (R2.COUNTER = RC2.COUNTER)' +&#13;&#10;                                             '  AND (RC2.PLACEID = '+ VarToStr(&#60;KRD_COMM_2.&#34;PLACEID&#34;&#62;) +&#13;&#10;                                             ') AND (RC2.ID = ' + VarToStr(&#60;KRD_COMM_2.&#34;ID&#34;&#62;) + &#13;&#10;                                             ') AND (RC2.G32 = ' + VarToStr(&#60;KRD_COMM_2.&#34;G32&#34;&#62;) +                                                                              &#13;&#10;                                             ') AND (R2.OUT_DATE &#60;= ' + CHR(39) + VarToStr(dtOnDate) + CHR(39) + &#13;&#10;                                             ')&#34;)');}  &#13;&#10;      if ((&#60;KRD_COMM_2.&#34;G35&#34;&#62; - vG35_OUT)&#62;0) or ((&#60;KRD_COMM_2.&#34;G311&#34;&#62; - vG311_OUT)&#62;0) then  begin&#13;&#10;        if vFirstPodhTov = 0 then vFirstPodhTov := &#60;KRD_COMM_2.&#34;G32&#34;&#62;;                                                                                                                                                                                                                                                 &#13;&#10;        DetailData1.Visible := True;&#13;&#10;        FirstTovar(vFirstPodhTov);&#13;&#10;        iLine := iLine + 1;            &#13;&#10;      end            &#13;&#10;      else&#13;&#10;        DetailData1.Visible := False;&#13;&#10;     //end&#13;&#10;     //else&#13;&#10;       //DetailData1.Visible := False;                  &#13;&#10;  end&#13;&#10;  else&#13;&#10;    FirstTovar(1);&#13;&#10; &#13;&#10;  if rbtnPeriod.checked then   &#13;&#10;    if cbxDO1orDO2.checked then&#13;&#10;       if (COMPUTE('FIELDVALUE(&#34;' + sDataSetName + '&#34;, &#34;BEG_KEEP&#34;)') &#60;= dtPeriodEnd) AND  (COMPUTE('FIELDVALUE(&#34;' + sDataSetName + '&#34;, &#34;BEG_KEEP&#34;)') &#62;= dtPeriodStart) then&#13;&#10;         begin                 &#13;&#10;           DetailData1.Visible := True;&#13;&#10;           iLine := iLine + 1;&#13;&#10;         end                  &#13;&#10;       else DetailData1.Visible := False&#13;&#10;     else  iLine := iLine + 1;                      &#13;&#10;  &#13;&#10;  &#13;&#10;  if rbtnCurBook.checked then iLine := iLine + 1;&#13;&#10;&#13;&#10;  if (DetailData1.Visible = True) or cbxDO1orDO2.checked then begin&#13;&#10;    &#13;&#10;      dQtyAtStoreG311 := IIF(&#60;KRD_COMM_2.&#34;G311&#34;&#62;&#60;&#62;NULL,&#60;KRD_COMM_2.&#34;G311&#34;&#62;,0);&#13;&#10;      If &#60;KRD_COMM_2.&#34;G311&#34;&#62; = NULL then vQtyAtStoreG311 := 'часть места'&#13;&#10;        else vQtyAtStoreG311 := STR(dQtyAtStoreG311);        &#13;&#10;      // номера ТТН&#13;&#10;      COMPUTE('OPENQUERY(&#34;PAPER&#34;, &#34;STS_DB&#34;, &#34; SELECT KP.PAPERNO, KP.PAPERNAME, KP.PAPERDATE FROM KR_PAPER KP, KR_C_P KCP, PAPERS P '+&#13;&#10;                                             ' WHERE '+&#13;&#10;                                             ' (KP.PLACEID = KCP.PLACEID) AND (KP.ID = KCP.ID) AND (KCP.DOC_COUNTER = KP.COUNTER)' +&#13;&#10;                                             ' AND (KP.PAPERNAME = P.PAPERNAME) ' +                                                 &#13;&#10;                                             ' AND (KCP.PLACEID= ' +STR(&#60;KRD_COMM_2.&#34;PLACEID&#34;&#62;)+ ') AND (KCP.ID= ' +STR(&#60;KRD_COMM_2.&#34;ID&#34;&#62;)+ ')'+           &#13;&#10;                                             ' AND (KCP.G32= ' + STR(&#60;KRD_COMM_2.&#34;G32&#34;&#62;) + ')'+                                                                                                &#13;&#10;                                             ' AND (P.PAPER_IS_WAYBILL = 1) &#34;)');&#13;&#10;      COMPUTE('FIRST(&#34;PAPER&#34;)');                                                           &#13;&#10;      TransDocNo := Compute('FIELDVALUE(&#34;PAPER&#34;, &#34;PAPERNO&#34;)');&#13;&#10;      TransDocName := Compute('FIELDVALUE(&#34;PAPER&#34;, &#34;PAPERNAME&#34;)');&#13;&#10;      TransDocDate := IIF( Compute('FIELDVALUE(&#34;PAPER&#34;, &#34;PAPERDATE&#34;)') &#60;&#62; '0:00:00', Compute('FIELDVALUE(&#34;PAPER&#34;, &#34;PAPERDATE&#34;)'),'');&#13;&#10;      COMPUTE('CLOSEDATASET(&#34;PAPER&#34;)');      &#13;&#10;                                                              &#13;&#10;                                  &#13;&#10;       COMPUTE('SETFILTER(&#34;REL_COMM_2&#34;, &#34;&#34;)');&#13;&#10;       vFilterCrit := 'PLACEID=' + IntToStr(&#60;KRD_COMM_2.&#34;PLACEID&#34;&#62;) + ' AND ID=' + IntToStr(&#60;KRD_COMM_2.&#34;ID&#34;&#62;) + ' AND G32=' + IntToStr(&#60;KRD_COMM_2.&#34;G32&#34;&#62;);&#13;&#10;       COMPUTE('SETFILTER(&#34;REL_COMM_2&#34;, &#34;' + vFilterCrit + '&#34;)');&#13;&#10;       COMPUTE('FIRST(&#34;REL_COMM_2&#34;)');                    &#13;&#10;       if COMPUTE('RECORDCOUNT(&#34;REL_COMM_2&#34;)') &#60;&#62; 0 then FindDO2();   &#13;&#10;       COMPUTE('SETFILTER(&#34;REL_COMM_2&#34;, &#34;&#34;)');  &#13;&#10;  end;// if DetailData1.Visible = True then begin                         &#13;&#10;end;&#13;&#10;&#13;&#10;procedure FindDO2();&#13;&#10;begin&#13;&#10;    while COMPUTE('EOF(&#34;REL_COMM_2&#34;)') = '0' do begin&#13;&#10;      if COMPUTE('FINDKEY(&#34;REL_MAIN_2&#34;, [&#34;' + IntToStr(&#60;REL_COMM_2.&#34;PLACEID&#34;&#62;) + '&#34;, &#34;' + IntToStr(&#60;REL_COMM_2.&#34;ID&#34;&#62;) + '&#34;, &#34;' + IntToStr(&#60;REL_COMM_2.&#34;COUNTER&#34;&#62;) + '&#34;])') &#60;&#62; '0' then begin&#13;&#10;        &#13;&#10;        if rbtnPeriod.checked then&#13;&#10;          if cbxDO1orDO2.checked then begin&#13;&#10;            if (&#60;REL_MAIN_2.&#34;OUT_DATE&#34;&#62;&#60;&#62;NULL) and (&#60;REL_MAIN_2.&#34;OUT_DATE&#34;&#62; &#60;= dtPeriodEnd) and (&#60;REL_MAIN_2.&#34;OUT_DATE&#34;&#62; &#62;= dtPeriodStart)then begin                                                                           &#13;&#10;                WriteDO2();                                                                                                                         &#13;&#10;                IF DetailData1.Visible = False then&#13;&#10;                begin                          &#13;&#10;                  DetailData1.Visible := True;&#13;&#10;                  iLine := iLine + 1;&#13;&#10;                end;                        &#13;&#10;            end;                        &#13;&#10;          end&#13;&#10;        else&#13;&#10;          if (&#60;REL_MAIN_2.&#34;OUT_DATE&#34;&#62;&#60;&#62;NULL) then&#13;&#10;            WriteDO2();                                   &#13;&#10;          &#13;&#10;        if rbtnOstatki.checked then                               &#13;&#10;            if (&#60;REL_MAIN_2.&#34;OUT_DATE&#34;&#62;&#60;&#62;NULL) and (&#60;REL_MAIN_2.&#34;OUT_DATE&#34;&#62; &#60;= dtOnDate) then&#13;&#10;               WriteDO2();                         &#13;&#10;                &#13;&#10;&#13;&#10;        if rbtnCurBook.checked then                &#13;&#10;            if (&#60;REL_MAIN_2.&#34;OUT_DATE&#34;&#62;&#60;&#62;NULL) then&#13;&#10;               WriteDO2();&#13;&#10;                 &#13;&#10;      end;&#13;&#10;      COMPUTE('NEXT(&#34;REL_COMM_2&#34;)');&#13;&#10;    end;&#13;&#10;    COMPUTE('SETFILTER(&#34;REL_MAIN_2&#34;, &#34;&#34;)');    &#13;&#10;end;&#13;&#10;&#13;&#10;Procedure WriteDO2();&#13;&#10;begin&#13;&#10;    if vOutDate &#60;&#62; '' then        &#13;&#10;        vOutDate := vOutDate + CHR(13);&#13;&#10;    if vOutTime &#60;&#62; '' then           &#13;&#10;        vOutTime := vOutTime + CHR(13); &#13;&#10;    vOutDate := vOutDate + FormatDateTime('DD.MM.YYYY', IIF(&#60;REL_COMM_2.&#34;RELEASE_OUT_DATE&#34;&#62; &#60;&#62; NULL, &#60;REL_COMM_2.&#34;RELEASE_OUT_DATE&#34;&#62;, &#60;REL_MAIN_2.&#34;OUT_DATE&#34;&#62;));&#13;&#10;    vOutTime := vOutTime + FormatDateTime('HH:NN', IIF(&#60;REL_COMM_2.&#34;RELEASE_OUT_DATE&#34;&#62; &#60;&#62; NULL, &#60;REL_COMM_2.&#34;RELEASE_OUT_DATE&#34;&#62;, &#60;REL_MAIN_2.&#34;OUT_DATE&#34;&#62;));&#13;&#10;    if vOutDate = '' then  vOutTime := '';            &#13;&#10;                    &#13;&#10;    if vGTD &#60;&#62; '' then vGTD := vGTD + CHR(13);&#13;&#10;    vGTD := vGTD + &#60;REL_MAIN_2.&#34;DOC_NO&#34;&#62;;&#13;&#10;    if vDTType &#60;&#62; '' then vDTType := vDTType + CHR(13);                                                     &#13;&#10;      vDTType := vDTType + &#60;REL_MAIN_2.&#34;DOC_TYPE&#34;&#62;;                  &#13;&#10;    if vDTDate &#60;&#62; '' then vDTDate := vDTDate + CHR(13);&#13;&#10;      vDTDate := vDTDate + FormatDateTime('DD.MM.YYYY', &#60;REL_MAIN_2.&#34;RELEASE_DATE&#34;&#62;);                                                          &#13;&#10;    if sRelAtPeriodG311 &#60;&#62; '' then sRelAtPeriodG311 := sRelAtPeriodG311 + CHR(13);&#13;&#10;    sRelAtPeriodG311 := sRelAtPeriodG311 + IIF(&#60;REL_COMM_2.&#34;RELEASE_G311&#34;&#62;&#60;&#62;Null, VarToStr(&#60;REL_COMM_2.&#34;RELEASE_G311&#34;&#62;), 'часть места'); &#13;&#10;    //Считаем суммы&#13;&#10;    dSumRelAtPeriodG311 := dSumRelAtPeriodG311 + IIF(&#60;REL_COMM_2.&#34;RELEASE_G311&#34;&#62;&#60;&#62;Null, &#60;REL_COMM_2.&#34;RELEASE_G311&#34;&#62;, 0);  &#13;&#10;end;&#13;&#10;&#13;&#10;  &#13;&#10;procedure Band3OnAfterPrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  COMPUTE('CANCELRANGE(&#34;KRD_COMM_2&#34;)');&#13;&#10;  COMPUTE('CANCELRANGE(&#34;KRD_COMM_PAPERS_2&#34;)');&#13;&#10;  COMPUTE('CANCELRANGE(&#34;KRD_PAPERS_2&#34;)');&#13;&#10;  if not vOnCurrentBook then begin &#13;&#10;    COMPUTE('CLOSEDATASET(&#34;QRYKRDMAIN2&#34;)');&#13;&#10;    COMPUTE('HIDEPROGRESS()');                                 &#13;&#10;  end;&#13;&#10;end;  &#13;&#10;&#13;&#10;&#13;&#10;&#13;&#10;&#13;&#10;&#13;&#10;begin          &#13;&#10; iLine := 0;                        &#13;&#10;end." OnStartReport="OnStartReport" PropData="">
