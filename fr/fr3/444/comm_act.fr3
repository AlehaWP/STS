<TfrxReport Version="5.3.1" DotMatrixReport="False" EngineOptions.ConvertNulls="False" IniFile="\Software\Fast Reports" PreviewOptions.Buttons="4095" PreviewOptions.Zoom="1" PrintOptions.Printer="По умолчанию" PrintOptions.PrintOnSheet="0" ReportOptions.Author="ООО &#34;СТМ&#34;" ReportOptions.CreateDate="41710.4919254282" ReportOptions.Description.Text="" ReportOptions.Name="Коммерческий акт (Пр.№ 2688 от 29.12.2012)" ReportOptions.LastChange="43894.5552785532" ScriptLanguage="PascalScript" ScriptText.Text="Var&#13;&#10;  vActNo, vActOrderNo, vSVHName, vSVHDate, vSVHAddress, vSVHInfo, vSVHPerson, vTypeSVH, vTypeDoc : String;&#13;&#10;  vStage, vActDate, vActTime, vReasons, vGoodReason, vG42CURR : String;                                      &#13;&#10;  vGoodNo, vTTN, vTTNDate, vTTNCode, sSQL : String;                                                                                  &#13;&#10;  vAddress1, vAddress11, vAddress2, vAddress22 : String;&#13;&#10;  vAcceptDate, vAcceptTime, vCreateDate, vPaperCounter, vPaperCode, vPolPerson, vPerPerson: String;&#13;&#10;  vActDateTime : TDateTime;                                      &#13;&#10;&#13;&#10;procedure MasterData3OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  IF VarToStr(&#60;q3.&#34;FACT_G42_CURRENCY&#34;&#62;) &#60;&#62; '' then&#13;&#10;    vG42CURR := COMPUTE ('CURRENCYABBR (&#34;' + &#60;q3.&#34;FACT_G42_CURRENCY&#34;&#62; + '&#34;)')&#13;&#10;  else vG42CURR := COMPUTE ('CURRENCYABBR (&#34;' + &#60;q3.&#34;G42_CURRENCY&#34;&#62; + '&#34;)');                   &#13;&#10;  &#13;&#10;  vAcceptDate := FormatDateTime ('DD.MM.YYYY', &#60;q3.&#34;ACCEPTDATE&#34;&#62;);&#13;&#10;  vAcceptTime := FormatDateTime ('HH:MM:SS', &#60;q3.&#34;ACCEPTDATE&#34;&#62;);  &#13;&#10;end;&#13;&#10;&#13;&#10;procedure Header3OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  If vPaperCounter &#60;&#62; '' then begin&#13;&#10;    sSQL := 'SELECT * FROM KR_PAPER WHERE PLACEID=' + VarToStr(&#60;KRD_DOP.&#34;PLACEID&#34;&#62;) + &#13;&#10;            ' AND ID=' + VarToStr(&#60;KRD_DOP.&#34;ID&#34;&#62;) + &#13;&#10;            ' AND COUNTER=(SELECT DOC_COUNTER FROM KR_C_P WHERE PLACEID=' +  VarToStr(&#60;KRD_DOP.&#34;PLACEID&#34;&#62;) + &#13;&#10;            ' AND ID=' + VarToStr(&#60;KRD_DOP.&#34;ID&#34;&#62;) +&#13;&#10;            ' AND DOC_TYPE=13 AND G32=' + VarToStr(&#60;q3.&#34;G32&#34;&#62;) + ')';&#13;&#10;    COMPUTE ('OPENQUERY (&#34;qryPapers&#34;, &#34;STS_DB&#34;, &#34;' + sSQL + '&#34;)');&#13;&#10;    vTTN := COMPUTE ('REFERENCE (&#34;PAPERS&#34;, &#34;PAPERNAME&#34;, &#34;' + COMPUTE ('FIELDVALUE (&#34;qryPapers&#34;, &#34;PAPERNAME&#34;)') + '&#34;, &#34;PAPERFULLNAME&#34;)') + ' № ' + COMPUTE ('FIELDVALUE (&#34;qryPapers&#34;, &#34;PAPERNO&#34;)');                                                                                                     &#13;&#10;    vTTNDAte := COMPUTE ('FIELDVALUE (&#34;qryPapers&#34;, &#34;PAPERDATE&#34;)');&#13;&#10;    vTTNCode := COMPUTE ('FIELDVALUE (&#34;qryPapers&#34;, &#34;PAPERCODE&#34;)');    &#13;&#10;  end;  &#13;&#10;end;    &#13;&#10;  &#13;&#10;begin&#13;&#10;if KRD_DOP.RECORDCOUNT &#62; 0 then&#13;&#10;begin            &#13;&#10;  vActNo := &#60;KRD_DOP.&#34;DOC_NO&#34;&#62;;&#13;&#10;  vActOrderNo := &#60;KRD_DOP.&#34;DOC_ORDER_NUMBER&#34;&#62;;&#13;&#10;&#13;&#10;  COMPUTE ('LOCATE (&#34;STORES_2&#34;, &#34;PLACEID&#34;, ' + VarToStr(&#60;KRD_DOP.&#34;PLACEID&#34;&#62;) + ')');                                                                      &#13;&#10;  vSVHName := &#60;STORES_2.&#34;NAME&#34;&#62;;&#13;&#10;  vSVHDate := '';&#13;&#10;  vSVHAddress := &#60;STORES_2.&#34;ADDRESS&#34;&#62;;&#13;&#10;    &#13;&#10;  case &#60;STORES_2.&#34;STORE_TYPE&#34;&#62; of&#13;&#10;    '', 'СВХ', Null: begin&#13;&#10;      vTypeSVH := 'СВХ';&#13;&#10;      vTypeDoc := 'Свидетельство о включении в реестр владельцев СВХ';&#13;&#10;    end;&#13;&#10;    'СП': begin&#13;&#10;      vTypeSVH := 'Склад получателя товаров';&#13;&#10;      vTypeDoc := 'Разрешение на временное хранение на складе получателя товаров';&#13;&#10;    end;&#13;&#10;    'ПЗТК': begin&#13;&#10;      vTypeSVH := 'ПЗТК';&#13;&#10;      vTypeDoc := 'Приказ(разрешение) таможни';&#13;&#10;    end;&#13;&#10;    'ВЗТК': begin&#13;&#10;      vTypeSVH := 'ВЗТК ';&#13;&#10;      vTypeDoc := 'Приказ(разрешение) таможни';            &#13;&#10;    end;                &#13;&#10;    'ЗТК': begin&#13;&#10;      vTypeSVH := 'ЗТК';&#13;&#10;      vTypeDoc := 'Приказ(разрешение) таможни';&#13;&#10;    end;&#13;&#10;    'СВХУТ': begin&#13;&#10;      vTypeSVH := 'СВХ';&#13;&#10;      vTypeDoc := 'Свидетельство о включении в реестр владельцев СВХ';&#13;&#10;    end;&#13;&#10;  end;&#13;&#10;    &#13;&#10;  vSVHInfo := vTypeDoc + ' № ' + &#60;STORES_2.&#34;LICENCENO&#34;&#62; + ' от ' + FormatDateTime('DD.MM.YYYY', &#60;STORES_2.&#34;LICENCEDATE&#34;&#62;);&#13;&#10;  vSVHPerson := &#60;KRD_DOP.&#34;WAREHOUSEPERSON_POST&#34;&#62; + ' ' + &#60;KRD_DOP.&#34;WAREHOUSEPERSON_SURNAME&#34;&#62; + ' ' + &#60;KRD_DOP.&#34;WAREHOUSEPERSON_NAME&#34;&#62; + ' ' + &#60;KRD_DOP.&#34;WAREHOUSEPERSON_MDLNAME&#34;&#62;;&#13;&#10;        &#13;&#10;  COMPUTE ('OPENQUERY (&#34;qRC&#34;, &#34;STS_DB&#34;, &#34;SELECT SDM_STAGE, SDM_DATETIME FROM KRD_CSDM WHERE PLACEID=' +VarToStr(&#60;KRD_DOP.&#34;PLACEID&#34;&#62;)+ ' AND ID=' +VarToStr(&#60;KRD_DOP.&#34;ID&#34;&#62;)+ ' AND COUNTER=' +VarToStr(&#60;KRD_DOP.&#34;COUNTER&#34;&#62;)+ '&#34;)');                                             &#13;&#10;&#13;&#10;  if Length(&#60;KRD_DOP.&#34;DOC_SDM_DATETIME&#34;&#62;) &#62; 0 then&#13;&#10;    vActDateTime := &#60;KRD_DOP.&#34;DOC_SDM_DATETIME&#34;&#62;&#13;&#10;  else&#13;&#10;    vActDateTime := COMPUTE ('FIELDVALUE (&#34;qRC&#34;, &#34;SDM_DATETIME&#34;)');                                                        &#13;&#10;                                                                                   &#13;&#10;  vActDate := FormatDateTime ('DD.MM.YYYY', vActDateTime);&#13;&#10;  vActTime := FormatDateTime ('HH:NN:SS', vActDateTime);&#13;&#10;    &#13;&#10;  vCreateDate :=  FormatDateTime ('DD.MM.YYYY', &#60;KRD_DOP.&#34;DOC_DATETIME&#34;&#62;);                                      &#13;&#10;  vReasons := ReplaceStr(ReplaceStr(&#60;KRD_DOP.&#34;REASONS&#34;&#62;,Chr(13),''), Chr(10), ' ');  &#13;&#10;                                   &#13;&#10;  if Length(&#60;KRD_DOP.&#34;DOC_SDM_STAGE&#34;&#62;) &#62; 0 then&#13;&#10;    vStage := &#60;KRD_DOP.&#34;DOC_SDM_STAGE&#34;&#62;&#13;&#10;  else                     &#13;&#10;    vStage := COMPUTE ('FIELDVALUE (&#34;qRC&#34;, &#34;SDM_STAGE&#34;)');                             &#13;&#10;      &#13;&#10;  CASE vStage of     &#13;&#10;  0: vStage := 'на этапе принятия товара';&#13;&#10;  1: vStage := 'в процессе временного хранения';&#13;&#10;  end;&#13;&#10;&#13;&#10;  q1.Active := False;&#13;&#10;  q1.SQL.Text := 'SELECT G32, SDM_REASONS FROM KRD_CSDM WHERE SDM_KIND=' +chr(39)+ '1' +chr(39)+ ' AND PLACEID=' + VarToStr(&#60;KRD_DOP.&#34;PLACEID&#34;&#62;) + ' AND ID=' + VarToStr(&#60;KRD_DOP.&#34;ID&#34;&#62;) + ' AND COUNTER=' + VarToStr(&#60;KRD_DOP.&#34;COUNTER&#34;&#62;);&#13;&#10;  q1.Active := True;                                              &#13;&#10;&#13;&#10;  q2.Active := False;&#13;&#10;  q2.SQL.Text := 'SELECT' +&#13;&#10;                 ' G32, DOC_G33, FACT_G33, DOC_G312, FACT_G312, DOC_G311, FACT_G311,' +&#13;&#10;                 ' DOC_G35, FACT_G35, DOC_G42, FACT_G42, SDM_REASONS' +                                                                      &#13;&#10;                 ' FROM KRD_CSDM' +&#13;&#10;                 ' WHERE' +&#13;&#10;                 ' SDM_KIND=' +chr(39)+ '0' +chr(39)+&#13;&#10;                 ' AND COUNTER=' +VarToStr(&#60;KRD_DOP.&#34;COUNTER&#34;&#62;)+ &#13;&#10;                 ' AND PLACEID=' + VarToStr(&#60;KRD_DOP.&#34;PLACEID&#34;&#62;) + &#13;&#10;                 ' AND ID=' + VarToStr(&#60;KRD_DOP.&#34;ID&#34;&#62;);                                                        &#13;&#10;  q2.Active := True;&#13;&#10;&#13;&#10;  q3.Active := False;&#13;&#10;  q3.SQL.Text := 'SELECT C2.G32, C2.DOC_G33, C.G33, C2.DOC_G312, C.G312, C2.DOC_G311, C.G311, C2.DOC_G35, C.G35, C2.DOC_G42, C.G42, C.BOXNO, C2.FACT_G42_CURRENCY, C.G42_CURRENCY, C.STORAGE_TYPE, C.ACCEPTDATE, C.STORAGE_DATE FROM KRD_CSDM C2, KRD_COMM C WHERE SDM_KIND=' +chr(39)+ '2' +chr(39)+ ' AND C.PLACEID=C2.PLACEID AND C.ID=C2.ID AND C.G32=C2.G32 AND C2.COUNTER=' +VarToStr(&#60;KRD_DOP.&#34;COUNTER&#34;&#62;)+ ' AND C2.PLACEID=' + VarToStr(&#60;KRD_DOP.&#34;PLACEID&#34;&#62;) + ' AND C2.ID=' + VarToStr(&#60;KRD_DOP.&#34;ID&#34;&#62;);&#13;&#10;  q3.Active := True;&#13;&#10;&#13;&#10;  vPaperCounter := '';    &#13;&#10;  KRD_PAPERS.FIRST;&#13;&#10;  while KRD_PAPERS.EOF = false do&#13;&#10;    begin                                                       &#13;&#10;      if (COMPUTE ('REFERENCE (&#34;PAPERS&#34;, &#34;PAPER_DOCG44_CODE&#34;, &#34;' + VarToStr(&#60;KRD_PAPERS.&#34;PAPERCODE&#34;&#62;) + '&#34;, &#34;PAPER_IS_WAYBILL&#34;)') = '1') and (vPaperCounter = '')then&#13;&#10;        begin        &#13;&#10;          vPaperCounter := VarToStr(&#60;KRD_PAPERS.&#34;COUNTER&#34;&#62;);&#13;&#10;          vPaperCode := VarToStr(&#60;KRD_PAPERS.&#34;PAPERCODE&#34;&#62;);                                              &#13;&#10;        end;                                                                   &#13;&#10;      KRD_PAPERS.NEXT;                                                                                              &#13;&#10;    end;&#13;&#10;{    &#13;&#10;  If vPaperCounter &#60;&#62; '' then begin&#13;&#10;    sSQL := 'SELECT * FROM KR_PAPER WHERE PLACEID=' + VarToStr(&#60;KRD_DOP.&#34;PLACEID&#34;&#62;) + &#13;&#10;            ' AND ID=' + VarToStr(&#60;KRD_DOP.&#34;ID&#34;&#62;) + &#13;&#10;            ' AND COUNTER=(SELECT DOC_COUNTER FROM KR_C_P WHERE PLACEID=' +  VarToStr(&#60;KRD_DOP.&#34;PLACEID&#34;&#62;) + &#13;&#10;            ' AND ID=' + VarToStr(&#60;KRD_DOP.&#34;ID&#34;&#62;) +&#13;&#10;            ' AND DOC_TYPE=13 AND G32=' + VarToStr(&#60;q3.&#34;G32&#34;&#62;) + ')';&#13;&#10;    COMPUTE ('OPENQUERY (&#34;qryPapers&#34;, &#34;STS_DB&#34;, &#34;' + sSQL + '&#34;)');&#13;&#10;    vTTN := COMPUTE ('REFERENCE (&#34;PAPERS&#34;, &#34;PAPERNAME&#34;, &#34;' + COMPUTE ('FIELDVALUE (&#34;qryPapers&#34;, &#34;PAPERNAME&#34;)') + '&#34;, &#34;PAPERFULLNAME&#34;)') + ' № ' + COMPUTE ('FIELDVALUE (&#34;qryPapers&#34;, &#34;PAPERNO&#34;)');                                                                                                     &#13;&#10;    vTTNDAte := COMPUTE ('FIELDVALUE (&#34;qryPapers&#34;, &#34;PAPERDATE&#34;)');&#13;&#10;    vTTNCode := COMPUTE ('FIELDVALUE (&#34;qryPapers&#34;, &#34;PAPERCODE&#34;)');    &#13;&#10;  end;&#13;&#10;}      &#13;&#10;  vAddress1 := &#60;KRD_MAIN.&#34;G022&#34;&#62;;&#13;&#10;  vAddress11 := &#60;KRD_MAIN.&#34;G023&#34;&#62;;&#13;&#10;  vAddress2 := &#60;KRD_MAIN.&#34;G082&#34;&#62;;&#13;&#10;  vAddress22 := &#60;KRD_MAIN.&#34;G083&#34;&#62;;&#13;&#10;    &#13;&#10;  vPerPerson := &#60;KRD_DOP.&#34;CARRIERPERSON_POST&#34;&#62; + ' ' + &#60;KRD_DOP.&#34;CARRIERPERSON_SURNAME&#34;&#62; + ' ' + &#60;KRD_DOP.&#34;CARRIERPERSON_NAME&#34;&#62; + ' ' + &#60;KRD_DOP.&#34;CARRIERPERSON_MDLNAME&#34;&#62;;&#13;&#10;  vPolPerson := &#60;KRD_DOP.&#34;RECIPIENTPERSON_POST&#34;&#62; + ' ' + &#60;KRD_DOP.&#34;RECIPIENTPERSON_SURNAME&#34;&#62; + ' ' + &#60;KRD_DOP.&#34;RECIPIENTPERSON_NAME&#34;&#62; + ' ' + &#60;KRD_DOP.&#34;RECIPIENTPERSON_MDLNAME&#34;&#62;;&#13;&#10;&#13;&#10;end&#13;&#10;else&#13;&#10;  begin&#13;&#10;    Page1.Visible := false;&#13;&#10;    showmessage ('Отсутствуют коммерческие акты');&#13;&#10;    Report.Terminated := true;  &#13;&#10;  end;            &#13;&#10;end.">
