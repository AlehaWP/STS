<TfrxReport Version="5.3.1" DotMatrixReport="False" EngineOptions.ConvertNulls="False" EngineOptions.DoublePass="True" IniFile="\Software\Fast Reports" PreviewOptions.Buttons="4095" PreviewOptions.Zoom="1" PrintOptions.Printer="По умолчанию" PrintOptions.PrintOnSheet="0" ReportOptions.CreateDate="38266.8187760417" ReportOptions.Description.Text="" ReportOptions.Name="Форма ДО2 (Пр.№ 2688 от 29.12.2012)" ReportOptions.LastChange="42755.7930435301" ReportOptions.VersionMajor="2" ScriptLanguage="PascalScript" ScriptText.Text="var&#13;&#10;  bPartInPlace, bPartOutPlace                                  : Boolean;&#13;&#10;  sPartInPlaceText,sPartOutPlaceText                           : String;  &#13;&#10;  bPrintItog                                                   : Boolean;&#13;&#10;  bPrintRegNum                                                 : Boolean; &#13;&#10;  bShowDialog                                                  : Boolean;&#13;&#10;  bManyParts                                                   : Boolean;        &#13;&#10;&#13;&#10;  sSQLText,sID,sG32, sNBD                                      : String;&#13;&#10;  sTypeSVH,sTypeSVH1,sNameSVH,sTypeDoc,sNumDoc,sDateDoc        : String;&#13;&#10;  sDatPodp,sPodpis                                             : String;&#13;&#10;  sAcceptDate, sGN, sG312, sG33, sStoreNum, sG35,sG311         : String;&#13;&#10;  sValCode, sStorageDate                                       : String;&#13;&#10;  dG35, dG311,dG42,dSumG35, dSumG311, dSumG42                  : Double;&#13;&#10;    &#13;&#10;  sOutDate, sOutG35, sOut_DocNo, sRecepient,sOutG311           : String;&#13;&#10;  dOutG311, dOutG42,dSumOutG35, dSumOutG311, dSumOutG42        : Double;&#13;&#10;&#13;&#10;  sOstG35                                                      : String;                                     &#13;&#10;  dOstG35,dOstG311,dOstG42,dSumOstG35,dSumOstG311,dSumOstG42   : Double;      &#13;&#10;&#13;&#10;            &#13;&#10;&#13;&#10;Const&#13;&#10;  sIniFullPath = ProgramPath() + 'print.ini';&#13;&#10;  sIniSection = 'do2.fr3';&#13;&#10;  sIniSectionCommon = 'Common';&#13;&#10;  sMainCounter = STR(&#60;REL_MAIN.&#34;MAIN_COUNTER&#34;&#62;);&#13;&#10;  sPlaceID = STR(&#60;REL_MAIN.&#34;PLACEID&#34;&#62;);&#13;&#10;  sMainID = STR(&#60;REL_MAIN.&#34;MAIN_ID&#34;&#62;);                                      &#13;&#10;  &#13;&#10;procedure Page1OnShow(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  chkPartInPlace.Checked := bPartInPlace;&#13;&#10;  edPartInPlaceText.Text := sPartInPlaceText;&#13;&#10;  chkPartOutPlace.Checked := bPartOutPlace;&#13;&#10;  edPartOutPlaceText.Text := sPartOutPlaceText;&#13;&#10;&#13;&#10;  chkItog.Checked:= bPrintItog;&#13;&#10;  chkRegNum.Checked := bPrintRegNum; &#13;&#10;end;&#13;&#10;&#13;&#10;procedure btnOkOnClick(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  bPartInPlace := chkPartInPlace.Checked;&#13;&#10;  sPartInPlaceText := edPartInPlaceText.Text;&#13;&#10;  bPartOutPlace := chkPartOutPlace.Checked;&#13;&#10;  sPartOutPlaceText := edPartOutPlaceText.Text;  &#13;&#10;  &#13;&#10;  bPrintItog := chkItog.Checked;&#13;&#10;  bPrintRegNum := chkRegNum.Checked;&#13;&#10;  &#13;&#10;  bShowDialog := chkShow.Checked;&#13;&#10;&#13;&#10;  WriteIniFile(sIniSection, 'PartInPlace', VarToStr(bPartInPlace), sIniFullPath);&#13;&#10;  WriteIniFile(sIniSection, 'PartInPlaceText', sPartInPlaceText, sIniFullPath);&#13;&#10;  WriteIniFile(sIniSection, 'PartOutPlace', VarToStr(bPartOutPlace), sIniFullPath);&#13;&#10;  WriteIniFile(sIniSection, 'PartOutPlaceText', sPartOutPlaceText, sIniFullPath);&#13;&#10;    &#13;&#10;  WriteIniFile(sIniSection, 'PrintItog', VarToStr(bPrintItog), sIniFullPath);     &#13;&#10;  WriteIniFile(sIniSection, 'PrintRegNum', VarToStr(bPrintRegNum), sIniFullPath);&#13;&#10;    &#13;&#10;  WriteIniFile(sIniSection, 'ShowDialog', VarToStr(bShowDialog), sIniFullPath);&#13;&#10;&#13;&#10;       &#13;&#10;end;&#13;&#10;&#13;&#10;procedure Page2OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  IF (&#60;REL_MAIN.&#34;REG_RELEASE_NO&#34;&#62; = NULL) or not bPrintRegNum                                          &#13;&#10;     then begin&#13;&#10;       Memo113.Visible := false;&#13;&#10;       Memo114.Visible := false;&#13;&#10;       Memo117.Visible := false;&#13;&#10;       Memo144.Visible := false;&#13;&#10;       Memo145.Visible := false;&#13;&#10;       Memo146.Visible := false;&#13;&#10;       Shape4.Visible := false;                                                             &#13;&#10;  end;  &#13;&#10;  &#13;&#10;  sSQLText := '';&#13;&#10;&#13;&#10;    &#13;&#10;    &#13;&#10;                                      &#13;&#10;  //Вариант 1  должен быть побыстрее, но проблеммы с сортировкой                                                                                                                                                                            &#13;&#10;  {&#13;&#10;  COMPUTE('OPENQUERY(&#34;GET_REL_ID&#34;, &#34;STS_DB&#34;, &#34;SELECT '+&#13;&#10;                                             ' R.ID, R.COUNTER, R.MAIN_NO, '+       &#13;&#10;                                             'FROM '+&#13;&#10;                                             ' RELEASE R ' +&#13;&#10;                                             ' WHERE '+&#13;&#10;                                             '  R.MAIN_COUNTER = ' + sMainCounter+&#13;&#10;                                             '  AND R.Placeid='+sPlaceid +&#13;&#10;                                             ' AND R.MAIN_ID='+sMainID+                                                                                   &#13;&#10;                                             ' ORDER BY MAIN_NO &#34;)');&#13;&#10;&#13;&#10;           &#13;&#10;  IF COMPUTE('RECORDCOUNT(&#34;GET_REL_ID&#34;)')=0 then RaiseException('ДО2 не найдено, выполнение прекращено');&#13;&#10;  COMPUTE('FIRST(&#34;GET_REL_ID&#34;)');    &#13;&#10;  &#13;&#10;  sSQLText := 'SELECT '+&#13;&#10;                '  RC.PLACEID, RC.ID, RC.COUNTER, RC.G32, '+&#13;&#10;                '  RC.RELEASE_G311, RC.RELEASE_G35, RC.RELEASE_G38, RC.RELEASE_G315, RC.RELEASE_G33, RC.RELEASE_G312,'+&#13;&#10;                '  RC.RELEASE_G315A,  RC.RELEASE_G315C,  RC.RELEASE_G42, RC.RELEASE_BOXNO, RC.RELEASE_OUT_DATE '+&#13;&#10;                'FROM '+&#13;&#10;                '  REL_COMM RC '+&#13;&#10;                'WHERE ' +&#13;&#10;                ' (ID='+COMPUTE('GET_REL_ID.ID')+' AND COUNTER='+COMPUTE('GET_REL_ID.COUNTER')+') ';     &#13;&#10;     &#13;&#10;  COMPUTE('NEXT(&#34;GET_REL_ID&#34;)');      &#13;&#10;  WHILE COMPUTE('EOF(&#34;GET_REL_ID&#34;)')=0 do&#13;&#10;    begin&#13;&#10;       sSQLText := sSQLText + ' OR (ID='+COMPUTE('GET_REL_ID.ID')+' AND COUNTER='+COMPUTE('GET_REL_ID.COUNTER')+') ';&#13;&#10;       COMPUTE('NEXT(&#34;GET_REL_ID&#34;)');           &#13;&#10;    end;&#13;&#10;&#13;&#10;  sSQLText := sSQLText + ' ORDER BY G32';&#13;&#10;&#13;&#10;  COMPUTE('CLOSEDATASET(&#34;GET_REL_ID&#34;)');      &#13;&#10;  }&#13;&#10;  &#13;&#10;  //Вариант 2&#13;&#10;  // showmessage(&#60;REL_MAIN.&#34;MAIN_ID&#34;&#62;);&#13;&#10;&#13;&#10;   //Проверка многопартийности&#13;&#10;   IF (sMainID=NULL) or (sMainID='') then RaiseException('ДО2 не найдено, выполнение прекращено');       &#13;&#10;   COMPUTE('OPENQUERY(&#34;ManyParts&#34;, &#34;STS_DB&#34;, &#34;SELECT MAIN_NO FROM RELEASE'+&#13;&#10;                                             ' WHERE '+                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                       &#13;&#10;                                             ' MAIN_COUNTER = '+ sMainCounter+              &#13;&#10;                                             ' AND Placeid='+ sPlaceid + &#13;&#10;                                             ' AND MAIN_ID='+sMainID+ &#13;&#10;                                             ' AND MAIN_NO &#62; 1'+                                                                                             &#13;&#10;                                             '&#34;)');&#13;&#10;   bManyParts := false;                                                                            &#13;&#10;   IF COMPUTE('RECORDCOUNT(&#34;ManyParts&#34;)')&#62;0 then bManyParts := true;                                                                                                                                  &#13;&#10;  &#13;&#10;   //IF sMainID=NULL then RaiseException('ДО2 не найдено, выполнение прекращено');                                                                 &#13;&#10;     &#13;&#10;   sSQLText :=  'SELECT '+&#13;&#10;                ' RC.PLACEID, RC.ID, RC.COUNTER, RC.G32, '+&#13;&#10;                ' RC.RELEASE_G311, RC.RELEASE_G35, RC.RELEASE_G38, RC.RELEASE_G315, RC.RELEASE_G33, RC.RELEASE_G312,'+&#13;&#10;                ' RC.RELEASE_G315A,  RC.RELEASE_G315C,  RC.RELEASE_G42, RC.RELEASE_BOXNO, RC.RELEASE_OUT_DATE '+&#13;&#10;                IIF(bManyParts, ', R.MAIN_NO, R.DOC_NO, R.DOC_TYPE ', '')+                               &#13;&#10;                'FROM '+&#13;&#10;                ' REL_COMM RC ';&#13;&#10;   IF bManyParts then                                      &#13;&#10;      sSQLText := sSQLText +  ' LEFT JOIN RELEASE R ON(R.PLACEID=RC.PLACEID AND R.ID=RC.ID AND R.COUNTER=RC.COUNTER) '+                                                                                             &#13;&#10;                              'WHERE ' +&#13;&#10;                              ' R.MAIN_COUNTER = ' + sMainCounter+&#13;&#10;                              ' AND R.Placeid='+sPlaceid +&#13;&#10;                              ' AND R.MAIN_ID='+sMainID+' '+         &#13;&#10;                              'ORDER BY MAIN_NO, G32'&#13;&#10;  else&#13;&#10;        sSQLText := sSQLText +'WHERE '+  &#13;&#10;                              ' RC.Placeid='+sPlaceid +&#13;&#10;                              ' AND RC.ID='+sMainID+' '+&#13;&#10;                              ' AND COUNTER ='+STR(&#60;REL_MAIN.&#34;COUNTER&#34;&#62;) +                                                                                                               &#13;&#10;                              'ORDER BY G32';        &#13;&#10;  qryComms.Active := False;                                                          &#13;&#10;  qryComms.SQL.Text := '';&#13;&#10;  qryComms.SQL.Text := sSQLText;&#13;&#10;  qryComms.Active := True;&#13;&#10;&#13;&#10;  dSumG35:=0;dSumG311:=0;dSumG42:=0;      &#13;&#10;  dSumOutG35:=0;dSumOutG311:=0;dSumOutG42:=0;          &#13;&#10;  dSumOstG35:=0;dSumOstG311:=0;dSumOstG42:=0;              &#13;&#10;end;  &#13;&#10;&#13;&#10;procedure ReportSummary1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  ReportSummary1.Visible := bPrintItog;  &#13;&#10;end;&#13;&#10;&#13;&#10;procedure ReportTitle1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;    sTypeSVH := '';&#13;&#10;    sNameSVH := '';&#13;&#10;    sTypeDoc := '';&#13;&#10;    sDateDoc := '';&#13;&#10;    sNumDoc  := '';&#13;&#10;&#13;&#10;    COMPUTE('OPENQUERY(&#34;GET_STORES&#34;, &#34;STS_DB&#34;, &#34;SELECT STORE_TYPE, NAME, LICENCEDATE, LICENCENO FROM STORES WHERE PLACEID='+sPlaceID+'&#34;)');     &#13;&#10;    &#13;&#10;    sTypeSVH := COMPUTE('GET_STORES.STORE_TYPE');      &#13;&#10;    sNameSVH :=COMPUTE('GET_STORES.Name');&#13;&#10;    sDateDoc := COMPUTE('GET_STORES.LICENCEDATE');&#13;&#10;    sNumDoc  := COMPUTE('GET_STORES.LICENCENO');&#13;&#10;&#13;&#10;    case sTypeSVH of&#13;&#10;      '', 'СВХ', Null: begin&#13;&#10;        sTypeSVH := 'СВХ';&#13;&#10;        sTypeSVH1 := 'СВХ';            &#13;&#10;        sTypeDoc := 'Свидетельство о включении в реестр владельцев СВХ от';&#13;&#10;      end;&#13;&#10;      'СП': begin&#13;&#10;        sTypeSVH := 'Склад получателя товаров';&#13;&#10;        sTypeSVH1 := 'склада';            &#13;&#10;        sTypeDoc := 'Разрешение на временное хранение на складе получателя товаров от';&#13;&#10;      end;&#13;&#10;      'ПЗТК': begin&#13;&#10;        sTypeSVH := 'ПЗТК';&#13;&#10;        sTypeSVH1 := 'ПЗТК';            &#13;&#10;        sTypeDoc := 'Приказ(разрешение) таможни от';&#13;&#10;      end;&#13;&#10;      'ВЗТК': begin&#13;&#10;        sTypeSVH := 'ВЗТК';&#13;&#10;        sTypeSVH1 := 'ВЗТК';            &#13;&#10;        sTypeDoc := 'Приказ(разрешение) таможни от';            &#13;&#10;      end;                &#13;&#10;      'ЗТК': begin&#13;&#10;        sTypeSVH := 'ЗТК';&#13;&#10;        sTypeSVH1 := 'ЗТК';            &#13;&#10;        sTypeDoc := 'Приказ(разрешение) таможни от';&#13;&#10;      end;&#13;&#10;      'СВХУТ': begin&#13;&#10;        sTypeSVH := 'СВХ';&#13;&#10;        sTypeSVH1 := 'СВХ';            &#13;&#10;        sTypeDoc := 'Свидетельство о включении в реестр владельцев СВХ от';&#13;&#10;      end;&#13;&#10;      else begin&#13;&#10;        sTypeSVH := &#60;STORES_2.&#34;STORE_TYPE&#34;&#62;;&#13;&#10;        sTypeSVH1 := 'СВХ';            &#13;&#10;        sTypeDoc := 'Свидетельство о включении в реестр владельцев СВХ от';          &#13;&#10;      end                          &#13;&#10;    end;    &#13;&#10;end;&#13;&#10;&#13;&#10;procedure MasterData1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;IF Engine.FinalPass then begin&#13;&#10;                                                                                        &#13;&#10;    if (sG32&#60;&#62;STR(&#60;qryComms.&#34;G32&#34;&#62;)) or (sID&#60;&#62;STR(&#60;qryComms.&#34;ID&#34;&#62;)) then&#13;&#10;    begin&#13;&#10;        MasterData1.Visible := true;                                                                              &#13;&#10;        sG32 := STR(&#60;qryComms.&#34;G32&#34;&#62;);&#13;&#10;        sID := STR(&#60;qryComms.&#34;ID&#34;&#62;);&#13;&#10;        //COMPUTE('OPENQUERY(&#34;KRDMAIN&#34;, &#34;STS_DB&#34;, &#34;SELECT NBD FROM KRD_MAIN WHERE ID='+sID+'&#34;)');&#13;&#10;        //sNBD := COMPUTE('KRDMAIN.NBD');&#13;&#10;        sNBD := &#60;KRD_MAIN.&#34;NBD&#34;&#62;;            &#13;&#10;        sAcceptDate:=''; sGN:=''; sG312:=''; sG33:=''; sStoreNum:=''; sG35:='';&#13;&#10;        sG311:=IIF(bPartInPlace,sPartInPlaceText,'0');        &#13;&#10;        sValCode:=''; sStorageDate:='';&#13;&#10;        dG35:=0; dG311:=0; dG42:=0;&#13;&#10;            &#13;&#10;        sOutDate:=''; sOutG35:=''; sOut_DocNo:=''; sRecepient:='';&#13;&#10;        sOutG311 := IIF(bPartOutPlace, sPartOutPlaceText, '0');        &#13;&#10;        dOutG311:=0; dOutG42:=0;&#13;&#10;        &#13;&#10;        sOstG35:='';                                     &#13;&#10;        dOstG311:=0; dOstG42:=0;        &#13;&#10;          &#13;&#10;        COMPUTE('OPENQUERY(&#34;PRIHOD&#34;, &#34;STS_DB&#34;, &#34;SELECT G312, GN, G33, G311, G35, G42, G42_CURRENCY, '+&#13;&#10;                                                   ' VALCODE, BOXNO, ACCEPTDATE, STORAGE_DATE '+&#13;&#10;                                                   'FROM KRD_COMM '+&#13;&#10;                                                   'WHERE '+&#13;&#10;                                                   ' PLACEID='+sPlaceID+&#13;&#10;                                                   ' AND ID='+sID+' AND G32='+sG32+&#13;&#10;                                                   '&#34;)');&#13;&#10;                                                     &#13;&#10;        sGN:=COMPUTE('PRIHOD.GN');&#13;&#10;        sG312:=COMPUTE('PRIHOD.G312');&#13;&#10;        sG33:=COMPUTE('PRIHOD.G33');&#13;&#10;        sStoreNum:=COMPUTE('PRIHOD.BOXNO');&#13;&#10;        dG35 := COMPUTE('PRIHOD.G35'); &#13;&#10;        sG35:=STR(dG35)+' кг.';          &#13;&#10;        sAcceptDate:=FormatDateTime('DD.MM.YYYY HH:MM', COMPUTE('PRIHOD.AcceptDate'));&#13;&#10;        sStorageDate:=COMPUTE('PRIHOD.Storage_Date');&#13;&#10;    &#13;&#10;        sValCode:= COMPUTE('CURRENCYABBR(PRIHOD.G42_CURRENCY)');                      &#13;&#10;    &#13;&#10;        dG311:=COMPUTE('PRIHOD.G311');&#13;&#10;        IF dG311 &#60;&#62; 0 then sG311 := STR(dG311);  &#13;&#10;          &#13;&#10;        dG42:=COMPUTE('PRIHOD.G42');&#13;&#10;    &#13;&#10;        dSumG35:=dSumG35+COMPUTE('PRIHOD.G35');                                    &#13;&#10;        dSumG311:=dSumG311+dG311;&#13;&#10;        dSumG42:=dSumG42+dG42;&#13;&#10;&#13;&#10;        if (dG35=0) and (dG311=0) and (dG42=0)&#13;&#10;        then MasterData1.Visible:=false;                                                                                &#13;&#10;    end;        &#13;&#10;    sOutDate:=FORMATDATETIME('dd.mm.yyyy hh:mm',&#60;qryComms.&#34;RELEASE_OUT_DATE&#34;&#62;);&#13;&#10;    IF sOutDate='' then sOutDate:= FORMATDATETIME('dd.mm.yyyy hh:mm',&#60;REL_MAIN.&#34;OUT_DATE&#34;&#62;);&#13;&#10;    sOut_DocNo:= IIF(bManyParts,  &#60;qryComms.&#34;DOC_TYPE&#34;&#62;+' '+&#60;qryComms.&#34;DOC_NO&#34;&#62;, &#60;REL_MAIN.&#34;DOC_TYPE&#34;&#62;+' '+&#60;REL_MAIN.&#34;DOC_NO&#34;&#62;);&#13;&#10;    sRecepient:= &#60;REL_MAIN.&#34;G082&#34;&#62;;&#13;&#10;&#13;&#10;    sOutG35 := STR(&#60;qryComms.&#34;RELEASE_G35&#34;&#62;) + ' кг.';&#13;&#10;    dOutG311:= &#60;qryComms.&#34;RELEASE_G311&#34;&#62;;&#13;&#10;    IF dOutG311 &#60;&#62; 0 then sOutG311 := STR(dOutG311);        &#13;&#10;    dOutG42:=&#60;qryComms.&#34;RELEASE_G42&#34;&#62;;                              &#13;&#10;      &#13;&#10;    dSumOutG35:=dSumOutG35+&#60;qryComms.&#34;RELEASE_G35&#34;&#62;;&#13;&#10;    dSumOutG311:=dSumOutG311+dOutG311;        &#13;&#10;    dSumOutG42:=dSumOutG42+dOutG42;&#13;&#10;    &#13;&#10;&#13;&#10;    COMPUTE('OPENQUERY(&#34;VIDANO&#34;, &#34;STS_DB&#34;, &#34;SELECT SUM(RC.RELEASE_G35) as G35, SUM(RC.RELEASE_G311) as G311, SUM(RC.RELEASE_G42) as G42 '+&#13;&#10;                                           'FROM REL_COMM RC '+&#13;&#10;                                           IIF(bManyParts, ' LEFT JOIN RELEASE R ON(R.PLACEID=RC.PLACEID AND R.ID=RC.ID AND R.COUNTER=RC.COUNTER) ','')+                                                                                                        &#13;&#10;                                           'WHERE '+&#13;&#10;                                           ' RC.PLACEID='+sPlaceID+&#13;&#10;                                           ' AND RC.ID='+sID+' AND RC.G32='+sG32+&#13;&#10;                                           ' AND (RC.COUNTER&#60;='+STR(&#60;qryComms.&#34;COUNTER&#34;&#62;)+&#13;&#10;                                           IIF(bManyParts, ' OR R.MAIN_ID='+sMainID, '')+')'+                                                                                                                                &#13;&#10;                                           '&#34;)');                                                                                 &#13;&#10;    dOstG35:=dG35 - COMPUTE('VIDANO.G35');                                                                 &#13;&#10;    sOstG35:=STR(dOstG35) + ' кг.';&#13;&#10;    dOstG311:=dG311 - COMPUTE('VIDANO.G311');&#13;&#10;    dOstG42:=dG42 - COMPUTE('VIDANO.G42');&#13;&#10;&#13;&#10;    dSumOstG35:=dSumOstG35+dOstG35;&#13;&#10;    dSumOstG42:=dSumOstG42+dOstG42;&#13;&#10;    dSumOstG311:=dSumOstG311+dOstG311;        &#13;&#10;end;        &#13;&#10;end;&#13;&#10;  &#13;&#10;procedure PageFooter1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;    sDatPodp := FORMATDATETIME('dd.mm.yyyy', &#60;REL_MAIN.&#34;OUT_DATE&#34;&#62;);&#13;&#10;    sPodpis := &#60;REL_MAIN.&#34;RELEASE_GIVE&#34;&#62;+'    '+&#60;REL_MAIN.&#34;RELEASE_GIVE_POST&#34;&#62;;  &#13;&#10;end;&#13;&#10;&#13;&#10;&#13;&#10;&#13;&#10;begin                                  &#13;&#10;  //Что печатать в графе 8   &#13;&#10;  bPartInPlace := IniFile(sIniSection, 'PartInPlace', 'False', sIniFullPath) = 'True';&#13;&#10;  sPartInPlaceText := IniFile(sIniSection, 'PartInPlaceText', 'часть места', sIniFullPath);&#13;&#10;  &#13;&#10;  //Что печатать в графе 14&#13;&#10;  bPartOutPlace := IniFile(sIniSection, 'PartOutPlace', 'False', sIniFullPath) = 'True';&#13;&#10;  sPartOutPlaceText := IniFile(sIniSection, 'PartOutPlaceText', 'часть места', sIniFullPath);&#13;&#10;&#13;&#10;  bPrintRegNum := IniFile(sIniSection, 'PrintRegNum', 'False', sIniFullPath) = 'True';&#13;&#10;    &#13;&#10;  bPrintItog := IniFile(sIniSection, 'PrintItog', 'False', sIniFullPath) = 'True';&#13;&#10;&#13;&#10;  bShowDialog := IniFile(sIniSection, 'ShowDialog', 'False', sIniFullPath) = 'True';&#13;&#10;&#13;&#10;    //Показывать или нет диалоговую форму&#13;&#10;  if ShiftPressed() then&#13;&#10;    Page1.Visible := True&#13;&#10;  else if bShowDialog then&#13;&#10;    Page1.Visible := True&#13;&#10;  else&#13;&#10;    Page1.Visible := False;      &#13;&#10;end." OnStartReport="OnStartReport">
