<TfrxReport Version="5.3.1" DotMatrixReport="False" EngineOptions.ConvertNulls="False" IniFile="\Software\Fast Reports" PreviewOptions.Buttons="4095" PreviewOptions.Zoom="1" PrintOptions.Printer="По умолчанию" PrintOptions.PrintOnSheet="0" ReportOptions.CreateDate="42905.4433324884" ReportOptions.Description.Text="" ReportOptions.Name="Форма ДО-1 (Приказ № 2688 от 29.12.2012)" ReportOptions.LastChange="43690.503173912" ScriptLanguage="PascalScript" ScriptText.Text="VAR&#13;&#10;  sWHType, sDocType, sPartPlace, sBoxNo,&#13;&#10;  sTransportDocs, sBacksideTransportDocs, sTransp,&#13;&#10;  sConsigneePrev, sSumG42Code : String;&#13;&#10;&#13;&#10;  bPrintConts, bPrintPartPlace, bPrintDeadlineNotAlways, bPrintTranspDocs, bPrintOnlyTD,&#13;&#10;  bPrintConsignee, bPrintTransport, bPrintRegistration,&#13;&#10;  bPrintSum, bPrintAsIs, bPrintRUB, bPrintUSD, bPrintEUR,&#13;&#10;  bPrintSumG42 : Boolean;&#13;&#10;&#13;&#10;  iDopListNo, TypeWeight, iCurrencyCode, DocumentsType : Integer;&#13;&#10;  &#13;&#10;  fSumG42 : Double;&#13;&#10;&#13;&#10;  N3, N4, N9, N7S : String;                       &#13;&#10;  N6, iSumG311 : Integer;&#13;&#10;  N7, N8, fSumG35, fSumG315A  : Double;&#13;&#10;  bIgnoreCommAct : Boolean;                                            &#13;&#10;    &#13;&#10;  &#13;&#10;procedure ReportTitle1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  fSumG35 := 0;&#13;&#10;  fSumG315A := 0;&#13;&#10;  iSumG311 := 0;&#13;&#10;  fSumG42 := 0;&#13;&#10;&#13;&#10;  qKC.SQL.Clear;    &#13;&#10;  if bIgnoreCommAct then begin&#13;&#10;    qKC.SQL.Add('SELECT');&#13;&#10;    qKC.SQL.Add('KC.ACCEPTDATE,');&#13;&#10;    qKC.SQL.Add('KC.G312,');&#13;&#10;    qKC.SQL.Add('KC.G33,');&#13;&#10;    qKC.SQL.Add('KC.BOXNO,');&#13;&#10;    qKC.SQL.Add('KC.G311,');&#13;&#10;    qKC.SQL.Add('KC.G35,KC.G315,KC.G315A,KC.G315C,KC.G315CN,');&#13;&#10;    qKC.SQL.Add('KC.G42,KC.G42_CURRENCY,');&#13;&#10;    qKC.SQL.Add('KC.STORAGE_DATE,KC.STORAGE_TYPE,');&#13;&#10;    qKC.SQL.Add('KC.REMARK,');&#13;&#10;    qKC.SQL.Add('KC.G32,KC.GN,');&#13;&#10;    qKC.SQL.Add('KC.PLACEID,KC.ID,');&#13;&#10;    qKC.SQL.Add('(SELECT COUNT(PLACEID) FROM KRD_CSDM WHERE PLACEID=KC.PLACEID AND ID=KC.ID AND G32=KC.G32 AND SDM_KIND IN ('+chr(39)+'0'+chr(39)+', '+chr(39)+'1'+chr(39)+')) AS ACT');&#13;&#10;    qKC.SQL.Add('FROM KRD_COMM KC');&#13;&#10;    qKC.SQL.Add('WHERE');&#13;&#10;    qKC.SQL.Add('KC.PLACEID=:placeid');&#13;&#10;    qKC.SQL.Add('AND KC.ID IN (');&#13;&#10;    qKC.SQL.Add('  SELECT');&#13;&#10;    qKC.SQL.Add('  DISTINCT KM.ID');&#13;&#10;    qKC.SQL.Add('  FROM KRD_MAIN KM');&#13;&#10;    qKC.SQL.Add('  WHERE');&#13;&#10;    qKC.SQL.Add('  PLACEID=:placeid');&#13;&#10;    qKC.SQL.Add('  AND MAIN_ID=:main_id)');&#13;&#10;    qKC.SQL.Add('AND (');  &#13;&#10;    qKC.SQL.Add('  SELECT');&#13;&#10;    qKC.SQL.Add('  COUNT(KCM.PLACEID)');&#13;&#10;    qKC.SQL.Add('  FROM KRD_CSDM KCM');&#13;&#10;    qKC.SQL.Add('  WHERE');&#13;&#10;    qKC.SQL.Add('  KCM.PLACEID=KC.PLACEID');&#13;&#10;    qKC.SQL.Add('  AND KCM.ID=KC.ID');&#13;&#10;    qKC.SQL.Add('  AND KCM.G32=KC.G32');&#13;&#10;    qKC.SQL.Add('  AND KCM.SDM_KIND='+chr(39)+'2'+chr(39)+') = 0');&#13;&#10;    qKC.SQL.Add('ORDER BY KC.GN, KC.G32');                                                                   &#13;&#10;  end&#13;&#10;  else begin&#13;&#10;    qKC.SQL.Add('SELECT');&#13;&#10;    qKC.SQL.Add('ACCEPTDATE,');&#13;&#10;    qKC.SQL.Add('G312,');&#13;&#10;    qKC.SQL.Add('G33,');&#13;&#10;    qKC.SQL.Add('BOXNO,');&#13;&#10;    qKC.SQL.Add('G311,');&#13;&#10;    qKC.SQL.Add('G35,G315,G315A,G315C,G315CN,');&#13;&#10;    qKC.SQL.Add('G42,G42_CURRENCY,');&#13;&#10;    qKC.SQL.Add('STORAGE_DATE,STORAGE_TYPE,');&#13;&#10;    qKC.SQL.Add('REMARK,');&#13;&#10;    qKC.SQL.Add('G32,GN,');&#13;&#10;    qKC.SQL.Add('PLACEID,ID');&#13;&#10;    qKC.SQL.Add('FROM KRD_COMM');&#13;&#10;    qKC.SQL.Add('WHERE');&#13;&#10;    qKC.SQL.Add('PLACEID=:placeid');&#13;&#10;    qKC.SQL.Add('AND ID IN (SELECT DISTINCT ID FROM KRD_MAIN WHERE PLACEID=:placeid AND MAIN_ID=:main_id)');&#13;&#10;    qKC.SQL.Add('ORDER BY GN, G32');  &#13;&#10;  end;&#13;&#10;  qKC.IgnoreDupParams := True;                                                      &#13;&#10;  qKC.ParamByName('placeid').DataType := 3;&#13;&#10;  qKC.ParamByName('placeid').Value := &#60;KRD_MAIN.&#34;PLACEID&#34;&#62;;  &#13;&#10;  qKC.ParamByName('main_id').DataType := 3;&#13;&#10;  qKC.ParamByName('main_id').Value := &#60;KRD_MAIN.&#34;MAIN_ID&#34;&#62;;                                                                            &#13;&#10;  qKC.Active := True;                                     &#13;&#10;//  qKC.Reopen;        &#13;&#10;                               &#13;&#10;  if bIgnoreCommAct then &#13;&#10;    qCSDM.Active := True&#13;&#10;  else&#13;&#10;    qCSDM.Active := False;                                                                     &#13;&#10;  &#13;&#10;  qStore.Active := True;&#13;&#10;  qPapers.Active := True;&#13;&#10;  qCommerceDocs.Active := True;&#13;&#10;  qCustomDocs.Active := True;                                                                &#13;&#10;        &#13;&#10;  case &#60;qStore.&#34;LICENCETYPE&#34;&#62; of&#13;&#10;       1: begin &#13;&#10;               sWHType := 'СВХ';                                                 &#13;&#10;               sDocType := 'Свидетельство о включении в реестр владельцев СВХ от';&#13;&#10;          end;                                                       &#13;&#10;       2: begin&#13;&#10;               sWHType := 'СВХ';                   &#13;&#10;               sDocType := 'Свидетельство о включении в реестр владельцев СВХ от';                   &#13;&#10;          end;                                  &#13;&#10;       3: begin&#13;&#10;               sWHType := 'Иное место временного хранения';                   &#13;&#10;               sDocType := 'Разрешение ТО на временное хранение в ином месте от';                   &#13;&#10;          end;                           &#13;&#10;       4: begin &#13;&#10;               sWHType := 'ПЗТК';                   &#13;&#10;               sDocType := 'Приказ(разрешение) таможни от';                   &#13;&#10;          end;                         &#13;&#10;       5: begin &#13;&#10;               sWHType := 'ВЗТК';                   &#13;&#10;               sDocType := 'Приказ(разрешение) таможни от';                   &#13;&#10;          end;&#13;&#10;  end;&#13;&#10;&#13;&#10;  if not bPrintRegistration or (Length (&#60;KRD_MAIN.&#34;REG_NBD&#34;&#62;) = 0) then&#13;&#10;    begin&#13;&#10;      RegShape.Visible := False;&#13;&#10;      RegInsp.Visible := False;&#13;&#10;      lblRegInsp.Visible := False;&#13;&#10;      RegDate.Visible := False;&#13;&#10;      lblRegDate.Visible := False;&#13;&#10;      lblRegInsSubtext.Visible := False;&#13;&#10;      RegNumber.Visible := False;&#13;&#10;      lblLNP.Visible := False;&#13;&#10;      LNP.Visible := False;&#13;&#10;        &#13;&#10;      RegShape2.Visible := False;&#13;&#10;      RegInsp2.Visible := False;&#13;&#10;      lblRegInsp2.Visible := False;&#13;&#10;      RegDate2.Visible := False;&#13;&#10;      lblRegDate2.Visible := False;&#13;&#10;      lblRegInsSubtext2.Visible := False;&#13;&#10;      RegNumber2.Visible := False;&#13;&#10;      lblLNP2.Visible := False;&#13;&#10;      LNP2.Visible := False;          &#13;&#10;    end;&#13;&#10;&#13;&#10;  qPapers.First;      &#13;&#10;  while not qPapers.Eof do&#13;&#10;    begin                                                    &#13;&#10;      sConsigneePrev := &#60;qPapers.&#34;G082&#34;&#62;;                                                              &#13;&#10;      if Length (sTransportDocs) = 0 then begin                                                  &#13;&#10;        sTransportDocs := &#60;qPapers.&#34;PAPERNAME&#34;&#62; + ' №' + TRIM(&#60;qPapers.&#34;PAPERNO&#34;&#62;);&#13;&#10;        if Length (&#60;qPapers.&#34;PAPERDATE&#34;&#62;) &#62; 0 then                                               &#13;&#10;          sTransportDocs := sTransportDocs + ' от ' + FormatDateTime('DD.MM.YYYY', &#60;qPapers.&#34;PAPERDATE&#34;&#62;);              &#13;&#10;      end                          &#13;&#10;      else begin                                           &#13;&#10;        sTransportDocs := sTransportDocs + ', ' + &#60;qPapers.&#34;PAPERNAME&#34;&#62; + ' №' + TRIM(&#60;qPapers.&#34;PAPERNO&#34;&#62;);&#13;&#10;        if Length (&#60;qPapers.&#34;PAPERDATE&#34;&#62;) &#62; 0 then                                                             &#13;&#10;          sTransportDocs := sTransportDocs + ' от ' + FormatDateTime('DD.MM.YYYY', &#60;qPapers.&#34;PAPERDATE&#34;&#62;);                                                                                             &#13;&#10;      end;                                   &#13;&#10;      qPapers.Next;&#13;&#10;      if bPrintConsignee and ((sConsigneePrev &#60;&#62; &#60;qPapers.&#34;G082&#34;&#62;) or qPapers.Eof) then&#13;&#10;        sTransportDocs := sTransportDocs + ' (в адрес: ' + sConsigneePrev + ')';          &#13;&#10;    end;  &#13;&#10;  &#13;&#10;  if DocumentsType &#62; 0 then&#13;&#10;    begin&#13;&#10;      qCommerceDocs.First;                                                             &#13;&#10;      while not qCommerceDocs.Eof do&#13;&#10;        begin&#13;&#10;          sTransportDocs := sTransportDocs + ', ' + &#60;qCommerceDocs.&#34;PAPERNAME&#34;&#62; + ' №' + TRIM(&#60;qCommerceDocs.&#34;PAPERNO&#34;&#62;);&#13;&#10;          if Length (&#60;qCommerceDocs.&#34;PAPERDATE&#34;&#62;) &#62; 0 then                                                           &#13;&#10;            sTransportDocs := sTransportDocs + ' от ' + FormatDateTime('DD.MM.YYYY', &#60;qCommerceDocs.&#34;PAPERDATE&#34;&#62;);                                                                                              &#13;&#10;          qCommerceDocs.Next;                                                                                                                                                         &#13;&#10;        end;                  &#13;&#10;      if DocumentsType &#62; 1 then&#13;&#10;        begin&#13;&#10;          qCustomDocs.First;                                                                         &#13;&#10;          while not qCustomDocs.Eof do&#13;&#10;            begin&#13;&#10;              sTransportDocs := sTransportDocs + ', ' + &#60;qCustomDocs.&#34;PAPERNAME&#34;&#62; + ' №' + TRIM(&#60;qCustomDocs.&#34;PAPERNO&#34;&#62;);&#13;&#10;              if Length (&#60;qCustomDocs.&#34;PAPERCLOSE&#34;&#62;) &#62; 0 then&#13;&#10;                sTransportDocs := sTransportDocs + ' от ' + FormatDateTime('DD.MM.YYYY', &#60;qCustomDocs.&#34;PAPERCLOSE&#34;&#62;);                                                                                                                                 &#13;&#10;              qCustomDocs.Next;                                                                                                                                                         &#13;&#10;            end;  &#13;&#10;        end;                  &#13;&#10;    end;&#13;&#10;      &#13;&#10;  if Length (sTransportDocs) &#62; 105 then&#13;&#10;    begin&#13;&#10;      BacksidePage.Visible := True;                                                                                 &#13;&#10;      sBacksideTransportDocs := sTransportDocs;                              &#13;&#10;      sTransportDocs := 'см. на обороте';                                                                  &#13;&#10;    end&#13;&#10;  else               &#13;&#10;    BacksidePage.Visible := False;                                                                             &#13;&#10;&#13;&#10;  sTransp := '';&#13;&#10;  while not qTransp.Eof do&#13;&#10;    begin&#13;&#10;      if Length (sTransp) = 0 then&#13;&#10;        begin                                     &#13;&#10;          sTransp := &#60;qTransp.&#34;CARNO&#34;&#62;;&#13;&#10;          if Length (&#60;qTransp.&#34;NTRAILER&#34;&#62;) &#62; 0 then sTransp := sTransp + ', ' + &#60;qTransp.&#34;NTRAILER&#34;&#62;;&#13;&#10;        end                                  &#13;&#10;      else&#13;&#10;        begin                                     &#13;&#10;          sTransp := sTransp + '; ' + &#60;qTransp.&#34;CARNO&#34;&#62;;&#13;&#10;          if Length (&#60;qTransp.&#34;NTRAILER&#34;&#62;) &#62; 0 then sTransp := sTransp + ', ' + &#60;qTransp.&#34;NTRAILER&#34;&#62;;                                                                    &#13;&#10;        end;                              &#13;&#10;      qTransp.Next;                          &#13;&#10;    end;                                                                       &#13;&#10;end;&#13;&#10;&#13;&#10;procedure Memo89OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  iDopListNo := iDopListNo + 1;                                                                    &#13;&#10;end;&#13;&#10;&#13;&#10;procedure cbxPrintPartPlaceOnClick(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  edtPartPlace.Enabled := cbxPrintPartPlace.Checked;         &#13;&#10;end;&#13;&#10;&#13;&#10;procedure cbxPrintSumOnClick(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  cbxPrintSumG42.Enabled := cbxPrintSum.Checked;      &#13;&#10;end;  &#13;&#10;  &#13;&#10;procedure btnOKOnClick(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  if cbxShowHints.Checked then&#13;&#10;    begin                         &#13;&#10;      Memo26.ShowHint := True;&#13;&#10;      Memo35.ShowHint := True;&#13;&#10;      Memo36.ShowHint := True;&#13;&#10;      Memo38.ShowHint := True;&#13;&#10;      Memo39.ShowHint := True;&#13;&#10;      Memo40.ShowHint := True;&#13;&#10;      Memo42.ShowHint := True;&#13;&#10;      Memo43.ShowHint := True;          &#13;&#10;    end;&#13;&#10;&#13;&#10;  // 0 - только накладные, 1 - накладные и инвойсы, 2 - накладные, инвойсы, таможенные документы                                                                                                                                                                                                                                                                          &#13;&#10;  DocumentsType := coboxDocuments.ItemIndex;&#13;&#10;  WriteIniFile('do1_2688', 'DocumentsType', DocumentsType);                                                                                                                                  &#13;&#10;&#13;&#10;  bPrintConts := cbxUseConts.Checked;&#13;&#10;  WriteIniFile('do1_2688', 'PrintConts', bPrintConts);  &#13;&#10;  &#13;&#10;  bPrintPartPlace := cbxPrintPartPlace.Checked;&#13;&#10;  sPartPlace := edtPartPlace.Text;&#13;&#10;  WriteIniFile('do1_2688', 'PrintPartPlace', bPrintPartPlace);&#13;&#10;  WriteIniFile('do1_2688', 'PartPlace', sPartPlace);&#13;&#10;&#13;&#10;  TypeWeight := coboxWeight.ItemIndex;                                                                                                                        &#13;&#10;  WriteIniFile('do1_2688', 'TypeWeight', TypeWeight);&#13;&#10;&#13;&#10;  bPrintDeadlineNotAlways := cbxPrintDeadlineNotAlways.Checked;&#13;&#10;  WriteIniFile('do1_2688', 'PrintDeadlineNotAlways', bPrintDeadlineNotAlways);&#13;&#10;&#13;&#10;  bPrintConsignee := cbxPrintConsignee.Checked;                                                                                                                                                       &#13;&#10;  bPrintTransport := cbxPrintTransport.Checked;&#13;&#10;  WriteIniFile('do1_2688', 'PrintConsignee', bPrintConsignee);&#13;&#10;  WriteIniFile('do1_2688', 'PrintTransport', bPrintTransport);&#13;&#10;&#13;&#10;  bPrintRegistration := cbxPrintRegistration.Checked;&#13;&#10;  WriteIniFile('do1_2688', 'PrintRegistration', bPrintRegistration);&#13;&#10;&#13;&#10;  bPrintSum := cbxPrintSum.Checked;&#13;&#10;  bPrintSumG42 := cbxPrintSumG42.Checked;                                                                              &#13;&#10;  bPrintAsIs := rbtnAsIs.Checked;&#13;&#10;  bPrintRUB := rbtnRUB.Checked;&#13;&#10;  bPrintUSD := rbtnUSD.Checked;&#13;&#10;  bPrintEUR := rbtnEUR.Checked;&#13;&#10;  bIgnoreCommAct := cbxIgnoreCommAct.Checked;                                                                                        &#13;&#10;    &#13;&#10;  WriteIniFile('do1_2688', 'PrintSum', bPrintSum);&#13;&#10;  WriteIniFile('do1_2688', 'PrintSumG42', bPrintSumG42);                                         &#13;&#10;  WriteIniFile('do1_2688', 'PrintAsIs', bPrintAsIs);&#13;&#10;  WriteIniFile('do1_2688', 'PrintRUB', bPrintRUB);&#13;&#10;  WriteIniFile('do1_2688', 'PrintUSD', bPrintUSD);&#13;&#10;  WriteIniFile('do1_2688', 'PrintEUR', bPrintEUR);                                   &#13;&#10;  WriteIniFile('do1_2688', 'IgnoreCommAct', bIgnoreCommAct);                                                                                                &#13;&#10;&#13;&#10;  WriteIniFile('do1_2688', 'ShowDialogAlways', ShowDialogAlways.Checked);    &#13;&#10;end;&#13;&#10;    &#13;&#10;procedure DialogPageOnActivate(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  cbxUseConts.Checked := bPrintConts;&#13;&#10;&#13;&#10;  coboxDocuments.ItemIndex := DocumentsType;                                                                                       &#13;&#10;  &#13;&#10;  cbxPrintPartPlace.Checked := bPrintPartPlace;&#13;&#10;  edtPartPlace.Enabled := bPrintPartPlace;                                                                                      &#13;&#10;  edtPartPlace.Text := sPartPlace;&#13;&#10;&#13;&#10;  coboxWeight.ItemIndex := TypeWeight;&#13;&#10;&#13;&#10;  cbxPrintDeadlineNotAlways.Checked := bPrintDeadlineNotAlways;&#13;&#10;&#13;&#10;  cbxPrintConsignee.Checked := bPrintConsignee;                                                                                                                                                        &#13;&#10;  cbxPrintTransport.Checked := bPrintTransport;&#13;&#10;&#13;&#10;  cbxPrintRegistration.Checked := bPrintRegistration;&#13;&#10;&#13;&#10;  cbxPrintSum.Checked := bPrintSum;&#13;&#10;  cbxPrintSumG42.Checked := bPrintSumG42;                                                                                          &#13;&#10;  cbxPrintSumOnClick(self);&#13;&#10;  cbxPrintSumG42OnClick(self);                    &#13;&#10;  rbtnAsIs.Checked := bPrintAsIs;&#13;&#10;  rbtnRUB.Checked := bPrintRUB;&#13;&#10;  rbtnUSD.Checked := bPrintUSD;&#13;&#10;  rbtnEUR.Checked := bPrintEUR;&#13;&#10;&#13;&#10;  cbxIgnoreCommAct.Checked := bIgnoreCommAct;                                                                                                      &#13;&#10;&#13;&#10;  ShowDialogAlways.Checked := IniFile('do1_2688', 'ShowDialogAlways', 'True');                                                                                                    &#13;&#10;end;    &#13;&#10;&#13;&#10;procedure Memo9OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  if bPrintSumG42 then&#13;&#10;    begin                         &#13;&#10;      if bPrintAsIs then&#13;&#10;        begin&#13;&#10;         fSumG42 := fSumG42 + N8;  &#13;&#10;        end&#13;&#10;      else if bPrintRUB then&#13;&#10;        fSumG42 := fSumG42 + COMPUTE ('TRANSCURRENCY (&#34;' + STR(&#60;KRD_MAIN.&#34;BD_DATE&#34;&#62;) + '&#34;, &#34;' + N9 + '&#34;, 643, ' + STR(N8) + ')')&#13;&#10;      else if bPrintUSD then&#13;&#10;        fSumG42 := fSumG42 + COMPUTE ('TRANSCURRENCY (&#34;' + STR(&#60;KRD_MAIN.&#34;BD_DATE&#34;&#62;) + '&#34;, &#34;' + N9 + '&#34;, 840, ' + STR(N8) + ')')  &#13;&#10;      else if bPrintEUR then&#13;&#10;        fSumG42 := fSumG42 + COMPUTE ('TRANSCURRENCY (&#34;' + STR(&#60;KRD_MAIN.&#34;BD_DATE&#34;&#62;) + '&#34;, &#34;' + N9 + '&#34;, 978, ' + STR(N8) + ')');&#13;&#10;    end;                                   