<TfrxReport Name="" DotMatrixReport="False" EngineOptions.ConvertNulls="False" IniFile="\Software\Fast Reports" PreviewOptions.Buttons="2047" PreviewOptions.Zoom="1" PrintOptions.Printer="По умолчанию" ReportOptions.CreateDate="38262.608752662" ReportOptions.Description.Text="" ReportOptions.Name="Справка о наличии лесоматериалов" ReportOptions.LastChange="38398.4627531944" ScriptLanguage="PascalScript" ScriptText.Text="Var&#13;&#10;  vPlaceId, vId: Integer;&#13;&#10;  dtOnDate, vOnDate: TDateTime;&#13;&#10;  vChief, vOrgName: String;&#13;&#10;  vLicenceDate: TDateTime;&#13;&#10;  Enbl: Integer;&#13;&#10;  vRestG311, vRestG315C: Extended;&#13;&#10;  vOnDateG311, vOnDateG315C: Extended;&#13;&#10;  vOnDateG311SenderSum, vOnDateG315CSenderSum: Extended;&#13;&#10;  vOnDateG311PZTKSum, vOnDateG315CPZTKSum: Extended;&#13;&#10;  vLineCnt: Integer;&#13;&#10;  vSQL: String;&#13;&#10;  vCustName, vCustCode, vCN: String;&#13;&#10;  i: Integer;&#13;&#10;  vLdate: String;&#13;&#10;&#13;&#10;procedure Page1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  with Page1, Engine do begin&#13;&#10;    dtOnDate:=StrToDateDef(COMPUTE('INIFILE(&#34;Print&#34;,&#34;PeriodEnd&#34;,&#34;&#34;)'), Date);&#13;&#10;    dtDate.Date := dtOnDate;&#13;&#10;&#13;&#10;    vPlaceId:=StrToInt(COMPUTE('INIFILE(&#34;Print&#34;,&#34;PlaceId&#34;,&#34;1&#34;)'));&#13;&#10;    If COMPUTE('FINDKEY(&#34;STORES_2&#34;, [&#34;' + STR(vPlaceId) + '&#34;])') then begin&#13;&#10;       fldStoreLic.Text := &#60;STORES_2.&#34;LICENCENO&#34;&#62; ;      //   номер лицензии&#13;&#10;       fldStoreNum.Text := '№ склада: ' + &#60;STORES_2.&#34;STORE_NO&#34;&#62; ; //   номер склада&#13;&#10;       fldStoreNm.Text := &#60;STORES_2.&#34;NAME&#34;&#62; ;     //   название склада&#13;&#10;       fldLicExt.Text := &#60;STORES_2.&#34;LICENCENO_EXT&#34;&#62;;     //   Признак перерегистрации&#13;&#10;       vChief :=  &#60;STORES_2.&#34;CHIEF&#34;&#62; ;&#13;&#10;       vLicenceDate := &#60;STORES_2.&#34;LICENCEDATE&#34;&#62;;&#13;&#10;       btnOk.Enabled := True;&#13;&#10;       Enbl := 1;&#13;&#10;    end&#13;&#10;    else begin&#13;&#10;      vPlaceId:=StrToInt(COMPUTE('INIFILE(&#34;WorkPlace&#34;,&#34;PlaceNum&#34;,&#34;&#34;)'));&#13;&#10;      If COMPUTE('FindKey(&#34;STORES_2&#34;, [&#34;' + STR(vPlaceId) + '&#34;])') then begin&#13;&#10;        fldStoreLic.Text := &#60;STORES_2.&#34;LICENCENO&#34;&#62; ;      //   номер лицензии&#13;&#10;        fldStoreNum.Text := '№ склада: ' + &#60;STORES_2.&#34;STORE_NO&#34;&#62; ; //   номер склада&#13;&#10;        fldStoreNm.Text := &#60;STORES_2.&#34;NAME&#34;&#62; ;     //   название склада&#13;&#10;        fldLicExt.Text := &#60;STORES_2.&#34;LICENCENO_EXT&#34;&#62;;     //   Признак перерегистрации&#13;&#10;        vChief:=  &#60;STORES_2.&#34;CHIEF&#34;&#62; ;&#13;&#10;        vLicenceDate := &#60;STORES_2.&#34;LICENCEDATE&#34;&#62;;&#13;&#10;        btnOk.Enabled := True;&#13;&#10;        Enbl := 1;&#13;&#10;      end&#13;&#10;      else begin&#13;&#10;        btnOk.Enabled := False;&#13;&#10;        Enbl := 0;&#13;&#10;      end&#13;&#10;    end;&#13;&#10;&#13;&#10;    vId:=StrToInt(COMPUTE('INIFILE(&#34;Print&#34;,&#34;Id&#34;,&#34;1&#34;)'));&#13;&#10;    COMPUTE('FINDKEY(&#34;KRD_MAIN_2&#34;, [&#34;' + IntToStr(vPlaceId) + '&#34;, &#34;' + IntToStr(vId) + '&#34;])');&#13;&#10;    vOrgName := &#60;KRD_MAIN_2.&#34;G022&#34;&#62;;&#13;&#10;    If vOrgName &#60;&#62; '' then begin&#13;&#10;      fldClientName.Text := vOrgName;      //   наименование организации&#13;&#10;      if Enbl = 1 then btnOk.Enabled := True;&#13;&#10;    end&#13;&#10;    else btnOk.Enabled := False;&#13;&#10;&#13;&#10;    If StrToInt(COMPUTE('INIFILE(&#34;Print&#34;,&#34;AllClients&#34;,&#34;1&#34;)')) = 1 then&#13;&#10;      AllClients.Checked := True&#13;&#10;    else OneClient.Checked := True;&#13;&#10;  end&#13;&#10;end;&#13;&#10;&#13;&#10;procedure ColorButton1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  with ColorButton1, Engine do begin&#13;&#10;    COMPUTE('GLOBALVAR(&#34;strStoreNumLic&#34;,String)');&#13;&#10;    COMPUTE('GLOBALVAR(&#34;strStoreNum&#34;,String)');&#13;&#10;    COMPUTE('GLOBALVAR(&#34;strStoreName&#34;,String)');&#13;&#10;    COMPUTE('GLOBALVAR(&#34;strStoreAddress&#34;,String)');&#13;&#10;    COMPUTE('GLOBALVAR(&#34;strLicExt&#34;,String)');&#13;&#10;    COMPUTE('GLOBALVAR(&#34;sPlaceId&#34;,Integer)');&#13;&#10;    COMPUTE('OPENQUERY(&#34;Stors&#34;, &#34;STS_DB&#34;, &#34;Select * from STORES&#34;)');&#13;&#10;    COMPUTE('SELECTVALUES (&#34;Список складов&#34;, &#34;Stors&#34;, ' +&#13;&#10;                 '[[&#34;LICENCENO&#34;,   &#34;Номер лицензии&#34;, 15], ' +&#13;&#10;                  '[&#34;LICENCEDATE&#34;,   &#34;Дата лицензии&#34;, 15], ' +&#13;&#10;                  '[&#34;LICENCENO_EXT&#34;,   &#34; &#34;, 1], ' +&#13;&#10;                  '[&#34;STORE_NO&#34;,   &#34;Номер склада&#34;, 15], ' +&#13;&#10;                  '[&#34;NAME&#34;,   &#34;Название склада&#34;, 30], ' +&#13;&#10;                  '[&#34;PLACEID&#34;,   &#34;&#34;, 10], ' +&#13;&#10;                  '[&#34;ADDRESS&#34;, &#34;Адрес склада&#34;, 30]], ' +&#13;&#10;                 '[[&#34;LICENCENO&#34;, &#34;strStoreNumLic&#34;], ' +&#13;&#10;                  '[&#34;STORE_NO&#34;, &#34;strStoreNum&#34;], ' +&#13;&#10;                  '[&#34;NAME&#34;, &#34;strStoreName&#34;], ' +&#13;&#10;                  '[&#34;LICENCENO_EXT&#34;, &#34;strLicExt&#34;], ' +&#13;&#10;                  '[&#34;PLACEID&#34;, &#34;sPlaceId&#34;], ' +&#13;&#10;                  '[&#34;ADDRESS&#34;, &#34;strStoreAddress&#34;]], ' +&#13;&#10;                  '&#34;&#34;, &#34;STS_DB&#34;)');&#13;&#10;    fldStoreLic.Text := COMPUTE('Value(&#34;strStoreNumLic&#34;)');  //   номер  лиценцзии&#13;&#10;    fldStoreNum.Text := '№ склада: ' + COMPUTE('Value(&#34;strStoreNum&#34;)') ; //   номер склада&#13;&#10;    fldStoreNm.Text := COMPUTE('Value(&#34;strStoreName&#34;)') ;     //   название склада&#13;&#10;    fldLicExt.Text := COMPUTE('Value(&#34;strLIcExt&#34;)');     //   Признак перерегистрации&#13;&#10;    vPlaceId := StrToInt(COMPUTE('Value(&#34;sPlaceId&#34;)'));&#13;&#10;    COMPUTE('FINDKEY(&#34;STORES_2&#34;, [&#34;' + IntToStr(vPlaceid) + '&#34;])');&#13;&#10;    vLdate:=  FormatDateTime('DD.MM.YYYY', &#60;STORES_2.&#34;LICENCEDATE&#34;&#62;);&#13;&#10;    If vLDate  &#60;&#62; '' then begin&#13;&#10;      btnOK.Enabled := True;&#13;&#10;      Enbl := 1;&#13;&#10;      btnChClient.Enabled := True;&#13;&#10;    end&#13;&#10;    else begin&#13;&#10;      MessageDlg('Заполните дату выдачи лицензии в &#34;Информации о складе&#34;!',0,0,0);&#13;&#10;      btnOK.Enabled := False;&#13;&#10;      Enbl := 0;&#13;&#10;      btnChClient.Enabled := False;&#13;&#10;    end&#13;&#10;  end&#13;&#10;end;&#13;&#10;&#13;&#10;procedure btnOkOnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  with btnOk, Engine do begin&#13;&#10;    //Далее см. Params к таблицам&#13;&#10;    vOnDate := dtDate.Date + StrToTime('23:59:59');&#13;&#10;    //Активируем главный запрос&#13;&#10;    if AllClients.Checked = True then begin&#13;&#10;      vSQL := 'SELECT KM.PLACEID, ' +&#13;&#10;                     'KM.ID, ' +&#13;&#10;                     'KM.G022, ' +&#13;&#10;                     'KM.G023 ' +&#13;&#10;              'FROM KRD_MAIN KM ' +&#13;&#10;              'WHERE KM.PLACEID = ' + Chr(39) + IntToStr(vPlaceId) + Chr(39);&#13;&#10;      qryOrgsMain.SQL.Clear;&#13;&#10;      qryOrgsMain.SQL.Add(vSQL);&#13;&#10;      qryOrgsMain.Active := True;&#13;&#10;      COMPUTE('WRITEINIFILE(&#34;Print&#34;,&#34;AllClients&#34;,&#34;1&#34;)')&#13;&#10;    end&#13;&#10;    else begin&#13;&#10;      vSQL := 'SELECT KM.PLACEID, ' +&#13;&#10;                     'KM.ID, ' +&#13;&#10;                     'KM.G022, ' +&#13;&#10;                     'KM.G023 ' +&#13;&#10;              'FROM KRD_MAIN KM ' +&#13;&#10;              'WHERE KM.PLACEID = ' + Chr(39) + IntToStr(vPlaceId) + Chr(39) +&#13;&#10;              ' AND KM.ID = ' + Chr(39) + IntToStr(vId) + Chr(39);&#13;&#10;      qryOrgsMain.SQL.Clear;&#13;&#10;      qryOrgsMain.SQL.Add(vSQL);&#13;&#10;      qryOrgsMain.Active := True;&#13;&#10;      COMPUTE('WRITEINIFILE(&#34;Print&#34;,&#34;AllClients&#34;,&#34;0&#34;)');&#13;&#10;    end;&#13;&#10;    qryComms.Active := True;&#13;&#10;    qryOutComms.Active := True;&#13;&#10;    qryIn.Active := True;&#13;&#10;    qryOut.Active := True;&#13;&#10;    //сохраняем настройки&#13;&#10;    Compute('WRITEINIFILE(&#34;Print&#34;,&#34;PeriodEnd&#34;,&#34;' + FormatDateTime('DD.MM.YYYY', dtDate.Date) + '&#34;)');&#13;&#10;    Compute('WRITEINIFILE(&#34;Print&#34;,&#34;PlaceId&#34;,&#34;' + IntToStr(vPlaceId) + '&#34;)');&#13;&#10;    Compute('WRITEINIFILE(&#34;Print&#34;,&#34;Id&#34;,&#34;' + IntToStr(vId) + '&#34;)');&#13;&#10;  end&#13;&#10;end;&#13;&#10;&#13;&#10;procedure AllClientsOnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  with AllClients, Engine do begin&#13;&#10;    if AllClients.Checked = True then begin&#13;&#10;      fldClientName.Enabled := False;&#13;&#10;      btnChClient.Enabled := False;&#13;&#10;    end&#13;&#10;  end&#13;&#10;end;&#13;&#10;&#13;&#10;procedure OneClientOnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  with OneClient, Engine do begin&#13;&#10;    if OneClient.Checked = True then begin&#13;&#10;      fldClientName.Enabled := True;&#13;&#10;      btnChClient.Enabled := True;&#13;&#10;    end&#13;&#10;  end&#13;&#10;end;&#13;&#10;&#13;&#10;procedure btnChClientOnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  with btnChClient, Engine do begin&#13;&#10;    COMPUTE('GLOBALVAR(&#34;sId&#34;, Integer)');&#13;&#10;    COMPUTE('GLOBALVAR(&#34;sOrgName&#34;, String)');&#13;&#10;    COMPUTE('GLOBALVAR(&#34;sOrgAddress&#34;, String)');&#13;&#10;&#13;&#10;    vSQL := 'SELECT KM.ID, KM.G022, KM.G023 ' +&#13;&#10;            'FROM KRD_MAIN KM ' +&#13;&#10;            'WHERE KM.PLACEID=' + CHR(39) + IntToStr(vPlaceId) + CHR(39);&#13;&#10;&#13;&#10;    COMPUTE('OPENQUERY(&#34;ORG_S&#34;, &#34;STS_DB&#34;, &#34;' + vSQL + '&#34;)');&#13;&#10;    COMPUTE('SelectValues (&#34;Список клиентов (отправителей)&#34;, &#34;ORG_S&#34;, ' +&#13;&#10;                       '[[&#34;G022&#34;,   &#34;Наименование организации&#34;, 40], ' +&#13;&#10;                        '[&#34;G023&#34;,   &#34;Адрес организации&#34;, 40]], ' +&#13;&#10;                       '[[&#34;G022&#34;, &#34;sOrgName&#34;], ' +&#13;&#10;                        '[&#34;G023&#34;, &#34;sOrgAddress&#34;], ' +&#13;&#10;                        '[&#34;ID&#34;, &#34;sId&#34;]], ' +&#13;&#10;                        '&#34;&#34;, &#34;STS_DB&#34;)');&#13;&#10;    fldClientName.Text := COMPUTE('Value(&#34;sOrgName&#34;)');  //адрес организации&#13;&#10;    vId := StrToInt(COMPUTE('Value(&#34;sId&#34;)'));&#13;&#10;    if IntToStr(vId) &#60;&#62; '' then begin&#13;&#10;      if Enbl = 1 then btnOk.Enabled := True&#13;&#10;    end&#13;&#10;    else btnOk.Enabled := False;&#13;&#10;  end&#13;&#10;end;&#13;&#10;&#13;&#10;procedure MasterData1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  with MasterData1, Engine do begin&#13;&#10;    vOnDateG311SenderSum := 0;&#13;&#10;    vOnDateG315CSenderSum := 0;&#13;&#10;&#13;&#10;    vLineCnt := 0;&#13;&#10;&#13;&#10;    if &#60;qryIn.&#34;SUM_IN_G311&#34;&#62; &#60;&#62; NULL then begin&#13;&#10;      vRestG311 := &#60;qryIn.&#34;SUM_IN_G311&#34;&#62;;&#13;&#10;      if &#60;qryOut.&#34;SUM_REL_G311&#34;&#62; &#60;&#62; NULL then vRestG311 := &#60;qryIn.&#34;SUM_IN_G311&#34;&#62; - &#60;qryOut.&#34;SUM_REL_G311&#34;&#62;&#13;&#10;    end&#13;&#10;    else vRestG311 := 0;&#13;&#10;&#13;&#10;    if &#60;qryIn.&#34;SUM_IN_G315C&#34;&#62; &#60;&#62; NULL then begin&#13;&#10;      vRestG315C := &#60;qryIn.&#34;SUM_IN_G315C&#34;&#62;;&#13;&#10;      if &#60;qryOut.&#34;SUM_REL_G315C&#34;&#62; &#60;&#62; NULL then vRestG315C := &#60;qryIn.&#34;SUM_IN_G315C&#34;&#62; - &#60;qryOut.&#34;SUM_REL_G315C&#34;&#62;&#13;&#10;    end&#13;&#10;    else vRestG315C := 0;&#13;&#10;&#13;&#10;    if (vRestG311 &#60;= 0) and (StrToFloatDef(FormatFloat('#.###', vRestG315C), 0) &#60;= 0) then begin&#13;&#10;      MasterData1.Visible := False;&#13;&#10;      DetailHeader1.Visible := False;&#13;&#10;      DetailFooter1.Visible := False;&#13;&#10;    end&#13;&#10;    else begin&#13;&#10;      MasterData1.Visible := True;&#13;&#10;      DetailHeader1.Visible := True;&#13;&#10;      DetailFooter1.Visible := True;&#13;&#10;    end;&#13;&#10;  end&#13;&#10;end;&#13;&#10;&#13;&#10;procedure ReportTitle1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  with ReportTitle1, Engine do begin&#13;&#10;    vOnDateG311PZTKSum := 0;&#13;&#10;    vOnDateG315CPZTKSum := 0;&#13;&#10;    vLineCnt := 0;&#13;&#10;  end&#13;&#10;end;&#13;&#10;&#13;&#10;procedure DetailData1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  with DetailData1, Engine do begin&#13;&#10;    vOnDateG311 := 0;&#13;&#10;    vOnDateG315C := 0;&#13;&#10;    //Кол-во на дату&#13;&#10;    vOnDateG311 := IIF(&#60;qryComms.&#34;SUM_IN_G311&#34;&#62;&#60;&#62;NULL,&#60;qryComms.&#34;SUM_IN_G311&#34;&#62;,0) -&#13;&#10;                   IIF(&#60;qryOutComms.&#34;SUM_OUT_G311&#34;&#62;&#60;&#62;NULL,&#60;qryOutComms.&#34;SUM_OUT_G311&#34;&#62;,0);&#13;&#10;    vOnDateG315C := IIF(&#60;qryComms.&#34;SUM_IN_G315C&#34;&#62;&#60;&#62;NULL,&#60;qryComms.&#34;SUM_IN_G315C&#34;&#62;,0) -&#13;&#10;                    IIF(&#60;qryOutComms.&#34;SUM_OUT_G315C&#34;&#62;&#60;&#62;NULL,&#60;qryOutComms.&#34;SUM_OUT_G315C&#34;&#62;,0);&#13;&#10;    //Если в начале все было по нолям и ничего не пришло за период, то строку с товаром не показываем&#13;&#10;    if (vOnDateG311 &#60;= 0) AND (StrToFloatDef(FormatFloat('#.###', vOnDateG315C), 0) &#60;= 0) then&#13;&#10;      DetailData1.Visible := False&#13;&#10;    else begin&#13;&#10;      vLineCnt := vLineCnt + 1;&#13;&#10;      DetailData1.Visible := True;&#13;&#10;      //Кол-во по поставщику на дату&#13;&#10;      vOnDateG311SenderSum := vOnDateG311SenderSum + vOnDateG311;&#13;&#10;      vOnDateG315CSenderSum := vOnDateG315CSenderSum + vOnDateG315C;&#13;&#10;    end;&#13;&#10;  end&#13;&#10;end;&#13;&#10;&#13;&#10;procedure PageHeader1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  with PageHeader1, Engine do begin&#13;&#10;    COMPUTE('FINDKEY(&#34;STORES_2&#34;, [&#34;' + IntToStr(vPLaceId) + '&#34;])');&#13;&#10;  end&#13;&#10;end;&#13;&#10;&#13;&#10;procedure DetailFooter1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  with DetailFooter1, Engine do begin&#13;&#10;    //Считаем по ПЗТК&#13;&#10;    //Кол-во по поставщику на начало периода&#13;&#10;    vOnDateG311PZTKSum := vOnDateG311PZTKSum + vOnDateG311SenderSum;&#13;&#10;    vOnDateG315CPZTKSum := vOnDateG315CPZTKSum + vOnDateG315CSenderSum;&#13;&#10;  end&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Memo1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  with Memo1, Engine do begin&#13;&#10;    vCustName := &#34;&#34;; vCustCode := &#34;&#34;; vCN := &#34;&#34;;&#13;&#10;    i := 0;&#13;&#10;    vCustCode := Copy(&#60;STORES_2.&#34;CUSTOMS_CODE&#34;&#62;, 1, 5) + &#34;000&#34;;&#13;&#10;    vCustName := COMPUTE('REFERENCE(&#34;CUSTOMS.DB&#34;, &#34;CUSTOMS_CODE&#34;, &#34;' + vCustCode + '&#34;, &#34;CUSTOMS_NAME&#34;)');&#13;&#10;    for i := 1 to Length(vCustName) do begin&#13;&#10;      vCN := vCN + Copy(vCustName,i,1);&#13;&#10;      if Rightcopy(vCN,2) = &#34;АЯ&#34; then vCN := Copy(vCN,1,LENGTH(vCN)-2) + &#34;ОЙ&#34;;&#13;&#10;    end;&#13;&#10;    vCustName := vCN;&#13;&#10;    if Rightcopy(vCustName,7) = &#34;ТАМОЖНЯ&#34; then vCustName := Copy(vCustName,1,LENGTH(vCustName)-8);&#13;&#10;  end&#13;&#10;end;&#13;&#10;&#13;&#10;begin&#13;&#10;&#13;&#10;end." PropData="">
