    <TfrxBDEQuery Name="qryOutComms" UserName="qryOutComms" FieldAliases.Text="" Master="qryComms" SQL.Text="SELECT KC.G33, KC.FORESTGOST, KC.FORESTSORT, KC.FORESTLENGTH, KC.FORESTDIAMETER,&#13;&#10;       KC.PLACEID, KC.ID,&#13;&#10;       SUM(RC.RELEASE_G311) AS SUM_OUT_G311,&#13;&#10;       SUM(RC.RELEASE_G315C) AS SUM_OUT_G315C &#13;&#10;FROM KRD_COMM KC&#13;&#10;     LEFT JOIN REL_COMM RC ON RC.PLACEID = KC.PLACEID&#13;&#10;                          AND RC.ID = KC.ID&#13;&#10;                          AND RC.G32 = KC.G32&#13;&#10;     LEFT JOIN RELEASE R ON R.PLACEID = RC.PLACEID&#13;&#10;                        AND R.ID = RC.ID&#13;&#10;                        AND R.COUNTER = RC.COUNTER&#13;&#10;WHERE KC.PLACEID = :PLACEID&#13;&#10;  AND KC.ID = :ID&#13;&#10;  AND KC.FORESTGOST = :FORESTGOST&#13;&#10;  AND KC.FORESTSORT = :FORESTSORT&#13;&#10;  AND KC.FORESTLENGTH = :FORESTLENGTH&#13;&#10;  AND KC.FORESTDIAMETER = :FORESTDIAMETER&#13;&#10;  AND (R.OUT_DATE &#60;= :OUT_DATE&#13;&#10;    OR R.OUT_DATE IS NULL)&#13;&#10;GROUP BY KC.G33, KC.FORESTGOST, KC.FORESTSORT, KC.FORESTLENGTH, KC.FORESTDIAMETER,&#13;&#10;         KC.PLACEID, KC.ID&#13;&#10;ORDER BY &#13;&#10;   KC.G33, KC.FORESTGOST, KC.FORESTSORT, KC.FORESTLENGTH, KC.FORESTDIAMETER" DatabaseName="STS_DB" SessionName="Default" PropData=""/>
