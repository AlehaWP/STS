    <TfrxBDEQuery Name="qryORGSMain" UserName="qryORGSMain" FieldAliases.Text="" SQL.Text="SELECT KM.PLACEID,&#13;&#10;       KM.ID,&#13;&#10;       KM.G022,&#13;&#10;       KM.G023&#13;&#10;FROM KRD_MAIN KM&#13;&#10;WHERE KM.PLACEID = :PLACEID&#13;&#10;  AND KM.ID = :ID" DatabaseName="STS_DB" SessionName="Default" PropData="05704C65667402080470546F7003C0000A506172616D657465727301010C3A000000204E616D653D22504C4143454944222044617461547970653D226674496E7465676572222045787072657373696F6E3D2276506C61636549642200010C30000000204E616D653D224944222044617461547970653D226674496E7465676572222045787072657373696F6E3D22764964220000"/>
    <TfrxBDEQuery Name="qryIn" UserName="qryIn" FieldAliases.Text="" Master="qryORGSMain" SQL.Text="SELECT KM.PLACEID, KM.ID,&#13;&#10;       SUM(KCM.G311) AS SUM_IN_G311,&#13;&#10;       SUM(KCM.G315C) AS SUM_IN_G315C&#13;&#10;FROM KRD_MAIN KM&#13;&#10;     INNER JOIN KRD_CMAP KCM ON KM.PLACEID = KCM.PLACEID &#13;&#10;                            AND KM.ID = KCM.ID&#13;&#10;WHERE KM.PLACEID = :PLACEID&#13;&#10;  AND KM.ID = :ID&#13;&#10;  AND KCM.ACT_DATE &#60;= :ON_DATE&#13;&#10;GROUP BY KM.PLACEID, KM.ID&#13;&#10;ORDER BY KM.PLACEID, KM.ID" DatabaseName="STS_DB" SessionName="Default" PropData=""/>
    <TfrxBDEQuery Name="qryOut" UserName="qryOut" FieldAliases.Text="" Master="qryORGSMain" SQL.Text="SELECT KM.PLACEID, KM.ID,&#13;&#10;       SUM(RC.RELEASE_G311) AS SUM_REL_G311, &#13;&#10;       SUM(RC.RELEASE_G315C) AS SUM_REL_G315C&#13;&#10;FROM KRD_MAIN KM&#13;&#10;     INNER JOIN REL_COMM RC ON KM.PLACEID = RC.PLACEID &#13;&#10;                           AND KM.ID = RC.ID&#13;&#10;     INNER JOIN RELEASE R ON RC.PLACEID = R.PLACEID &#13;&#10;                         AND RC.ID = R.ID&#13;&#10;                         AND RC.COUNTER = R.COUNTER&#13;&#10;WHERE KM.PLACEID = :PLACEID&#13;&#10;  AND KM.ID = :ID&#13;&#10;  AND R.OUT_DATE &#60;= :ON_DATE&#13;&#10;GROUP BY KM.PLACEID, KM.ID&#13;&#10;ORDER BY KM.PLACEID, KM.ID" DatabaseName="STS_DB" SessionName="Default" PropData=""/>
    <TfrxBDEQuery Name="qryComms" UserName="qryComms" FieldAliases.Text="" Master="qryORGSMain" SQL.Text="SELECT KC.G33, KC.FORESTGOST, KC.FORESTSORT, KC.FORESTLENGTH, KC.FORESTDIAMETER,&#13;&#10;       KC.PLACEID, KC.ID,&#13;&#10;       SUM(KCM.G311) AS SUM_IN_G311,&#13;&#10;       SUM(KCM.G315C) AS SUM_IN_G315C&#13;&#10;FROM KRD_COMM KC&#13;&#10;     INNER JOIN KRD_CMAP KCM ON KCM.PLACEID = KC.PLACEID&#13;&#10;                            AND KCM.ID = KC.ID&#13;&#10;                            AND KCM.G32 = KC.G32&#13;&#10;WHERE KC.PLACEID = :PLACEID&#13;&#10;  AND KC.ID = :ID&#13;&#10;  AND KCM.ACT_DATE &#60;= :ON_DATE&#13;&#10;GROUP BY KC.G33, KC.FORESTGOST, KC.FORESTSORT, KC.FORESTLENGTH, KC.FORESTDIAMETER,&#13;&#10;         KC.PLACEID, KC.ID" DatabaseName="STS_DB" SessionName="Default" PropData=""/>
