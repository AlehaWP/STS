<TfrxReport Version="5.3.1" DotMatrixReport="False" EngineOptions.ConvertNulls="False" IniFile="\Software\Fast Reports" PreviewOptions.Buttons="4095" PreviewOptions.Zoom="1" PrintOptions.Printer="По умолчанию" PrintOptions.PrintOnSheet="0" ReportOptions.Author="ООО &#34;СТМ&#34;" ReportOptions.CreateDate="42809.6348794676" ReportOptions.Description.Text="" ReportOptions.Name="Отчет об остатках товаров на складе" ReportOptions.LastChange="42867.7307106366" ScriptLanguage="PascalScript" ScriptText.Text="var         &#13;&#10;&#13;&#10;  iLicenceType, vPlaceId, iSelected, iLine, iRestG311, iSumRestG311 : Integer;&#13;&#10;&#13;&#10;  dRestG35, dRestG42, dSumRestG35, dSumRestG42 : Double;                                                                                      &#13;&#10;    &#13;&#10;  vLicenceNo, vLicenceDate, vSvhName, sWHType, sWHType1, sDocType, &#13;&#10;  sTransport, sContainers, sTranspDocs, sqlText: String;&#13;&#10;&#13;&#10;  bPrintTranspDocs, bPrintTransport, bPrintContainers, bExportExcel : Boolean;                                                                          &#13;&#10;                          &#13;&#10;&#13;&#10;procedure btnOKOnClick(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  bPrintTranspDocs := cbxPrintTranspDocs.Checked;                                                                                                                      &#13;&#10;  bPrintTransport := cbxPrintTransport.Checked;&#13;&#10;  bPrintContainers := cbxPrintContainers.Checked;&#13;&#10;  bExportExcel := cbxExportExcel.Checked;                                                                                        &#13;&#10;&#13;&#10;  qryComms.Active    := True;    &#13;&#10;  qRelSum.Active     := True;    &#13;&#10;  qTransport.Active  := True;                              &#13;&#10;  qContainers.Active := True;  &#13;&#10;  qTranspDocs.Active := True;&#13;&#10;    &#13;&#10;  if cbxG082.Text &#60;&#62; 'Все получатели (без фильтра)' then&#13;&#10;    begin&#13;&#10;      sqlText := 'SELECT KM.NBD,KM.BD_DATE,KM.REG_NBD,KM.G082,' +    &#13;&#10;                 'KC.BOXNO,KC.G33,KC.G312,KC.G311,KC.G35,KC.G42,' +       &#13;&#10;                 'KC.G42_CURRENCY,KC.STORAGE_DATE,KC.PLACEID,KC.ID,KC.G32 ' +&#13;&#10;                 'FROM KRD_COMM AS KC ' +          &#13;&#10;                 'INNER JOIN KRD_MAIN AS KM ON KM.PLACEID=KC.PLACEID AND KM.ID=KC.ID ' +                      &#13;&#10;                 'WHERE (KC.PLACEID=:placeid) ' +&#13;&#10;                 'AND (KM.G082=:g082) ' +                          &#13;&#10;                 'AND (KM.IN_ARCHIVE &#60;&#62; 1 OR KM.IN_ARCHIVE IS NULL)';                                        &#13;&#10;      qryComms.SQL.Text := sqlText;&#13;&#10;      qryComms.ParamByName('g082').DataType := 1;                                                      &#13;&#10;      qryComms.ParamByName('g082').Value := cbxG082.Text;&#13;&#10;    end;                                 &#13;&#10;    &#13;&#10;  qryComms.ParamByName('placeid').Value := vPlaceId;&#13;&#10;  qryComms.Reopen;                                &#13;&#10;             &#13;&#10;  WriteIniFile('rpt_rests', 'PlaceId', vPlaceId);&#13;&#10;  WriteIniFile('rpt_rests', 'LicenceNo', vLicenceNo);&#13;&#10;  WriteIniFile('rpt_rests', 'LicenceDate', vLicenceDate);&#13;&#10;  WriteIniFile('rpt_rests', 'LicenceType', iLicenceType);      &#13;&#10;  WriteIniFile('rpt_rests', 'SvhName', vSvhName);&#13;&#10;  WriteIniFile('rpt_rests', 'PrintTransport', bPrintTransport);&#13;&#10;  WriteIniFile('rpt_rests', 'PrintContainers', bPrintContainers);&#13;&#10;  WriteIniFile('rpt_rests', 'PrintTranspDocs', bPrintTranspDocs);&#13;&#10;  WriteIniFile('rpt_rests', 'ExportExcel', bExportExcel);                                                                                                          &#13;&#10;end;&#13;&#10;&#13;&#10;procedure sbtnSelectLicOnClick(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;   COMPUTE('GLOBALVAR (&#34;vPlaceId&#34;, Integer, 0)');&#13;&#10;   COMPUTE('GLOBALVAR (&#34;vLicenceNo&#34;, String)');&#13;&#10;   COMPUTE('GLOBALVAR (&#34;vLicenceDate&#34;, String)');&#13;&#10;   COMPUTE('GLOBALVAR (&#34;iLicenceType&#34;, Integer)');       &#13;&#10;   COMPUTE('GLOBALVAR (&#34;vSvhName&#34;, String)');                                                   &#13;&#10;   iSelected := COMPUTE('SelectValues(&#34;Выбор складской площадки&#34;, &#34;STORES_2&#34;, [[&#34;LICENCENO&#34;, &#34;№ лицензии&#34;, 25], [&#34;STORE_NO&#34;, &#34;№ склада&#34;, 10], [&#34;NAME&#34;, &#34;Название склада&#34;, 40]], [[&#34;PLACEID&#34;, &#34;vPlaceId&#34;], [&#34;LICENCENO&#34;, &#34;vLicenceNo&#34;], [&#34;LICENCEDATE&#34;, &#34;vLicenceDate&#34;], [&#34;NAME&#34;, &#34;vSvhName&#34;], [&#34;LICENCETYPE&#34;, &#34;iLicenceType&#34;]], &#34;PLACEID&#34;, &#34;STS_DB&#34;)');&#13;&#10;&#13;&#10;   if iSelected = 1 then&#13;&#10;     begin                            &#13;&#10;       vPlaceId := COMPUTE (&#34;vPlaceId&#34;);&#13;&#10;       vLicenceNo := COMPUTE (&#34;vLicenceNo&#34;);&#13;&#10;       vLicenceDate := COMPUTE (&#34;vLicenceDate&#34;);&#13;&#10;       iLicenceType := COMPUTE (&#34;iLicenceType&#34;);                                                                                &#13;&#10;       vSvhName := COMPUTE (&#34;vSvhName&#34;);&#13;&#10;&#13;&#10;       edtLicNo.Text := vLicenceNo;       &#13;&#10;       lblLicDate.Caption := vLicenceDate;       &#13;&#10;       edtSvhName.Text := vSvhName;&#13;&#10;     end;&#13;&#10;&#13;&#10;  btnOK.Enabled := length(vLicenceNo) &#60;&#62; 0;    &#13;&#10;end;&#13;&#10;&#13;&#10;procedure OnStartReport(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  vPlaceId := IniFile('rpt_rests', 'PlaceId', 0);&#13;&#10;  vLicenceNo := IniFile('rpt_rests', 'LicenceNo', '');&#13;&#10;  vLicenceDate := IniFile('rpt_rests', 'LicenceDate', '');&#13;&#10;  iLicenceType := IniFile('rpt_rests', 'LicenceType', '1');                                                                             &#13;&#10;  vSvhName := IniFile('rpt_rests', 'SvhName', '');&#13;&#10;&#13;&#10;  bExportExcel := IniFile('rpt_rests', 'ExportExcel', 'False');                                       &#13;&#10;&#13;&#10;  bPrintTransport := IniFile('rpt_rests', 'PrintTransport', 'False');     &#13;&#10;  bPrintContainers := IniFile('rpt_rests', 'PrintContainers', 'False');     &#13;&#10;  bPrintTranspDocs := IniFile('rpt_rests', 'PrintTranspDocs', 'False');                                                       &#13;&#10;end;&#13;&#10;&#13;&#10;procedure DialogPage1OnActivate(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  edtLicNo.Text := vLicenceNo;&#13;&#10;  edtSvhName.Text := vSvhName;&#13;&#10;  lblLicDate.Caption := vLicenceDate;&#13;&#10;&#13;&#10;  cbxPrintTransport.Checked := bPrintTransport;&#13;&#10;  cbxPrintContainers.Checked := bPrintContainers;&#13;&#10;  cbxPrintTranspDocs.Checked := bPrintTranspDocs;                                                                                                                 &#13;&#10;&#13;&#10;  btnOK.Enabled := length(vLicenceNo) &#60;&#62; 0;&#13;&#10;    &#13;&#10;  cbxG082.Clear;             &#13;&#10;  cbxG082.Items.Add('Все получатели (без фильтра)');&#13;&#10;  cbxG082.ItemIndex := 0;                                                   &#13;&#10;  while not qG082.EOF do&#13;&#10;    begin                         &#13;&#10;      cbxG082.Items.Add(&#60;qG082.&#34;G082&#34;&#62;);&#13;&#10;      qG082.Next;                                          &#13;&#10;    end;                            &#13;&#10;end;&#13;&#10;&#13;&#10;procedure MasterData1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  // проверка есть ли остатки или выдан полностью                                     &#13;&#10;  iRestG311 := iif(ValidInt(&#60;qryComms.&#34;G311&#34;&#62;), StrToInt (&#60;qryComms.&#34;G311&#34;&#62;), 0) - iif(ValidInt(&#60;qRelSum.&#34;SUM_REL_G311&#34;&#62;), StrToInt (&#60;qRelSum.&#34;SUM_REL_G311&#34;&#62;), 0);&#13;&#10;  dRestG35 := iif(ValidFloat(&#60;qryComms.&#34;G35&#34;&#62;), StrToFloat (&#60;qryComms.&#34;G35&#34;&#62;), 0) - iif(ValidFloat(&#60;qRelSum.&#34;SUM_REL_G35&#34;&#62;), StrToFloat (&#60;qRelSum.&#34;SUM_REL_G35&#34;&#62;), 0);&#13;&#10;  dRestG42 := iif(ValidFloat(&#60;qryComms.&#34;G42&#34;&#62;), StrToFloat (&#60;qryComms.&#34;G42&#34;&#62;), 0) - iif(ValidFloat(&#60;qRelSum.&#34;SUM_REL_G42&#34;&#62;), StrToFloat (&#60;qRelSum.&#34;SUM_REL_G42&#34;&#62;), 0);        &#13;&#10;&#13;&#10;  iSumRestG311 := iSumRestG311 + iRestG311;&#13;&#10;  dSumRestG35 := dSumRestG35 + dRestG35;&#13;&#10;  dSumRestG42 := dSumRestG42 + dRestG42;      &#13;&#10;        &#13;&#10;  if (iRestG311 = 0) and (dRestG35 = 0) and (dRestG42 = 0) then&#13;&#10;    MasterData1.Visible := False        &#13;&#10;  else&#13;&#10;    begin      &#13;&#10;      MasterData1.Visible := True;&#13;&#10;      iLine := iLine + 1;&#13;&#10;&#13;&#10;      // гр. места&#13;&#10;      if (&#60;qRelSum.&#34;SUM_REL_G311&#34;&#62; = null) or (&#60;qRelSum.&#34;SUM_REL_G311&#34;&#62; = 0) then                                                     &#13;&#10;        begin&#13;&#10;         mG311Output.Visible := False;                 &#13;&#10;         mG311Input.StretchMode := smMaxHeight;                                                                                      &#13;&#10;        end&#13;&#10;      else&#13;&#10;        begin&#13;&#10;          mG311Output.Visible := True;&#13;&#10;          mG311Input.StretchMode := smDontStretch;                                                                        &#13;&#10;        end;            &#13;&#10;   &#13;&#10;      // брутто                                       &#13;&#10;      if (&#60;qRelSum.&#34;SUM_REL_G35&#34;&#62; = null) or (&#60;qRelSum.&#34;SUM_REL_G35&#34;&#62; = 0) then                                                     &#13;&#10;        begin&#13;&#10;          mG35Output.Visible := False;                 &#13;&#10;          mG35Input.StretchMode := smMaxHeight;                       &#13;&#10;        end&#13;&#10;      else&#13;&#10;        begin&#13;&#10;          mG35Output.Visible := True;&#13;&#10;          mG35Input.StretchMode := smDontStretch;                                                                        &#13;&#10;        end;                  &#13;&#10;  &#13;&#10;      // стоимость&#13;&#10;      if (&#60;qRelSum.&#34;SUM_REL_G42&#34;&#62; = null) or (&#60;qRelSum.&#34;SUM_REL_G42&#34;&#62; = 0) then                                                     &#13;&#10;        begin&#13;&#10;          mG42Output.Visible := False;                 &#13;&#10;          mG42Input.StretchMode := smMaxHeight;                       &#13;&#10;        end&#13;&#10;      else&#13;&#10;        begin&#13;&#10;          mG42Output.Visible := True;&#13;&#10;          mG42Input.StretchMode := smDontStretch;                                                                        &#13;&#10;        end;                      &#13;&#10;    end;&#13;&#10;      &#13;&#10;  if COMPUTE ('STOPPRESSED ()') then &#13;&#10;    begin&#13;&#10;      COMPUTE ('HIDEINFORMATION ()');                                            &#13;&#10;      RaiseException('Построение отчета отменено пользователем');&#13;&#10;    end;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure ReportTitle1OnAfterPrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  COMPUTE ('SHOWINFORMATION (&#34;Пожалуйста, подождите..&#34;)');&#13;&#10;    &#13;&#10;  if bPrintTransport then&#13;&#10;    begin&#13;&#10;      if not bPrintContainers then&#13;&#10;        mTransport.StretchMode := smMaxHeight;  &#13;&#10;    end  &#13;&#10;  else&#13;&#10;    begin&#13;&#10;      mTransport.Visible := False;            &#13;&#10;      mTransport.Height := 0;                                                                         &#13;&#10;    end;  &#13;&#10;                &#13;&#10;  if not bPrintContainers then&#13;&#10;    begin&#13;&#10;      mContainers.Visible := False;            &#13;&#10;      mContainers.Height := 0;&#13;&#10;    end;&#13;&#10;&#13;&#10;  if not bPrintTranspDocs then&#13;&#10;    begin&#13;&#10;      mTranspDocs.Visible := False;&#13;&#10;      mContainers.Height := 0;&#13;&#10;      mRegNBD.StretchMode := smMaxHeight;          &#13;&#10;    end;    &#13;&#10;end;&#13;&#10;&#13;&#10;procedure ReportTitle1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin                                                                                                          &#13;&#10;  Page.EndlessHeight := bExportExcel;&#13;&#10;&#13;&#10;  case iLicenceType of&#13;&#10;       1: begin &#13;&#10;               sWHType := 'СВХ';&#13;&#10;               sWHType1 := 'СВХ';                                                 &#13;&#10;               sDocType := 'Свидетельство о включении в реестр владельцев СВХ от';&#13;&#10;          end;                                                       &#13;&#10;       2: begin&#13;&#10;               sWHType := 'СВХ';&#13;&#10;               sWHType1 := 'СВХ';                   &#13;&#10;               sDocType := 'Свидетельство о включении в реестр владельцев СВХ от';                   &#13;&#10;          end;                                  &#13;&#10;       3: begin&#13;&#10;               sWHType := 'Иное место временного хранения';&#13;&#10;               sWHType1 := 'иного места';                   &#13;&#10;               sDocType := 'Разрешение ТО на временное хранение в ином месте от';                   &#13;&#10;          end;                           &#13;&#10;       4: begin &#13;&#10;               sWHType := 'ПЗТК';&#13;&#10;               sWHType1 := 'ПЗТК';                   &#13;&#10;               sDocType := 'Приказ(разрешение) таможни от';                   &#13;&#10;          end;                         &#13;&#10;       5: begin &#13;&#10;               sWHType := 'ВЗТК';&#13;&#10;               sWHType1 := 'ВЗТК';                   &#13;&#10;               sDocType := 'Приказ(разрешение) таможни от';                   &#13;&#10;          end;&#13;&#10;   end;                                                   &#13;&#10;end;&#13;&#10;&#13;&#10;procedure mTransportOnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  sTransport := '';                                                   &#13;&#10;  if bPrintTransport then&#13;&#10;    begin&#13;&#10;      while not qTransport.EOF do  &#13;&#10;        begin&#13;&#10;          if Length(sTransport) = 0 then&#13;&#10;            begin&#13;&#10;              sTransport := &#60;qTransport.&#34;CARNO&#34;&#62;;                                                                                                     &#13;&#10;            end&#13;&#10;          else&#13;&#10;            begin&#13;&#10;              sTransport := sTransport + ', ' + &#60;qTransport.&#34;CARNO&#34;&#62;;                                                                                                                                                               &#13;&#10;            end;&#13;&#10;          qTransport.NEXT;                                                              &#13;&#10;        end;                  &#13;&#10;    end;              &#13;&#10;end;&#13;&#10;&#13;&#10;procedure mContainersOnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  sContainers := '';                                                   &#13;&#10;  if bPrintContainers then&#13;&#10;    begin&#13;&#10;      while not qContainers.EOF do  &#13;&#10;        begin&#13;&#10;          if Length(sContainers) = 0 then&#13;&#10;            begin&#13;&#10;              sContainers := &#60;qContainers.&#34;CONTNO&#34;&#62;;                                                                                                     &#13;&#10;            end&#13;&#10;          else&#13;&#10;            begin&#13;&#10;              sContainers := sContainers + ', ' + &#60;qContainers.&#34;CONTNO&#34;&#62;;                                                                                                                                                               &#13;&#10;            end;&#13;&#10;          qContainers.NEXT;                                                              &#13;&#10;        end;                  &#13;&#10;    end;  &#13;&#10;end;&#13;&#10;&#13;&#10;procedure mTranspDocsOnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  sTranspDocs := '';                                                   &#13;&#10;  if bPrintTranspDocs then&#13;&#10;    begin&#13;&#10;      while not qTranspDocs.EOF do  &#13;&#10;        begin&#13;&#10;          if Length(sTranspDocs) = 0 then&#13;&#10;            begin&#13;&#10;              sTranspDocs := &#60;qTranspDocs.&#34;PAPERNAME&#34;&#62; + ' № ' + &#60;qTranspDocs.&#34;PAPERNO&#34;&#62; + ' от ' + FormatDateTime('DD.MM.YYYY', &#60;qTranspDocs.&#34;PAPERDATE&#34;&#62;);                                                                                                     &#13;&#10;            end&#13;&#10;          else&#13;&#10;            begin&#13;&#10;              sTranspDocs := sTranspDocs + ', ' + &#60;qTranspDocs.&#34;PAPERNAME&#34;&#62; + ' № ' + &#60;qTranspDocs.&#34;PAPERNO&#34;&#62; + ' от ' + FormatDateTime('DD.MM.YYYY', &#60;qTranspDocs.&#34;PAPERDATE&#34;&#62;);                                                                                                                                                               &#13;&#10;            end;&#13;&#10;          qTranspDocs.NEXT;                                                              &#13;&#10;        end;                  &#13;&#10;    end;  &#13;&#10;end;&#13;&#10;&#13;&#10;procedure OnStopReport(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  COMPUTE ('HIDEINFORMATION ()');         &#13;&#10;end;&#13;&#10;&#13;&#10;begin&#13;&#10;&#13;&#10;end." OnStartReport="OnStartReport" OnStopReport="OnStopReport"