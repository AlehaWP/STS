<?xml version="1.0" encoding="utf-8" standalone="no"?><TfrxReport Version="4.6.78" DotMatrixReport="False" EngineOptions.ConvertNulls="False" EngineOptions.DoublePass="True" IniFile="\Software\Fast Reports" PreviewOptions.Buttons="4095" PreviewOptions.Zoom="1" PrintOptions.Printer="По умолчанию" PrintOptions.PrintOnSheet="0" ReportOptions.CreateDate="38262.5249943866" ReportOptions.Description.Text="" ReportOptions.Name="Форма ДО4мв (Пр.№ 958 от 03.09.2003)" ReportOptions.LastChange="40661.4523010764" ReportOptions.VersionMajor="2" ReportOptions.VersionMinor="0" ReportOptions.VersionRelease="0" ScriptLanguage="PascalScript" ScriptText.Text="Var&#13;&#10;  bStorExists, vTDByComms, PrintFirstAll, vExport: Boolean;&#13;&#10;  vType, StoreDocsBlock, AistCompatible, CustomDocsOnly,&#13;&#10;  NumLines, MAINID, MainG32, CountPapers: Integer;&#13;&#10;  datestart, dateend, IncludeAllDO2, NotInclude, inStore, PrintFactIn,&#13;&#10;  onCurrBook, vPlaceId, vChief, sStoreKeeper,NumCustQuery,&#13;&#10;  sTempDo2Comm, sqlText,LicNum, StorName, ReportPeriod,&#13;&#10;  ss, sss, Text1, Tex_t, vBegKeep, vNumLine, nuv,&#13;&#10;  nG32, nG312, nG33, nG42, vG311, vrG311, vrG42, vOutDate,&#13;&#10;  vrG35, vG35, sValBukCode, vReleaseDoc, TransDocNames, sStoreNum,&#13;&#10;  vG082, vBegKeep1, vBegKeep2, vFilterCrit, NttN: String;&#13;&#10;  dtPeriodStart, dtPeriodEnd, PeriodStart, dtNumCustQuery: TDateTime;&#13;&#10;  vLicenceDate: Variant;&#13;&#10;  sTypeSVH, sTypeSVH1, sNameSVH, sTypeDoc, sDateDoc, sNumDoc, sConts, sConstStore: string;&#13;&#10;&#13;&#10;  bBrutto, bUnits, bVolume: Boolean;&#13;&#10;&#13;&#10;  bUseFactInfo, bAllowSplit: Boolean;&#13;&#10;  vUseStoreDocs: Boolean;&#13;&#10;&#13;&#10;  bSortByNBD, bSortByBdDate, bSortByBegKeep: Boolean;&#13;&#10;  bPartInPlace, bPartOutPlace, bStorageDateNoPrint    : Boolean;&#13;&#10;    &#13;&#10;  bGoodStoreNumKPS, bPrintContType, bUseConts, bPrintPlomb: Boolean;      &#13;&#10;&#13;&#10;  sPartInPlaceText,sPartOutPlaceText                           : String;  &#13;&#10;  sPaperNames, sPaperCodes: String;&#13;&#10;  DO4Date :String;      &#13;&#10;  bLinks : Boolean;                                          &#13;&#10;procedure Page1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  COMPUTE(' globalvar (&#34;strStoreNumLic&#34;,String)');&#13;&#10;  COMPUTE(' globalvar (&#34;strStoreNum&#34;,String)');&#13;&#10;  COMPUTE(' globalvar (&#34;strStoreName&#34;,String)');&#13;&#10;  COMPUTE(' globalvar (&#34;strLicExt&#34;,String)');&#13;&#10;  COMPUTE(' globalvar (&#34;sPlaceId&#34;,Integer)');&#13;&#10;  COMPUTE(' globalvar (&#34;sChief&#34;,string)');&#13;&#10;  COMPUTE(' globalvar (&#34;sLicenceDate&#34;,DateTime)');&#13;&#10;  vtype:=0;       {переменная для определения какой тип мера определения товара&#13;&#10;                 1 - объем&#13;&#10;                 2 - доп. един.&#13;&#10;                 3 - брутто}&#13;&#10;&#13;&#10;  bSTORexists := False;&#13;&#10;  //установка  периода&#13;&#10;  dtFrom.Enabled := True;&#13;&#10;  dtTo.Enabled := True;&#13;&#10;&#13;&#10;  DateDO4.Date := Date;                                                              &#13;&#10;&#13;&#10;  if COMPUTE('INIFILE(&#34;Print&#34;,&#34;TDByComms&#34;,&#34;False&#34;)') = 'True' then&#13;&#10;    cbTDByComms.Checked := True&#13;&#10;  else&#13;&#10;    cbTDByComms.Checked := False;&#13;&#10;&#13;&#10;  rbtnPeriod.Checked := True;&#13;&#10;&#13;&#10;  datestart := COMPUTE('INIFILE(&#34;Print&#34;,&#34;LicenceDate&#34;,&#34;&#34;)');&#13;&#10;  fldStoreLicDate.Date:= STRTODATEDEF(datestart, DATE);&#13;&#10;&#13;&#10;  datestart:=COMPUTE('INIFILE(&#34;Print&#34;,&#34;PeriodStart&#34;,&#34;&#34;)');&#13;&#10;  dateend := COMPUTE('INIFILE(&#34;Print&#34;,&#34;PeriodEnd&#34;,&#34;&#34;)');&#13;&#10;  dtFrom.Date := STRTODATEDEF(datestart, DATE);&#13;&#10;  dtTo.Date := STRTODATEDEF(dateend, DATE);&#13;&#10;&#13;&#10;  IncludeAllDO2 := Compute('INIFILE(&#34;Print&#34;,&#34;IncludeAllDO2&#34;,&#34;1&#34;)');&#13;&#10;  if IncludeAllDO2 = '0' then&#13;&#10;    chkDO2.Checked := True&#13;&#10;  else&#13;&#10;    chkDO2.Checked := False;&#13;&#10;&#13;&#10;  // не включать до1 оформленные ранее&#13;&#10;  cbxNotInclude.Enabled := True;&#13;&#10;&#13;&#10;  NotInclude := COMPUTE('INIFILE(&#34;Print&#34;,&#34;NotInclude&#34;,&#34;1&#34;)');&#13;&#10;  If NotInclude = '0' then&#13;&#10;    cbxNotInclude.Checked := False&#13;&#10;  else&#13;&#10;    cbxNotInclude.Checked := True  ;&#13;&#10;&#13;&#10;  // включать до1 оформленные ранее  и не выпущенные&#13;&#10;  cbxStore.Enabled := True;&#13;&#10;&#13;&#10;  inStore := COMPUTE('INIFILE(&#34;Print&#34;,&#34;InStore&#34;,&#34;0&#34;)');&#13;&#10;  If inStore = '0' then&#13;&#10;    cbxStore.Checked := False&#13;&#10;  else&#13;&#10;    cbxStore.Checked := True;&#13;&#10;&#13;&#10;  // печать по текущей книге&#13;&#10;  onCurrBook:=  COMPUTE('INIFILE(&#34;Print&#34;,&#34;OnCurrentBook&#34;,&#34;0&#34;)');&#13;&#10;  if onCurrBook = '0' then&#13;&#10;    rbtnBook.Checked := False&#13;&#10;  else&#13;&#10;    rbtnBook.Checked := True;&#13;&#10;&#13;&#10;  chkPartInPlace.Checked := bPartInPlace;&#13;&#10;  edPartInPlaceText.Text := sPartInPlaceText;&#13;&#10;  chkPartOutPlace.Checked := bPartOutPlace;&#13;&#10;  edPartOutPlaceText.Text := sPartOutPlaceText;&#13;&#10;&#13;&#10;  chkGoodStoreNumKPS.Checked := bGoodStoreNumKPS;  &#13;&#10;    &#13;&#10;  chkUseConts.Checked := bUseConts;&#13;&#10;  &#13;&#10;  chkPrintPlomb.Checked := bPrintPlomb;&#13;&#10;  chkPrintPlomb.Enabled := bUseConts;&#13;&#10;    &#13;&#10;  chkPrintContType.Checked := bPrintContType;&#13;&#10;  chkPrintContType.Enabled := bUseConts;    &#13;&#10;&#13;&#10;  chkStorageDateNoPrint.Checked := bStorageDateNoPrint;&#13;&#10;    &#13;&#10;  chkBrutto.Checked := bBrutto;&#13;&#10;  chkUnits.Checked := bUnits;&#13;&#10;  chkVolume.Checked := bVolume;&#13;&#10;&#13;&#10;  // сортировка&#13;&#10;  bSortByBegKeep := IniFile('Print', 'SortBy', '1') = '1';&#13;&#10;  bSortByBdDate := IniFile('Print', 'SortBy', '1') = '2';&#13;&#10;  bSortByNBD := IniFile('Print', 'SortBy', '1') = '3';&#13;&#10;&#13;&#10;  rbtnSortByBegKeep.Checked := bSortByBegKeep;&#13;&#10;  rbtnSortByBdDate.Checked := bSortByBdDate;&#13;&#10;  rbtnSortByNBD.Checked := bSortByNBD;&#13;&#10;&#13;&#10;  // проверка, открыта ли книга учета&#13;&#10;  If Not BookOpened then begin&#13;&#10;    rbtnPeriod.Checked := True;&#13;&#10;    rbtnBook.Enabled := False&#13;&#10;  end;&#13;&#10;&#13;&#10;  // уполномоченный работник склада&#13;&#10;  StoreKeep.Text  :=  COMPUTE('INIFILE(&#34;Print&#34;,&#34;StoreKeeper&#34;,&#34; &#34;)');&#13;&#10;&#13;&#10;  chkAllowSplit.Checked := bAllowSplit;                                        &#13;&#10;&#13;&#10;  //установка склада&#13;&#10;  fldStoreLicNum.Text := '';&#13;&#10;  //fldStoreNum.Text := '';&#13;&#10;  lblStoreName.Text := '';&#13;&#10;  fldLicExt.Text := '';&#13;&#10;&#13;&#10;  vPlaceId:=COMPUTE('INIFILE(&#34;Print&#34;,&#34;PlaceId&#34;,&#34;&#34;)');&#13;&#10;  If COMPUTE('FindKey(&#34;STORES_2&#34;, [&#34;' + VarToStr(vPlaceId) + '&#34;])') = '1' then begin&#13;&#10;    fldStoreLicNum.Text := &#60;STORES_2.&#34;LICENCENO&#34;&#62; ;      //   номер лицензии&#13;&#10;    lblStoreName.Text := &#60;STORES_2.&#34;NAME&#34;&#62; ;     //   название склада&#13;&#10;    fldLicExt.Text := &#60;STORES_2.&#34;LICENCENO_EXT&#34;&#62;;     //   Признак перерегистрации&#13;&#10;    vChief:=  &#60;STORES_2.&#34;CHIEF&#34;&#62; ;&#13;&#10;    vLicenceDate := &#60;STORES_2.&#34;LICENCEDATE&#34;&#62;;&#13;&#10;    cbtnOk.Enabled := True;&#13;&#10;  end&#13;&#10;  else begin&#13;&#10;    vPlaceId:=COMPUTE('INIFILE(&#34;WorkPlace&#34;,&#34;PlaceNum&#34;,&#34;&#34;)');&#13;&#10;    If COMPUTE('FindKey(&#34;STORES_2&#34;, [&#34;' + VarToStr(vPlaceId) + '&#34;])') = '1' then begin&#13;&#10;      fldStoreLicNum.Text := &#60;STORES_2.&#34;LICENCENO&#34;&#62; ;      //   номер лицензии&#13;&#10;      lblStoreName.Text := &#60;STORES_2.&#34;NAME&#34;&#62; ;     //   название склада&#13;&#10;      fldLicExt.Text := &#60;STORES_2.&#34;LICENCENO_EXT&#34;&#62;;     //   Признак перерегистрации&#13;&#10;      vChief:=  &#60;STORES_2.&#34;CHIEF&#34;&#62; ;&#13;&#10;      vLicenceDate := &#60;STORES_2.&#34;LICENCEDATE&#34;&#62;;&#13;&#10;      cbtnOk.Enabled := True;&#13;&#10;    end&#13;&#10;    else&#13;&#10;      cbtnOk.Enabled := False;&#13;&#10;  end;&#13;&#10;&#13;&#10;  If Compute('INIFILE(&#34;Print&#34;,&#34;LicenceNum&#34;,&#34;&#34;)') &#60;&#62; '' then&#13;&#10;    fldStoreLicNum.Text := Compute('INIFILE(&#34;Print&#34;,&#34;LicenceNum&#34;,&#34;&#34;)');&#13;&#10;  If Compute('INIFILE(&#34;Print&#34;,&#34;StoreName&#34;,&#34;&#34;)') &#60;&#62; '' then&#13;&#10;    lblStoreName.Text := Compute('INIFILE(&#34;Print&#34;,&#34;StoreName&#34;,&#34;&#34;)');&#13;&#10;&#13;&#10;  if vUseStoreDocs then&#13;&#10;    rbtnInfoFact.Caption := 'фактические сведения (из складского документа учета принятия груза на временное хранение)';&#13;&#10;&#13;&#10;  rbtnInfoFact.Checked := bUseFactInfo;&#13;&#10;  rbtnInfoDoc.Checked := not bUseFactInfo;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure cbtnOkOnBeforePrint(Sender: TfrxComponent);&#13;&#10;Var&#13;&#10;  sOrderBy: String;&#13;&#10;  sSortBy: String;&#13;&#10;begin&#13;&#10;  IF UseDateDO4.Checked then DO4Date := FORMATDATETIME('DD/MM/YYYY HH:NN', DateDO4.Date + Time)&#13;&#10;  else DO4Date := FORMATDATETIME('DD/MM/YYYY HH:NN', DATE + TIME);&#13;&#10;  bLinks := False;                                    &#13;&#10;    &#13;&#10;  dtPeriodEnd := Trunc(dtTo.Date) + STRTOTIME('23:59:59');&#13;&#10;  //Проверим галку &#34;Не включать ранее периода&#34;&#13;&#10;  //if cbxNotInclude.Checked = True then&#13;&#10;  dtPeriodStart := Trunc(dtFrom.Date) + STRTOTIME('00:00:00');&#13;&#10;  // else&#13;&#10;  // dtPeriodStart := STRTODATE('01.01.1900') + STRTOTIME('00:00:01');&#13;&#10;  //Запомним начало периода отчета&#13;&#10;  PeriodStart :=Trunc( dtFrom.Date) + STRTOTIME('00:00:00');&#13;&#10;&#13;&#10;  // &#34;веса&#34;&#13;&#10;  bBrutto := chkBrutto.Checked;&#13;&#10;  bUnits := chkUnits.Checked;&#13;&#10;  bVolume := chkVolume.Checked;&#13;&#10;&#13;&#10;  COMPUTE('WRITEINIFILE(&#34;Print&#34;, &#34;DO4mvBrutto&#34;, &#34;' + VarToStr(bBrutto) + '&#34;)');&#13;&#10;  COMPUTE('WRITEINIFILE(&#34;Print&#34;, &#34;DO4mvUnits&#34;, &#34;' + VarToStr(bUnits) + '&#34;)');&#13;&#10;  COMPUTE('WRITEINIFILE(&#34;Print&#34;, &#34;DO4mvVolume&#34;, &#34;' + VarToStr(bVolume) + '&#34;)');&#13;&#10;&#13;&#10;  bUseFactInfo := rbtnInfoFact.Checked;&#13;&#10;&#13;&#10;  bPartInPlace := chkPartInPlace.Checked;&#13;&#10;  sPartInPlaceText := edPartInPlaceText.Text;&#13;&#10;  bPartOutPlace := chkPartOutPlace.Checked;&#13;&#10;  sPartOutPlaceText := edPartOutPlaceText.Text;&#13;&#10;&#13;&#10;  bUseConts := chkUseConts.Checked;      &#13;&#10;  bPrintContType := chkPrintContType.Checked;                &#13;&#10;  bPrintPlomb :=  chkPrintPlomb.Checked;        &#13;&#10;  bGoodStoreNumKPS := chkGoodStoreNumKPS.Checked;  &#13;&#10;     &#13;&#10;    &#13;&#10;  bStorageDateNoPrint := chkStorageDateNoPrint.Checked;&#13;&#10;  COMPUTE('WRITEINIFILE(&#34;Print&#34;, &#34;StorageDateNoPrint&#34;, &#34;' + VarToStr(bStorageDateNoPrint) + '&#34;)');&#13;&#10;    &#13;&#10;  bSortByNBD := rbtnSortByNBD.Checked;&#13;&#10;  bSortByBdDate := rbtnSortByBdDate.Checked;&#13;&#10;  bSortByBegKeep := rbtnSortByBegKeep.Checked;&#13;&#10;&#13;&#10;  sSortBy := iif(bSortByNBD, '3', iif(bSortByBdDate, '2', '1'));&#13;&#10;&#13;&#10;  COMPUTE('WRITEINIFILE(&#34;Print&#34;, &#34;SortBy&#34;, &#34;' + sSortBy + '&#34;)');&#13;&#10;&#13;&#10;  // номер требования таможни&#13;&#10;  NumCustQuery  := edtNumCust.Text;&#13;&#10;  dtNumCustQuery  := dtNumCust.Date;&#13;&#10;&#13;&#10;  // работник склада&#13;&#10;  sStoreKeeper := StoreKeep.Text;&#13;&#10;  Compute('WRITEINIFILE(&#34;Print&#34;,&#34;StoreKeeper&#34;,&#34;'+sStoreKeeper+'&#34;)');&#13;&#10;    &#13;&#10;  // AllowSplit                     &#13;&#10;  bAllowSplit := chkAllowSplit.Checked;&#13;&#10;&#13;&#10;  // export&#13;&#10;  vExport := cbExport.Checked;      &#13;&#10;    &#13;&#10;  //Объявляем переменные&#13;&#10;  NumLines := 0;&#13;&#10;  MAINID := 0;&#13;&#10;  MainG32 := 0;&#13;&#10;  PrintFirstAll := True;&#13;&#10;&#13;&#10;  if rbtnBook.Checked = True then begin&#13;&#10;    sTempDo2Comm := CREATEDO3COMM(StrToInt(vPlaceId), dtPeriodStart, dtPeriodEnd,'', 2);&#13;&#10;  end&#13;&#10;  else if cbxNotInclude.Enabled = True then begin&#13;&#10;    if cbxNotInclude.Checked = True then&#13;&#10;      sTempDo2Comm := CREATEDO3COMM(StrToInt(vPlaceId), dtPeriodStart, dtPeriodEnd,'', 1)&#13;&#10;    else&#13;&#10;      sTempDo2Comm := CREATEDO3COMM(StrToInt(vPlaceId), dtPeriodStart, dtPeriodEnd,'');&#13;&#10;  end&#13;&#10;  else&#13;&#10;    sTempDo2Comm := CREATEDO3COMM(StrToInt(vPlaceId), dtPeriodStart, dtPeriodEnd,'', 3);&#13;&#10;                             &#13;&#10;  if bSortByNBD then&#13;&#10;    sOrderBy := ' ORDER BY NBD, ACCEPTDATE, GN, OUT_DATE'&#13;&#10;  else if bSortByBdDate then&#13;&#10;    sOrderBy := ' ORDER BY BD_DATE, NBD, GN, OUT_DATE'&#13;&#10;  else&#13;&#10;    sOrderBy := ' ORDER BY ACCEPTDATE, NBD, GN, OUT_DATE';&#13;&#10;&#13;&#10;  if rbtnBook.Checked = True then begin&#13;&#10;&#13;&#10;    If chkDO1.Checked = true then&#13;&#10;       sqlText:='select  *  from '+ CHR(39)+sTempDo2Comm+CHR(39)+' WHERE (A_MODE = 7) ORDER BY SHOW_NBD, BEG_KEEP, NBD, GN, OUT_DATE '&#13;&#10;    else&#13;&#10;       sqlText:='select  *  from '+ CHR(39)+sTempDo2Comm+CHR(39)+ sOrderBy&#13;&#10;&#13;&#10;  end&#13;&#10;  else if bSTORexists = False then begin&#13;&#10;&#13;&#10;    If chkDO1.Checked = true then&#13;&#10;       sqlText:='select  *  from '+ CHR(39)+sTempDo2Comm+CHR(39)+' WHERE (A_MODE = 7) ORDER BY BEG_KEEP, NBD , GN, OUT_DATE '&#13;&#10;    else&#13;&#10;       sqlText:='select  *  from '+ CHR(39)+sTempDo2Comm+CHR(39)+ sOrderBy;&#13;&#10;&#13;&#10;  end&#13;&#10;  else begin&#13;&#10;&#13;&#10;    COMPUTE('FIRST(&#34;STOR&#34;)');&#13;&#10;&#13;&#10;    vPlaceId := COMPUTE('FieldValue(&#34;Stor&#34;, &#34;PLACEID&#34;)');&#13;&#10;&#13;&#10;    If chkDO1.Checked = true then&#13;&#10;      sqlText:='select  *  from '+ CHR(39)+sTempDo2Comm+CHR(39)+' WHERE (A_MODE = 7) AND ((PLACEID= '+ vPlaceId+') '&#13;&#10;    else&#13;&#10;      sqlText:='select  *  from '+ CHR(39)+sTempDo2Comm+CHR(39)+' WHERE ((PLACEID= '+ vPlaceId+') ';&#13;&#10;&#13;&#10;    COMPUTE('Next(&#34;STOR&#34;)');&#13;&#10;&#13;&#10;    while COMPUTE('EOF(&#34;STOR&#34;)') = '0' do begin&#13;&#10;      vPlaceId := COMPUTE('FieldValue(&#34;Stor&#34;, &#34;PLACEID&#34;)');&#13;&#10;      sqlText := sqlText + ' OR (PLACEID= '+ vPlaceId+') ';&#13;&#10;      COMPUTE('Next(&#34;STOR&#34;)');&#13;&#10;    end;&#13;&#10;&#13;&#10;    sqlText := sqlText + ')' + sOrderBy;&#13;&#10;&#13;&#10;  end;&#13;&#10;&#13;&#10;  // ShowMessage(sqlText) ;&#13;&#10;  MainQuery.SQL.Clear;&#13;&#10;  MainQuery.SQL.Add(sqlText);&#13;&#10;  MainQuery.Active:=True;&#13;&#10;&#13;&#10;  //сохраняем настройки&#13;&#10;  Compute('WRITEINIFILE(&#34;Print&#34;,&#34;PeriodStart&#34;,&#34;'+DATETOSTR(dtFrom.Date)+'&#34;)');&#13;&#10;  Compute('WRITEINIFILE(&#34;Print&#34;,&#34;PeriodEnd&#34;,&#34;'+DateToStr(dtTo.Date)+'&#34;)');&#13;&#10;  Compute('WRITEINIFILE(&#34;Print&#34;,&#34;PlaceId&#34;,&#34;'+vPlaceId+'&#34;)');&#13;&#10;  Compute('WRITEINIFILE(&#34;Print&#34;,&#34;StoreKeeper&#34;,&#34;'+sStoreKeeper+'&#34;)');&#13;&#10;  COMPUTE('WRITEINIFILE(&#34;Print&#34;,&#34;TDByComms&#34;,&#34;'+VarToStr(cbTDByComms.Checked)+'&#34;)');&#13;&#10;&#13;&#10;  LicNum := fldStoreLicNum.Text;&#13;&#10;  ss := '&#34;';&#13;&#10;  sss := Chr(39);&#13;&#10;  LicNum := REPLACESTR(LicNum, ss, ' ');&#13;&#10;  LicNum := REPLACESTR(LicNum, sss, ' ');&#13;&#10;  Compute('WRITEINIFILE(&#34;Print&#34;,&#34;LicenceNum&#34;,&#34;'+LicNum+'&#34;)');&#13;&#10;&#13;&#10;  StorName := lblStoreName.Text;&#13;&#10;&#13;&#10;  Compute('WRITEINIFILE(&#34;Print&#34;,&#34;LicenceDate&#34;,&#34;'+DATETOSTR(fldStoreLicDate.Date)+'&#34;)');&#13;&#10;  ss := '&#34;';&#13;&#10;  sss := Chr(39);&#13;&#10;  StorName := REPLACESTR(StorName, ss, ' ');&#13;&#10;  StorName := REPLACESTR(StorName, sss, ' ');&#13;&#10;  Compute('WRITEINIFILE(&#34;Print&#34;,&#34;StoreName&#34;,&#34;'+StorName+'&#34;)');&#13;&#10;&#13;&#10;  if chkDO2.Checked = True then begin&#13;&#10;    Compute('WRITEINIFILE(&#34;Print&#34;,&#34;IncludeAllDO2&#34;,&#34;0&#34;)');&#13;&#10;    IncludeAllDO2 := '1'&#13;&#10;  end&#13;&#10;  else begin&#13;&#10;    Compute('WRITEINIFILE(&#34;Print&#34;,&#34;IncludeAllDO2&#34;,&#34;1&#34;)');&#13;&#10;    IncludeAllDO2 := '0'&#13;&#10;  end;&#13;&#10;&#13;&#10;  WriteIniFile(&#34;Print&#34;, 'PartInPlace', VarToStr(bPartInPlace));&#13;&#10;  WriteIniFile(&#34;Print&#34;, 'PartInPlaceText', sPartInPlaceText);&#13;&#10;  WriteIniFile(&#34;Print&#34;, 'PartOutPlace', VarToStr(bPartOutPlace));&#13;&#10;  WriteIniFile(&#34;Print&#34;, 'PartOutPlaceText', sPartOutPlaceText);&#13;&#10;&#13;&#10;  WriteIniFile(&#34;Print&#34;, 'GoodStoreNumKPS', VarToStr(bGoodStoreNumKPS));      &#13;&#10;  WriteIniFile(&#34;Print&#34;, 'UseConts', VarToStr(bUseConts));    &#13;&#10;  WriteIniFile(&#34;Print&#34;, 'PrintPlomb', VarToStr(bPrintPlomb));        &#13;&#10;  WriteIniFile(&#34;Print&#34;, 'PrintContType', VarToStr(bPrintContType));&#13;&#10;&#13;&#10;&#13;&#10;  WriteIniFile(&#34;Print&#34;, 'AllowSplit', VarToStr(bAllowSplit));&#13;&#10;&#13;&#10;  if cbxNotInclude.Checked = True then&#13;&#10;    Compute('WRITEINIFILE(&#34;Print&#34;,&#34;NotInclude&#34;,&#34;1&#34;)')&#13;&#10;  else&#13;&#10;    Compute('WRITEINIFILE(&#34;Print&#34;,&#34;NotInclude&#34;,&#34;0&#34;)');&#13;&#10;&#13;&#10;  if cbxStore.Checked = True then&#13;&#10;    Compute('WRITEINIFILE(&#34;Print&#34;,&#34;InStore&#34;,&#34;1&#34;)')&#13;&#10;  else&#13;&#10;    Compute('WRITEINIFILE(&#34;Print&#34;,&#34;InStore&#34;,&#34;0&#34;)');&#13;&#10;&#13;&#10;  if rbtnBook.Checked = True then begin&#13;&#10;    COMPUTE('WRITEINIFILE(&#34;Print&#34;,&#34;OnCurrentBook&#34;,&#34;1&#34;)');&#13;&#10;    onCurrBook := '1'&#13;&#10;  end&#13;&#10;  else begin&#13;&#10;    COMPUTE('WRITEINIFILE(&#34;Print&#34;,&#34;OnCurrentBook&#34;,&#34;0&#34;)');&#13;&#10;    onCurrBook := '0'&#13;&#10;  end;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure btnCallRefOnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  with btnCallRef, Engine do&#13;&#10;  begin&#13;&#10;    COMPUTE(' globalvar (&#34;strStoreNumLic&#34;,String)');&#13;&#10;    COMPUTE(' globalvar (&#34;strStoreNum&#34;,String)');&#13;&#10;    COMPUTE(' globalvar (&#34;strStoreName&#34;,String)');&#13;&#10;    COMPUTE(' globalvar (&#34;strLicExt&#34;,String)');&#13;&#10;    COMPUTE(' globalvar (&#34;sChief&#34;,String)');&#13;&#10;    COMPUTE(' globalvar (&#34;sPlaceId&#34;,Integer)');&#13;&#10;    COMPUTE(' globalvar (&#34;sLicenceDate&#34;,DateTime)');&#13;&#10;    COMPUTE('OPENQUERY (&#34;Stors&#34;, &#34;STS_DB&#34;, &#34;Select * from STORES&#34;)');&#13;&#10;&#13;&#10;&#13;&#10;    IF COMPUTE('SelectRecords(&#34;Выбор складских площадок&#34;, &#34;STORS&#34;,'+&#13;&#10;                          '['+&#13;&#10;                           '[&#34;LICENCENO&#34;, &#34;№ лицензии&#34;, 12],'+&#13;&#10;                           '[&#34;STORE_NO&#34;, &#34;№ склада&#34;, 8],'+&#13;&#10;                           '[&#34;NAME&#34;, &#34;Название склада&#34;, 30]'+&#13;&#10;                           '],'+&#13;&#10;                           '&#34;STOR&#34;, &#34;&#34;, &#34;STS_DB&#34;)') = '1' then&#13;&#10;&#13;&#10;      begin&#13;&#10;        bStorExists := True;&#13;&#10;        COMPUTE('FIRST(&#34;STOR&#34;)');&#13;&#10;        fldStoreLicNum.Text := COMPUTE('FieldValue(&#34;Stor&#34;, &#34;LICENCENO&#34;)');  //   номер  лиценцзии&#13;&#10;        //fldStoreNum.Text := '№ склада: ' + COMPUTE('FieldValue(&#34