;    sG35 := '0 ' + ' КГ'   &#13;&#10;  else if (sG315C = '') and (sG315A = '') then&#13;&#10;    sG35 := '0 ' + ' КГ'   &#13;&#10;  else&#13;&#10;    sG35 := '';&#13;&#10;&#13;&#10;  //Если отмечена одна из характеристик&#13;&#10;  if bVolume or bUnits or bBrutto then&#13;&#10;    vG35 := IIF(bVolume and (sG315C &#60;&#62; ''), sG315C + CHR(13) + CHR(10), '') +&#13;&#10;            IIF(bUnits and (sG315A &#60;&#62; ''), sG315A + CHR(13) + CHR(10), '') +&#13;&#10;            IIF(bBrutto and (sG35 &#60;&#62; ''), sG35, '')&#13;&#10;  // если ничего не отмечено&#13;&#10;  else begin&#13;&#10;    //Сначала смотрим заполнен ли объем (в АИСТе нет такой характеристики)&#13;&#10;    if (sG315C &#60;&#62; '') and (AistCompatible = 0) then&#13;&#10;      vG35 := sG315C&#13;&#10;    //Затем смотрим заполнены ли доп.ед. измерения (в АИСТе нет такой характеристики)&#13;&#10;    else if (sG315A &#60;&#62; '') and (AistCompatible = 0) then&#13;&#10;      vG35 := sG315A&#13;&#10;    //Если объем и доп.ед. не заполнены, печатаем объем&#13;&#10;    else&#13;&#10;      vG35 := sG35;&#13;&#10;  end;&#13;&#10;end          &#13;&#10;end;&#13;&#10;&#13;&#10;procedure Memo89OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;//if Engine.FinalPass then begin    &#13;&#10;    vStorageEndDate := '';&#13;&#10;    if VarToStr(&#60;qryComms.&#34;STORAGE_DATE&#34;&#62;) &#60;&#62; '' then&#13;&#10;      vStorageEndDate := FORMATDATETIME(&#34;DD.MM.YYYY&#34;, &#60;qryComms.&#34;STORAGE_DATE&#34;&#62;)&#13;&#10;    else if VarToStr(&#60;qryComms.&#34;STORE_PERIOD&#34;&#62;) &#60;&#62; '' then begin&#13;&#10;             if VarToStr(&#60;qryComms.&#34;ACCEPTDATE&#34;&#62;) &#60;&#62; '' then&#13;&#10;               vStorageEndDate := FORMATDATETIME(&#34;DD.MM.YYYY&#34;, &#60;qryComms.&#34;ACCEPTDATE&#34;&#62; + &#60;qryComms.&#34;STORE_PERIOD&#34;&#62;)&#13;&#10;             else vStorageEndDate := FORMATDATETIME(&#34;DD.MM.YYYY&#34;, &#60;qryComms.&#34;BEG_KEEP&#34;&#62; + &#60;qryComms.