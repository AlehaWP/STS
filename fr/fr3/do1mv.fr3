<TfrxReport Version="4.6.78" DotMatrixReport="False" EngineOptions.ConvertNulls="False" EngineOptions.DoublePass="True" IniFile="\Software\Fast Reports" PreviewOptions.Buttons="4095" PreviewOptions.Zoom="1" PrintOptions.Printer="По умолчанию" PrintOptions.PrintOnSheet="0" ReportOptions.CreateDate="38250.6727854398" ReportOptions.Description.Text="" ReportOptions.Name="Форма ДО1мв (Пр.№ 958 от 03.09.2003)" ReportOptions.LastChange="40694.512132963" ScriptLanguage="PascalScript" ScriptText.Text="Var&#13;&#10;  CustomDocsOnly, AistCompatible, StoreDocsBlock,          //Настройки из sts.ini&#13;&#10;  iTotalPages, vPlaceId, vId, vG32: Integer;&#13;&#10;  vUseStoreDocs: Boolean;&#13;&#10;  vSumG46: Extended;                                               //&#13;&#10;  stLicDate: TDateTime;&#13;&#10;  sStoreLicence, sStoreLicDate, sStoreName,&#13;&#10;  vDONo, vStorageEndDate,&#13;&#10;  sTrans, sDocNames, sStoreNum, sRemark,&#13;&#10;  vG311, vG35, vG42, vMainTable, vSQLText: String;&#13;&#10;  sTypeSVH, sNameSVH, sTypeDoc, sDateDoc, sNumDoc: string;&#13;&#10;  sG311 : Double;                                    &#13;&#10;  sPaperNames, sPaperCodes,  sConstStore,sConts : String;&#13;&#10;    &#13;&#10;  bBrutto, bUnits, bVolume: Boolean;&#13;&#10;  bGoodStoreNumKPS: Boolean;&#13;&#10;  bRemarkG082: Boolean;&#13;&#10;  bUseFactInfo, bUseConts: Boolean;&#13;&#10;&#13;&#10;  bAcceptDate, bBdDate, bBegKeep: Boolean;  &#13;&#10;&#13;&#10;  bStorageDateNoPrint, bPrintContType, bPrintPlomb: Boolean;&#13;&#10;  bPartPlace: Boolean;&#13;&#10;  sPartPlaceText: String;                       &#13;&#10;  bPrintWaybill: Boolean;  &#13;&#10;  bPrintRegNum, bPrintItog: Boolean;      &#13;&#10;  vSumG315C, vSumG315A, vSumG35 :Double;&#13;&#10;  sUnits, sSumUnits, sUnitsV, vAllSum: String;&#13;&#10;  KK, K : String;  &#13;&#10;  &#13;&#10;Const&#13;&#10;  sIniFullPath = ProgramPath() + 'print.ini';&#13;&#10;  sIniSection = 'do1mv.fr3';&#13;&#10;  sIniSectionCommon = 'Common';&#13;&#10;&#13;&#10;procedure MoveSettingsToNewIni;&#13;&#10;Const&#13;&#10;  //Список настроек, разделенных символом &#34;|&#34;&#13;&#10;  sSets = 'DO1mvBrutto-Brutto|DO1mvUnits-Units|DO1mvVolume-Volume|DO1mvGoodStoreNumKPS-GoodStoreNumKPS|ShowDialogDO1mv-ShowDialog';&#13;&#10;  sSetsCommon = '';&#13;&#10;  sPDelim  = '|';&#13;&#10;  sPDelim2 = '-';&#13;&#10;Var&#13;&#10;  i: Integer;&#13;&#10;  sFromOldIni, sFromNewIni, sParamName, sParamNameOld, sParamNameNew: String;&#13;&#10;begin&#13;&#10;  for i := 1 to WordCount(sSets, sPDelim) do begin&#13;&#10;    sParamName := ExtractWord(i, sSets, sPDelim);&#13;&#10;    If WordCount(sParamName, sPDelim2) &#62; 1 then begin //Если переименовываем параметр&#13;&#10;      sParamNameOld := ExtractWord(1, sParamName, sPDelim2);&#13;&#10;      sParamNameNew := ExtractWord(2, sParamName, sPDelim2);&#13;&#10;    end&#13;&#10;    else begin                                        //Если НЕ переименовываем параметр&#13;&#10;      sParamNameOld := ExtractWord(1, sParamName, sPDelim2);&#13;&#10;      sParamNameNew := ExtractWord(1, sParamName, sPDelim2);&#13;&#10;    end;&#13;&#10;    sFromOldIni := IniFile('Print', sParamNameOld, '');&#13;&#10;    sFromNewIni := IniFile(sIniSection, sParamNameNew, '', sIniFullPath);&#13;&#10;    if Trim(sFromNewIni) = '' then&#13;&#10;      WriteIniFile(sIniSection, sParamNameNew, sFromOldIni, sIniFullPath);&#13;&#10;  end;&#13;&#10;  for i := 1 to WordCount(sSetsCommon, sPDelim) do begin&#13;&#10;    sParamName := ExtractWord(i, sSetsCommon, sPDelim);&#13;&#10;    If WordCount(sParamName, sPDelim2) &#62; 1 then begin //Если переименовываем параметр&#13;&#10;      sParamNameOld := ExtractWord(1, sParamName, sPDelim2);&#13;&#10;      sParamNameNew := ExtractWord(2, sParamName, sPDelim2);&#13;&#10;    end&#13;&#10;    else begin                                        //Если НЕ переименовываем параметр&#13;&#10;      sParamNameOld := ExtractWord(1, sParamName, sPDelim2);&#13;&#10;      sParamNameNew := ExtractWord(1, sParamName, sPDelim2);&#13;&#10;    end;&#13;&#10;    sFromOldIni := IniFile('Print', sParamNameOld, '');&#13;&#10;    sFromNewIni := IniFile(sIniSectionCommon, sParamNameNew, '', sIniFullPath);&#13;&#10;    if Trim(sFromNewIni) = '' then&#13;&#10;      WriteIniFile(sIniSectionCommon, sParamNameNew, sFromOldIni, sIniFullPath);&#13;&#10;  end;&#13;&#10;end;&#13;&#10;&#13;&#10;          &#13;&#10;  &#13;&#10;procedure ReportTitle1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;//if Engine.FinalPass then begin&#13;&#10;    sTypeSVH := '';&#13;&#10;    sNameSVH := '';&#13;&#10;    sTypeDoc := '';&#13;&#10;    sDateDoc := '';&#13;&#10;    sNumDoc  := '';&#13;&#10;&#13;&#10;    if COMPUTE('FINDKEY(&#34;STORES_2&#34;, [&#34;' + IntToStr(&#60;KRD_MAIN.&#34;PLACEID&#34;&#62;) + '&#34;])') &#60;&#62; '0' then&#13;&#10;      sTypeSVH := &#60;STORES_2.&#34;STORE_TYPE&#34;&#62;&#13;&#10;    else sTypeSVH := '';&#13;&#10;&#13;&#10;    if (&#60;KRD_MAIN.&#34;G142&#34;&#62; &#60;&#62; Null) and (&#60;KRD_MAIN.&#34;G142&#34;&#62; &#60;&#62; '') then begin&#13;&#10;      sNameSVH := &#60;KRD_MAIN.&#34;G142&#34;&#62;;&#13;&#10;      sDateDoc := &#60;KRD_MAIN.&#34;G145&#34;&#62;;&#13;&#10;      sNumDoc := &#60;KRD_MAIN.&#34;G144&#34;&#62;;&#13;&#10;    end&#13;&#10;    else if COMPUTE('FINDKEY(&#34;STORES_2&#34;, [&#34;' + IntToStr(&#60;KRD_MAIN.&#34;PLACEID&#34;&#62;) + '&#34;])') &#60;&#62; '0' then begin&#13;&#10;      sNameSVH := &#60;STORES_2.&#34;Name&#34;&#62;;&#13;&#10;      sDateDoc := DateToStr(&#60;STORES_2.&#34;LICENCEDATE&#34;&#62;);&#13;&#10;      sNumDoc  := &#60;STORES_2.&#34;LICENCENO&#34;&#62;&#13;&#10;    end&#13;&#10;    else begin&#13;&#10;      sNameSVH := '';&#13;&#10;      sDateDoc := '';&#13;&#10;      sNumDoc  := '';&#13;&#10;    end;&#13;&#10;&#13;&#10;    case &#60;STORES_2.&#34;STORE_TYPE&#34;&#62; of&#13;&#10;      '', 'СВХ', Null: begin&#13;&#10;        sTypeSVH := 'СВХ';&#13;&#10;        sTypeDoc := 'Свидетельство о включении в реестр владельцев СВХ от';&#13;&#10;      end;&#13;&#10;      'СП': begin&#13;&#10;        sTypeSVH := 'Склад получателя товаров';&#13;&#10;        sTypeDoc := 'Разрешение на временное хранение на складе получателя товаров от';&#13;&#10;      end;&#13;&#10;      'ПЗТК': begin&#13;&#10;        sTypeSVH := 'ПЗТК';&#13;&#10;        sTypeDoc := 'Приказ(распоряжение) таможни от';&#13;&#10;      end;&#13;&#10;      'ВЗТК': begin&#13;&#10;        sTypeSVH := 'ВЗТК';&#13;&#10;        sTypeDoc := 'Приказ(распоряжение) таможни от';  &#13;&#10;      end;                &#13;&#10;      'ЗТК': begin&#13;&#10;        sTypeSVH := 'ЗТК';&#13;&#10;        sTypeDoc := 'Приказ(распоряжение) таможни от';&#13;&#10;      end;&#13;&#10;      'СВХУТ': begin&#13;&#10;        sTypeSVH := 'СВХ';&#13;&#10;        sTypeDoc := 'Свидетельство о включении в реестр владельцев СВХ от';&#13;&#10;      end;&#13;&#10;    end;&#13;&#10;//  end&#13;&#10;end;&#13;&#10;&#13;&#10;procedure PrintPlomb (s : integer);      &#13;&#10;begin&#13;&#10;sConstStore :='';  &#13;&#10;  COMPUTE('SETFILTER(&#34;KRD_COMM_PAPERS_2&#34;, &#34;PLACEID = ' + IntToStr(vPlaceId) +&#13;&#10;                                          ' AND ID = ' + IntToStr(vId) +&#13;&#10;                                         ' AND G32 = ' + IntToStr(vG32) +&#13;&#10;                                    ' AND DOC_TYPE = 11&#34;)');  &#13;&#10;  if StrToInt(COMPUTE('RECORDCOUNT(&#34;KRD_COMM_PAPERS_2&#34;)')) &#62; 0 then begin&#13;&#10;    COMPUTE('FIRST(&#34;KRD_COMM_PAPERS_2&#34;)');&#13;&#10;    While StrToInt(COMPUTE('EOF(&#34;KRD_COMM_PAPERS_2&#34;)')) = 0 do begin&#13;&#10;      COMPUTE('SETRANGE(&#34;KRD_CONT_2&#34;, [&#34;' + IntToStr(&#60;KRD_COMM_PAPERS_2.&#34;PLACEID&#34;&#62;) + '&#34;, &#34;' +&#13;&#10;                                            IntToStr(&#60;KRD_COMM_PAPERS_2.&#34;ID&#34;&#62;) + '&#34;, &#34;' +&#13;&#10;                                            IntToStr(&#60;KRD_COMM_PAPERS_2.&#34;DOC_COUNTER&#34;&#62;) + '&#34;])');&#13;&#10;      if Length(sConstStore) &#62; 0 then sConstStore := sConstStore + ',';&#13;&#10;      if (COMPUTE('FIELDVALUE(&#34;KRD_CONT_2&#34;, &#34;PLOMB&#34;)') &#60;&#62; '') and chkPrintPlomb.Checked then begin&#13;&#10;        if chkPrintContType.Checked then                                                                                                                   &#13;&#10;          sConstStore := sConstStore + COMPUTE('FIELDVALUE(&#34;KRD_CONT_2&#34;, &#34;CONTTYPE&#34;)') + ' ' + COMPUTE('FIELDVALUE(&#34;KRD_CONT_2&#34;, &#34;CONTNO&#34;)') + ' : '+ COMPUTE('FIELDVALUE(&#34;KRD_CONT_2&#34;, &#34;PLOMB&#34;)')&#13;&#10;        else&#13;&#10;          sConstStore := sConstStore + COMPUTE('FIELDVALUE(&#34;KRD_CONT_2&#34;, &#34;CONTNO&#34;)') + ' : '+ COMPUTE('FIELDVALUE(&#34;KRD_CONT_2&#34;, &#34;PLOMB&#34;)');                      &#13;&#10;      end                                 &#13;&#10;      else begin&#13;&#10;        if chkPrintContType.Checked then              &#13;&#10;          sConstStore := sConstStore + COMPUTE('FIELDVALUE(&#34;KRD_CONT_2&#34;, &#34;CONTTYPE&#34;)') + ' ' + COMPUTE('FIELDVALUE(&#34;KRD_CONT_2&#34;, &#34;CONTNO&#34;)')&#13;&#10;        else&#13;&#10;          sConstStore := sConstStore + COMPUTE('FIELDVALUE(&#34;KRD_CONT_2&#34;, &#34;CONTNO&#34;)');                                               &#13;&#10;      end;&#13;&#10;      COMPUTE('CANCELRANGE(&#34;KRD_CONT_2&#34;)');&#13;&#10;      COMPUTE('NEXT(&#34;KRD_COMM_PAPERS_2&#34;)');&#13;&#10;    end;&#13;&#10;  end&#13;&#10;  else begin&#13;&#10;    COMPUTE('SETRANGE(&#34;KRD_CONT_2&#34;, [&#34;' + IntToStr(vPlaceId) + '&#34;, &#34;' +&#13;&#10;                                          IntToStr(vId) + '&#34;])');&#13;&#10;    if chkPrintPlomb.Checked then begin                                                                               &#13;&#10;      if chkPrintContType.Checked then                                                  &#13;&#10;      sConstStore := COMPUTE('UNIONVALUES(&#34;KRD_CONT_2&#34;, [&#34;CONTTYPE&#34;, &#34;CONTNO&#34;,&#34;PLOMB&#34;], &#34;, &#34;, &#34; : &#34;)')&#13;&#10;      else&#13;&#10;      sConstStore := COMPUTE('UNIONVALUES(&#34;KRD_CONT_2&#34;, [&#34;CONTNO&#34;,&#34;PLOMB&#34;], &#34;, &#34;, &#34; : &#34;)');&#13;&#10;    end&#13;&#10;    else begin          &#13;&#10;      if chkPrintContType.Checked then                                                  &#13;&#10;      sConstStore := COMPUTE('UNIONVALUES(&#34;KRD_CONT_2&#34;, [&#34;CONTTYPE&#34;, &#34;CONTNO&#34;], &#34;, &#34;, &#34;:&#34; )')&#13;&#10;      else&#13;&#10;      sConstStore := COMPUTE('UNIONVALUES(&#34;KRD_CONT_2&#34;, [&#34;CONTNO&#34;], &#34;, &#34;, &#34; : &#34;)');            &#13;&#10;    end;      &#13;&#10;          &#13;&#10;    COMPUTE('CANCELRANGE(&#34;KRD_CONT_2&#34;)'); &#13;&#10;                        &#13;&#10;  end;&#13;&#10;  COMPUTE('SETFILTER(&#34;KRD_COMM_PAPERS_2&#34;, &#34;&#34;)');&#13;&#10;  //showmessage(sConstStore);                                &#13;&#10;  if s=1 then sStoreNum := sConstStore ;&#13;&#10;  if s=2 then sConts := sConstStore ;                                     &#13;&#10;end;    &#13;&#10;&#13;&#10;procedure MasterData1OnBeforePrint(Sender: TfrxComponent); &#13;&#10;begin&#13;&#10;  with MasterData1, Engine do begin&#13;&#10;    vPlaceId := &#60;qryComms.&#34;PLACEID&#34;&#62;;&#13;&#10;    vId := &#60;qryComms.&#34;ID&#34;&#62;;&#13;&#10;    vG32 := &#60;qryComms.&#34;G32&#34;&#62;;&#13;&#10;    if StoreDocsBlock = 1 then begin&#13;&#10;      COMPUTE('CANCELRANGE(&#34;IN_COMM_2&#34;)');&#13;&#10;      COMPUTE('SETRANGE(&#34;IN_COMM_2&#34;, [&#34;' + IntToStr(vPlaceId) + '&#34;, &#34;' +&#13;&#10;                                           IntToStr(vId) + '&#34;, &#34;' +&#13;&#10;                                           IntToStr(vG32) + '&#34;])');&#13;&#10;    end;&#13;&#10;  end;  &#13;&#10;&#13;&#10;//if Engine.FinalPass then begin&#13;&#10;  sStoreNum := '';&#13;&#10;  sConts := '';&#13;&#10;&#13;&#10;  if not bGoodStoreNumKPS then begin&#13;&#10;    If StoreDocsBlock = 0 then&#13;&#10;      sStoreNum := &#60;qryComms.&#34;BOXNO&#34;&#62;&#13;&#10;    else if (CustomDocsOnly = 1) and (AistCompatible = 0) then&#13;&#10;      sStoreNum := &#60;qryComms.&#34;BOXNO&#34;&#62;&#13;&#10;    else if Length(&#60;IN_COMM_2.&#34;BOXNO&#34;&#62;) &#62; 0 then&#13;&#10;      sStoreNum := &#60;IN_COMM_2.&#34;BOXNO&#34;&#62;&#13;&#10;    else sStoreNum := &#60;qryComms.&#34;BOXNO&#34;&#62;;&#13;&#10;&#13;&#10;    if bUseConts then begin    &#13;&#10;      if Length(sStoreNum) = 0 then begin&#13;&#10;      PrintPlomb(1);  &#13;&#10;     end;&#13;&#10;    end;                        &#13;&#10;  end&#13;&#10;  else begin&#13;&#10;                                         &#13;&#10;    sStoreNum := &#60;KRD_MAIN.&#34;NBD&#34;&#62; + '/' + COMPUTE('LEFTPAD(&#34;' + VarToStr(&#60;qryComms.&#34;GN&#34;&#62;) + '&#34;, &#34;6&#34;, &#34;0&#34;)');&#13;&#10;&#13;&#10;    if Trim(&#60;qryComms.&#34;SVHAREA&#34;&#62;) &#60;&#62; '' then&#13;&#10;      sStoreNum := sStoreNum + CHR(13) + 'Зона: ' + &#60;qryComms.&#34;SVHAREA&#34;&#62;;&#13;&#10;&#13;&#10;    if Trim(&#60;qryComms.&#34;SVHHANGAR&#34;&#62;) &#60;&#62; '' then&#13;&#10;      sStoreNum := sStoreNum + CHR(13) + 'Анг.: ' + &#60;qryComms.&#34;SVHHANGAR&#34;&#62;;&#13;&#10;&#13;&#10;    if Trim(&#60;qryComms.&#34;SVHRACK&#34;&#62;) &#60;&#62; '' then&#13;&#10;      sStoreNum := sStoreNum + CHR(13) + 'Стел.: ' + &#60;qryComms.&#34;SVHRACK&#34;&#62;;&#13;&#10;&#13;&#10;    if Trim(&#60;qryComms.&#34;SVHCELL&#34;&#62;) &#60;&#62; '' then&#13;&#10;      sStoreNum := sStoreNum + CHR(13) + 'Яч.: ' + &#60;qryComms.&#34;SVHCELL&#34;&#62;;&#13;&#10;&#13;&#10;    if bUseConts then begin    &#13;&#10;                                 &#13;&#10;      PrintPlomb(2);   &#13;&#10;      if Trim(sConts) &#60;&#62; '' then&#13;&#10;      sStoreNum := sStoreNum + CHR(13) + 'Конт.: ' + sConts;  &#13;&#10;    end;                &#13;&#10;//  end;&#13;&#10;end;  &#13;&#10;&#13;&#10;//if Engine.FinalPass then begin&#13;&#10;&#13;&#10;     if  bPrintWaybill then                             &#13;&#10;    COMPUTE('OPENQUERY(&#34;QRYREFPAPERS&#34;, &#34;STS_DB&#34;, &#34;SELECT PAPER_DOCG44_CODE, PAPERNAME ' + &#13;&#10;                                               'FROM PAPERS ' + &#13;&#10;                                               'WHERE PAPER_IS_WAYBILL = 1&#34;)')&#13;&#10;     else&#13;&#10;     COMPUTE('OPENQUERY(&#34;QRYREFPAPERS&#34;, &#34;STS_DB&#34;, &#34;SELECT PAPER_DOCG44_CODE, PAPERNAME ' + &#13;&#10;                                               'FROM PAPERS ' + &#13;&#10;                                               'WHERE PAPER_IS_WAYBILL = 1' +          &#13;&#10;                                                 ' OR PAPER_IS_INVOICE = 1&#34;)');      &#13;&#10;    sPaperNames := '|' + COMPUTE('JOINVALUES(&#34;QRYREFPAPERS&#34;, &#34;|&#34;, [&#34;PAPERNAME&#34;])') + '|';&#13;&#10;    sPaperCodes := '|' + COMPUTE('JOINVALUES(&#34;QRYREFPAPERS&#34;, &#34;|&#34;, [&#34;PAPER_DOCG44_CODE&#34;])') + '|';                                   &#13;&#10;    COMPUTE('CLOSEDATASET(&#34;QRYREFPAPERS&#34;)');    &#13;&#10;    sDocNames := &#34;&#34;;&#13;&#10;    COMPUTE('SETFILTER(&#34;KRD_COMM_PAPERS_2&#34;, &#34;PLACEID = ' + IntToStr(vPlaceId) +&#13;&#10;                                            ' AND ID = ' + IntToStr(vId) +&#13;&#10;                                           ' AND G32 = ' + IntToStr(vG32) +&#13;&#10;                                      ' AND DOC_TYPE = 13&#34;)');&#13;&#10;    if StrToInt(COMPUTE('RECORDCOUNT(&#34;KRD_COMM_PAPERS_2&#34;)')) &#62; 0 then begin&#13;&#10;      COMPUTE('FIRST(&#34;KRD_COMM_PAPERS_2&#34;)');&#13;&#10;      While StrToInt(COMPUTE('EOF(&#34;KRD_COMM_PAPERS_2&#34;)')) = 0 do begin&#13;&#10;        COMPUTE('SETRANGE(&#34;KRD_PAPERS_2&#34;, [&#34;' + IntToStr(&#60;KRD_COMM_PAPERS_2.&#34;PLACEID&#34;&#62;) + '&#34;, &#34;' +&#13;&#10;                                                IntToStr(&#60;KRD_COMM_PAPERS_2.&#34;ID&#34;&#62;) + '&#34;, &#34;' +&#13;&#10;                                                IntToStr(&#60;KRD_COMM_PAPERS_2.&#34;DOC_COUNTER&#34;&#62;) + '&#34;])');&#13;&#10;        if sDocNames &#60;&#62; &#34;&#34; then sDocNames := sDocNames + &#34;, &#34;;&#13;&#10;            if (Pos('|' + Trim(&#60;KRD_PAPERS_2.&#34;PAPERCODE&#34;&#62;) + '|', sPaperCodes) &#62; 0) or&#13;&#10;            (Pos('|' + Trim(&#60;KRD_PAPERS_2.&#34;PAPERNAME&#34;&#62;) + '|', sPaperNames) &#62; 0) then begin                                                   &#13;&#10;              if Length(VarToStr(&#60;KRD_PAPERS_2.&#34;PAPERDATE&#34;&#62;)) &#62; 2 then&#13;&#10;              sDocNames := sDocNames + &#60;KRD_PAPERS_2.&#34;PAPERNAME&#34;&#62; + ' №' + &#60;KRD_PAPERS_2.&#34;PAPERNO&#34;&#62; + ' от ' + FormatDateTime('DD.MM.YYYY', &#60;KRD_PAPERS_2.&#34;PAPERDATE&#34;&#62;)&#13;&#10;              else sDocNames := sDocNames + &#60;KRD_PAPERS_2.&#34;PAPERNAME&#34;&#62; + ' №' + &#60;KRD_PAPERS_2.&#34;PAPERNO&#34;&#62;;                                                                                                                                                                           &#13;&#10;           end;  &#13;&#10;                       &#13;&#10;        COMPUTE('CANCELRANGE(&#34;KRD_PAPERS_2&#34;)');&#13;&#10;        COMPUTE('NEXT(&#34;KRD_COMM_PAPERS_2&#34;)');&#13;&#10;              &#13;&#10;      end;&#13;&#10;    end&#13;&#10;    else begin&#13;&#10;      COMPUTE('SETRANGE(&#34;KRD_PAPERS_2&#34;, [&#34;' + IntToStr(vPlaceId) + '&#34;, &#34;' +&#13;&#10;                                              IntToStr(vId) + '&#34;])');&#13;&#10;      COMPUTE('FIRST(&#34;KRD_PAPERS_2&#34;)');&#13;&#10;      While StrToInt(COMPUTE('EOF(&#34;KRD_PAPERS_2&#34;)')) = 0 do begin&#13;&#10;        if (Pos('|' + Trim(&#60;KRD_PAPERS_2.&#34;PAPERCODE&#34;&#62;) + '|', sPaperCodes) &#62; 0) or&#13;&#10;           (Pos('|' + Trim(&#60;KRD_PAPERS_2.&#34;PAPERNAME&#34;&#62;) + '|', sPaperNames) &#62; 0) then begin                                                   &#13;&#10;             if Length(VarToStr(&#60;KRD_PAPERS_2.&#34;PAPERDATE&#34;&#62;)) &#62; 2 then&#13;&#10;             sDocNames := sDocNames + &#60;KRD_PAPERS_2.&#34;PAPERNAME&#34;&#62; + ' №' + &#60;KRD_PAPERS_2.&#34;PAPERNO&#34;&#62; + ' от ' + FormatDateTime('DD.MM.YYYY', &#60;KRD_PAPERS_2.&#34;PAPERDATE&#34;&#62;)&#13;&#10;             else sDocNames := sDocNames + &#60;KRD_PAPERS_2.&#34;PAPERNAME&#34;&#62; + ' №' + &#60;KRD_PAPERS_2.&#34;PAPERNO&#34;&#62;;&#13;&#10;         end;                                    &#13;&#10;      COMPUTE('NEXT(&#34;KRD_PAPERS_2&#34;)');&#13;&#10;      end;&#13;&#10;      COMPUTE('CANCELRANGE(&#34;KRD_PAPERS_2&#34;)');&#13;&#10;    end&#13;&#10;//  end    &#13;&#10;end;&#13;&#10;&#13;&#10;procedure fldCarnoOnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;//if Engine.FinalPass then begin&#13;&#10;    sTrans := &#34;&#34;;&#13;&#10;    if Length(&#60;KRD_MAIN.&#34;SHIP_NAME&#34;&#62;)&#62;0 then&#13;&#10;      sTrans := &#60;KRD_MAIN.&#34;SHIP_NAME&#34;&#62;&#13;&#10;    else begin&#13;&#10;      COMPUTE('SETRANGE(&#34;KRD_TRANSP_2&#34;, [&#34;' + IntToStr(vPlaceId) + '&#34;, &#34;' +&#13;&#10;                                              IntToStr(vId) + '&#34;])');&#13;&#10;      sTrans := COMPUTE('UNIONVALUES(&#34;KRD_TRANSP_2&#34;, [&#34;CARNO&#34;], &#34;, &#34;, &#34; &#34;)');&#13;&#10;      COMPUTE('CANCELRANGE(&#34;KRD_TRANSP_2&#34;)');&#13;&#10;    end&#13;&#10;//  end&#13;&#10;end;&#13;&#10;&#13;&#10;&#13;&#10;procedure Memo85OnBeforePrint(Sender: TfrxComponent);&#13;&#10;Var&#13;&#10;  sG311kc, sG311kcFact, sG311ic: String;&#13;&#10;  dG311kc, dG311kcFact, dG311ic: Double;      &#13;&#10;begin&#13;&#10;if Engine.FinalPass then begin                                                                 &#13;&#10;  vG311 := '';&#13;&#10;  if vUseStoreD