arToStr(bPrintWaybill), sIniFullPath);  &#13;&#10;    &#13;&#10;end;&#13;&#10;&#13;&#10;procedure Page1OnShow(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  chkRegNum.Checked := bPrintRegNum;    &#13;&#10;  chkItog.Checked := bPrintItog;    &#13;&#10;  chkBrutto.Checked := bBrutto;&#13;&#10;  chkUnits.Checked := bUnits;&#13;&#10;  chkVolume.Checked := bVolume;&#13;&#10;  chkGoodStoreNumKPS.Checked := bGoodStoreNumKPS;&#13;&#10;  chkRemarkG082.Checked := bRemarkG082;&#13;&#10;  chkShow.Checked := IniFile(sIniSection, 'ShowDialog', 'False', sIniFullPath) = 'True';&#13;&#10;  if vUseStoreDocs then&#13;&#10;    rbtnInfoFact.Caption := 'фактические сведения (из складского документа учета принятия груза на временное хранение)';&#13;&#10;&#13;&#10;  rbtnInfoFact.Checked := bUseFactInfo;&#13;&#10;  rbtnInfoDoc.Checked := not bUseFactInfo;&#13;&#10;&#13;&#10;  rbtnAcceptDate.Checked := bAcceptDate;    &#13;&#10;  rbtnBdDate.Checked := bBdDate;                                                                  &#13;&#10;  rbtnBegKeep.Checked := bBegKeep;&#13;&#10;&#13;&#10;  chkPartPlace.Checked := bPartPlace;&#13;&#10;    &#13;&#10;  chkStorageDateNoPrint.Checked := bStorageDateNoPrint;&#13;&#10;    &#13;&#10;  chkUseConts.Checked := bUseConts;&#13;&#10;    &#13;&#10;  chkPrintPlomb.Checked := bPrintPlomb;&#13;&#10;  chkPrintPlomb.Enabled := bUseConts;      &#13;&#10;    &#13;&#10;  chkPrintContType.Checked := bPrintContType;&#13;&#10;  chkPrintContType.Enabled := bUseConts;&#13;&#10;&#13;&#10;  cbxOnlyWaybill.Checked := bPrintWaybill;      &#13;&#10;end;&#13;&#10;&#13;&#10;procedure Memo90OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;//if Engine.FinalPass then begin    &#13;&#10;  sRemark := '';&#13;&#10;  sRemark := &#60;qryComms.&#34;Remark&#34;&#62;;&#13;&#10;  if bRemarkG082 then&#13;&#10;  begin&#13;&#10;    COMPUTE('SETFILTER(&#34;KRD_Main_2&#34;, &#34;PLACEID = ' + IntToStr(vPlaceId) +&#13;&#10;                                              ' AND ID = ' + IntToStr(vId) + '&#34;)');&#13;&#10;    if (StrToInt(COMPUTE('RECORDCOUNT(&#34;KRD_MAIN_2&#34;)')) &#62; 0)  AND (COMPUTE('FIELDVALUE(&#34;KRD_MAIN_2&#34;, &#34;G082&#34;)') &#60;&#62; '') then&#13;&#10;      begin&#13;&#10;        if Length(sRemark) &#62; 0 then sRemark := sRemark + ',';&#13;&#10;        sRemark := sRemark + COMPUTE('FIELDVALUE(&#34;KRD_MAIN_2&#34;, &#34;G082&#34;)');&#13;&#10;      end;&#13;&#10;    COMPUTE('SETFILTER(&#34;KRD_Main_2&#34;, &#34;&#34;)');&#13;&#10;  end;&#13;&#10;//end          &#13;&#10;end;&#13;&#10;&#13;&#10;procedure chkUseContsOnClick(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  chkPrintContType.Enabled := chkUseConts.Checked;&#13;&#10;  chkPrintPlomb.Enabled := chkUseConts.Checked;      &#13;&#10;  chkPrintPlomb.Checked :=  chkUseConts.Checked;        &#13;&#10;end;&#13;&#10;&#13;&#10;procedure Memo113OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;if Engine.FINALPASS then begin&#13;&#10;  sSumUnits := ' ДОП. ЕД.';  &#13;&#10;  if bVolume or bUnits or bBrutto then&#13;&#10;    vAllSum := IIF(bVolume and (vSumG315C &#60;&#62; 0), VarToStr(vSumG315C) + sUnitsV + CHR(13) + CHR(10), '') +&#13;&#10;            IIF(bUnits and (vSumG315A &#60;&#62; 0), VarToStr(vSumG315A) + sSumUnits + CHR(13) + CHR(10), '') +&#13;&#10;            IIF(bBrutto and (vSumG35 &#60;&#62; 0), VarToStr(vSumG35) + ' КГ', '')&#13;&#10;  else                      &#13;&#10;    vAllSum := IIF((vSumG315C &#60;&#62; 0), VarToStr(vSumG315C) + sUnitsV + CHR(13) + CHR(10), '') +&#13;&#10;            IIF((vSumG315A &#60;&#62; 0), VarToStr(vSumG315A) + sSumUnits + CHR(13) + CHR(10), '') +&#13;&#10;            IIF((vSumG35 &#60;&#62; 0), VarToStr(vSumG35) + ' КГ', '')    &#13;&#10;end  &#13;&#10;end;&#13;&#10;&#13;&#10;procedure ReportSummary1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  ReportSummary1.Visible := bPrintItog  &#13;&#10;end;&#13;&#10;&#13;&#10;begin&#13;&#10;&#13;&#10;  MoveSettingsToNewIni();&#13;&#10;&#13;&#10;  //Взяли из ini-шника &#34;Только таможенные документы учета&#34;&#13;&#10;  CustomDocsOnly := StrToInt(IniFile('RegBook', 'CustomsDocsOnly', '0'));&#13;&#10;  //Совместимость с АИСТом&#13;&#10;  AistCompatible := StrToInt(IniFile('Docs', 'AistCompatible', '0')); {включина ли настройка &#34;Совместимость с АИСТом&#34;, 1-да, 0-нет}&#13;&#10;  //Отключен ли блок складских документов&#13;&#10;  StoreDocsBlock := StrToInt(IniFile('Database', 'StoreDocsBlock', '0'));&#13;&#10;  if (StoreDocsBlock = 1) and ((AistCompatible = 1) or ((AistCompatible = 0) and (CustomDocsOnly = 0))) then&#13;&#10;    vUseStoreDocs := True      &#13;&#10;  else vUseStoreDocs := False;&#13;&#10;&#13;&#10;  bPrintRegNum := IniFile(sIniSection, 'PrintRegNum', 'False', sIniFullPath) = 'True';&#13;&#10;    &#13;&#10;  bPrintItog := IniFile(sIniSection, 'PrintItog', 'False', sIniFullPath) = 'True';  &#13;&#10;  &#13;&#10;  //Что печатать в графе 7&#13;&#10;  bBrutto := IniFile(sIniSection, 'Brutto', 'False', sIniFullPath) = 'True';&#13;&#10;  bUnits := IniFile(sIniSection, 'Units', 'False', sIniFullPath) = 'True';&#13;&#10;  bVolume := IniFile(sIniSection, 'Volume', 'False', sIniFullPath) = 'True';&#13;&#10;&#13;&#10;  bGoodStoreNumKPS := IniFile(sIniSection, 'GoodStoreNumKPS', 'False', sIniFullPath) = 'True';&#13;&#10;&#13;&#10;  bRemarkG082 := IniFile(sIniSection, 'RemarkG082', 'False', sIniFullPath) = 'True';&#13;&#10;&#13;&#10;  bUseFactInfo := IniFile(sIniSection, 'UseFactInfo', 'True', sIniFullPath) = 'True';&#13;&#10;&#13;&#10;  bUseConts := IniFile(sIniSection, 'UseConts', 'False', sIniFullPath) = 'True';      &#13;&#10;&#13;&#10;  if ShiftPressed() then&#13;&#10;    Page1.Visible := True&#13;&#10;  else if IniFile(sIniSection, 'ShowDialog', 'False', sIniFullPath) = 'False' then&#13;&#10;    Page1.Visible := False&#13;&#10;  else&#13;&#10;    Page1.Visible := True;&#13;&#10;&#13;&#10;  //Что печатать в графе 5   &#13;&#10;  bPartPlace := IniFile(sIniSection, 'PartPlace', 'False', sIniFullPath) = 'True';&#13;&#10;  bPrintWaybill := IniFile(sIniSection, 'PrintWaybill', 'False', sIniFullPath) = 'True';  &#13;&#10;  sPartPlaceText := IniFile(sIniSection, 'PartPlaceText', 'часть места', sIniFullPath);&#13;&#10;  &#13;&#10;  bAcceptDate := IniFile(sIniSection, 'AcceptDate', 'True', sIniFullPath) = 'True';         &#13;&#10;  bBdDate := IniFile(sIniSection, 'BdDate', 'False', sIniFullPath) = 'True';&#13;&#10;  bBegKeep := IniFile(sIniSection, 'BegKeep', 'False', sIniFullPath) = 'True';  &#13;&#10;&#13;&#10;  bPrintContType := IniFile(sIniSection, 'PrintContType', 'False', sIniFullPath) = 'True';&#13;&#10;  bPrintPlomb := IniFile(sIniSection, 'PrintPlomb', 'True', sIniFullPath) = 'True';      &#13;&#10;  bStorageDateNoPrint := IniFile(sIniSection, 'StorageDateNoPrint', 'True', sIniFullPath) = 'True';  &#13;&#10;  &#13;&#10;  vMainTable := CreateDo1Comm(&#60;KRD_MAIN.&#34;PLACEID&#34;&#62;, &#60;KRD_MAIN.&#34;MAIN_ID&#34;&#62;);&#13;&#10;  vSQLText := 'SELECT * FROM &#34;' + vMainTable + '&#34; ORDER BY PART_NO, GN, G32';&#13;&#10;  qryComms.SQL.Text := vSQLText;&#13;&#10;  qryComms.Active := True;&#13;&#10;  qryComms.First;&#13;&#10;end." PropData="">
